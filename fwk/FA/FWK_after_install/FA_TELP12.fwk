$Type: Action
$Name: HP_PROBER_PRB_HOME_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$-------------- -------------- ------------------------------------------------------------------------------------------------------------------- - - ---------------------------------------------------
 BODY                                                                                                                                                                                                     
                IF             PRB_ONLINE==0                                                                                                                                                              
                 LET           RTN_PRB_HOME="Wafer_ready"                                                                                                                                                 
                 RETURN                                                                                                                                                                                   
                END IF                                                                                                                                                                                    
 Prober_home                                                                                                                                                                                              
                ACTION         "FA_CHECK_MESSAGE"                                                                                                      ### AUTOMATION Check prober_alarm                  
                IF             (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP") OR (FA_GEM_COMMAND=="PJABORT") OR (FA_GEM_COMMAND=="CJABORT")     ### FA3.0                                          
                  PROBER_LOGWR Message="ABORT 043"                                                                                                     For debugging                                      
                  LET          RTN_PRB_HOME="Prober_abort"                                                                                             ### AUTOMATION                                     
                  RETURN                                                                                                                               ### AUTOMATION                                     
                END IF                                                                                                                                 ### AUTOMATION                                     
                                                                                                                                                                                                          
#               ACTION         "FA_CHECK_STATUSBYTE"                                                                                                   ### FA3.0                                          
#               IF             RTN_PRB_QUERY_READY=="Prober_abort"                                                                                     ### FA3.0                                          
#                 LET          PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                       ### FA3.0                                          
#                 LET          PNL_MSG_MSG1="$TELP12_FATAL_ERROR"                                                                                      ### FA3.0                                          
#                 LET          PNL_MSG_MSG2="HP_PROBER_PRB_HOME_TELP12"                                                                                ### FA3.0                                          
#                 PROBER_LOGWR Message="RESUME_ABORT 015"                                                                                              For debugging                                      
#                 LET          FA_ALID="7000"                                                                                                          ### FA3.0                                          
#                 LET          FA_ALTX="CustomAlarm"                                                                                                   ### FA3.0                                          
#                 LET          FA_ALMSG="Invalid Status Byte."                                                                                         ### FA3.0                                          
#                 ACTION       "FA_WAIT_ABORT"                                                                                                         ### FA3.0                                          
#                 IF           (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP") OR (FA_GEM_COMMAND=="PJABORT") OR (FA_GEM_COMMAND=="CJABORT")     ### FA3.0                                          
#                   LET        RTN_PRB_HOME="Prober_abort"                                                                                             ### FA3.0                                          
#                   RETURN                                                                                                                             ### FA3.0                                          
#                 END IF                                                                                                                               ### FA3.0                                          
#               END IF                                                                                                                                 ### FA3.0                                          
                                                                                                                                                                                                          
                PROBER_HOME                                                                                                                                                                               
                                                                                                                                                                                                          
                ACTION         "FA_CHECK_MESSAGE"                                                                                                      ### AUTOMATION Check prober_alarm                  
                IF             FA_GEM_COMMAND=="RESUME"                                                                                                ### AUTOMATION   Resume or Abort after prober alarm
                  PROBER_LOGWR Message="RESUME 016"                                                                                                    For debugging                                      
                END IF                                                                                                                                 ### AUTOMATION                                     
                IF             (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP") OR (FA_GEM_COMMAND=="PJABORT") OR (FA_GEM_COMMAND=="CJABORT")     ### FA3.0                                          
                  PROBER_LOGWR Message="ABORT 016"                                                                                                     For debugging                                      
                  LET          PRB_ABORT_ALL=-1                                                                                                        ### FA3.0                                          
#                 LET          RTN_PRB_HOME="Prober_abort"                                                                                             ### FA3.0                                          
                  RETURN                                                                                                                               ### AUTOMATION                                     
                END IF                                                                                                                                 ### AUTOMATION                                     
                                                                                                                                                                                                          
                IF             PROBER_HOME.Status=="OK"                                                                                                                                                   
                 IF            PROBER_HOME.Error=="#INIT_CHIP"                                                                                                                                            
                  LET          RTN_PRB_HOME="Wafer_ready"                                                                                                                                                 
                 END IF                                                                                                                                                                                   
                 IF            PROBER_HOME.Error=="#WAF_END"                                                                                                                                              
                  LET          RTN_PRB_HOME="Wafer_end"                                                                                                                                                   
                 END IF                                                                                                                                                                                   
                 RETURN                                                                                                                                                                                   
                ELSE                                                                                                                                                                                      
                 IF            PROBER_HOME.Error<>"#TIMEOUT_SRQ"                                                                                                                                          
                  LET          RTN_PRB_HOME="Prober_abort"                                                                                                                                                
                  LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_HOME: PROBER_HOME Error " + PROBER_HOME.Error                                                                                        
                  RETURN                                                                                                                                                                                  
                 END IF                                                                                                                                                                                   
                END IF                                                                                                                                                                                    
 Query_home_srq                                                                                                                                                                                           
                ACTION         "HP_PROBER_PRB_QUERY_READY_TELP12"                                                                                                                                         
                                                                                                                                                                                                          
                IF             RTN_PRB_QUERY_READY=="Wafer_end"                                                                                                                                           
                 LET           RTN_PRB_HOME="Wafer_end"                                                                                                                                                   
                ELSE                                                                                                                                                                                      
                 IF            RTN_PRB_QUERY_READY=="Wafer_ready"                                                                                                                                         
                  GOTO         Prober_home                                                                                                                                                                
                 ELSE                                                                                                                                                                                     
                  LET          RTN_PRB_HOME="Prober_abort"                                                                                                                                                
                  LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_HOME: PRB_QUERY_READY Error " + PROBER_READY.Error                                                                                   
                 END IF                                                                                                                                                                                   
                END IF                                                                                                                                                                                    
