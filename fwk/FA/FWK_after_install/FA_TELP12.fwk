                    ACTION   "FA_WAIT_ABORT_RESUME"                                                                                  
                    IF       FA_GEM_COMMAND=="ABORT"                                                                                 
                      LET    FA_STATUS="ABORT"                                                                                       
                      LET    FA_GEM_COMMAND=""                                                                                       
                      ACTION "FA_SEND_ABORTED"                                                                                       
