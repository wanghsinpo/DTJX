$Type: Action
$Name: HP_JDG_WAFER_AT_DIE
$Vers: 1
$Desc: Judge current wafer.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ---------------- -------------------------------------------------------------------------------------- - - -
 BODY                                                                                                              
      LET              RTN_JDG_WAFER_AT_DIE="Pass"                                                                 
                                                                                                                   
      IF               JDG_WAFER_YLD_ITEM<>0                                                                       
       JDG_WAF_BY_PARA Tgt_yld=JDG_WAFER_YLD_ITEM,Critical=JDG_WAFER_CRITIC_FLAG,Ignore=JDG_WAFER_IGNORE_FLAG      
       IF              JDG_WAF_BY_PARA.Jdg==0                                                                      
        LET            RTN_JDG_WAFER_AT_DIE="Fail"                                                                 
       END IF                                                                                                      
      END IF                                                                                                       
      IF               JDG_WAFER_YLD_DIE<>0                                                                        
       JDG_WAF_BY_DIE  Tgt_wafyld=JDG_WAFER_YLD_DIE,Tgt_dieyld=JDG_DIE_YLD_ITEM                                    
       IF              JDG_WAF_BY_DIE.Jdg==0                                                                       
        LET            RTN_JDG_WAFER_AT_DIE="Fail"                                                                 
       END IF                                                                                                      
      END IF                                                                                                       
$---- ---------------- -------------------------------------------------------------------------------------- - - -

$Type: Action
$Name: HP_JDG_WAFER_AT_WAFER
$Vers: 1
$Desc: Judge current wafer.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ---------------- --------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                     
      IF               JDG_WAFER_YLD_ITEM<>0                                                                              
       JDG_WAF_BY_PARA Tgt_yld=JDG_WAFER_YLD_ITEM,Last=1,Critical=JDG_WAFER_CRITIC_FLAG,Ignore=JDG_WAFER_IGNORE_FLAG      
       LET             TST_SLOT_YLD_ITEM[TST_SLOT_CURRENT]=JDG_WAF_BY_PARA.Yld                                            
      ELSE                                                                                                                
       LET             TST_SLOT_YLD_ITEM[TST_SLOT_CURRENT]=100                                                            
      END IF                                                                                                              
      IF               JDG_WAFER_YLD_DIE<>0                                                                               
       JDG_WAF_BY_DIE  Tgt_wafyld=JDG_WAFER_YLD_DIE,Tgt_dieyld=JDG_DIE_YLD_ITEM,Last=1                                    
       LET             TST_SLOT_YLD_DIE[TST_SLOT_CURRENT]=JDG_WAF_BY_DIE.Yld                                              
      ELSE                                                                                                                
       LET             TST_SLOT_YLD_DIE[TST_SLOT_CURRENT]=100                                                             
      END IF                                                                                                              
                                                                                                                          
      LET              WAFER.SYS_WAFERPASS=0                                                                              
                                                                                                                          
      IF               SYSTEM.REJECT<>""                                                                                  
       IF              JDG_WAFER_YLD_ITEM==0                                                                              
        IF             JDG_WAFER_YLD_DIE==0                                                                               
         LET           WAFER.SYS_WAFERPASS=1                                                                              
        END IF                                                                                                            
       END IF                                                                                                             
      ELSE                                                                                                                
       IF              (JDG_WAFER_YLD_ITEM==0) OR (JDG_WAF_BY_PARA.Jdg<>0)                                                
        IF             (JDG_WAFER_YLD_DIE==0) OR (JDG_WAF_BY_DIE.Jdg<>0)                                                  
         LET           WAFER.SYS_WAFERPASS=1                                                                              
        END IF                                                                                                            
       END IF                                                                                                             
      END IF                                                                                                              
                                                                                                                          
      IF               WAFER.SYS_WAFERPASS==1                                                                             
       LET             JDG_LOT_WAFER_PASS=JDG_LOT_WAFER_PASS+1                                                            
      ELSE                                                                                                                
       LET             JDG_LOT_WAFER_FAIL=JDG_LOT_WAFER_FAIL+1                                                            
      END IF                                                                                                              
