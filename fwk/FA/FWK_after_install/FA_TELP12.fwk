                 END IF                                                                                                                      
                                                                                                                                             
#                ACTION        "HP_PROBER_PRB_LOAD_RND_TELP12"                                                                               
                 ACTION        "FA_PRB_LOAD_RND_TELP12"                                                             ### AUTOMATION           
                                                                                                                                             
                 IF            RTN_PRB_LOAD_RND=="Align_reject"                                                                              
