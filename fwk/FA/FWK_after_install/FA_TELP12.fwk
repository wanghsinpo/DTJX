$Type: Action
$Name: FA_EXECLVL_CHG
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------- ----------------------------------------------- - - --------
 BODY                                                                     
      IF      FA_WAF_TST_COUNT==0                                         
        LET   FA_FAIL_RATE=0                                              
      ELSE                                                                
        LET   FA_TMPR1=FA_WAF_FAIL_COUNT                                  
        LET   FA_TMPR2=FA_WAF_TST_COUNT                                   
        LET   FA_FAIL_RATE=FA_TMPR1/FA_TMPR2                              
      END IF                                                              
      IF      FA_FAIL_RATE<=0.05                                          
        LET   FA_EXECLVL="1"                                              
        LET   FA_EXECRULE="GE"                                            
      ELSE IF FA_FAIL_RATE<=0.20                                          
        LET   FA_EXECLVL="1"                                              
        LET   FA_EXECRULE="GT"                                            
      ELSE IF FA_FAIL_RATE<=0.40                                          
        LET   FA_EXECLVL="2"                                              
        LET   FA_EXECRULE="GE"                                            
      ELSE IF FA_FAIL_RATE<=0.60                                          
        LET   FA_EXECLVL="3"                                              
        LET   FA_EXECRULE="GE"                                            
      ELSE IF FA_FAIL_RATE<=0.80                                          
        LET   FA_EXECLVL="4"                                              
        LET   FA_EXECRULE="GE"                                            
      ELSE IF FA_FAIL_RATE<=1.00                                          
        LET   FA_EXECLVL="5"                                              
        LET   FA_EXECRULE="EQ"                                            
      ELSE                                                                
        LET   FA_EXECLVL="0"                                              
        LET   FA_EXECRULE="EQ"                                            
      END IF                                                              
