                END IF                                                                                                                            
                                                                                                                                                  
                IF            TMP_PRB_STATUS=="OK"                                                                                                
                 IF           TMP_PRB_ERROR=="#UNLOAD"                                                                                            
                  LET         RTN_PRB_REALIGN="Wafer_end"                                                                                         
                 ELSE                                                                                                                             
                  LET         RTN_PRB_REALIGN="Wafer_ready"                                                                                       
                  LET         PNL_MSG_MSG1="$COMPLETE_ALIGN"                                                                                      
                  PANEL       "HP_DISP_MSG1_NONE",2                                                                                               
