$Type: Action
$Name: HP_EXEC_DIAG_HP4062UX
$Vers: 1
$Desc: Execute diagnosis for HP4062 series.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------------ ---------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                              
      LET          PNL_TTL_LABEL="$TTL_DIAG"                                                                                       
      LET          PNL_MSG_MSG1="$HOWTO_DIAG4062"                                                                                  
      PANEL        "HP_DISP_BLANK",2                                                                                               
      PANEL        "HP_DISP_MSG1_NONE",2                                                                                           
                                                                                                                                   
      TESTER_START Mode=0                                                                                                          
