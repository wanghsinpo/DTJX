$User: root
$---- -------------- ----------------------------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                                   
      LET            FIL_CUSTOMIZE=SYSTEM.FRAMEWORKPATH + "/" + SYSTEM.FRAMEWORK + ".cf"                                                                
                                                                                                                                                        
      ATTRIB_FILE    Filename=FIL_CUSTOMIZE                                                                                                             
      LET            FIL_PERM_CUSTOMIZE=ATTRIB_FILE.Permission                                                                                          
      LET            FIL_OWNER_CUSTOMIZE=ATTRIB_FILE.Owner                                                                                              
      LET            FIL_GROUP_CUSTOMIZE=ATTRIB_FILE.Group                                                                                              
                                                                                                                                                        
      UPDATE_FWKCONF File=FIL_CUSTOMIZE,Type=1                                                                                                          
      IF             UPDATE_FWKCONF.Errno<>0                                                                                                            
       LET           PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                                                  
       LET           PNL_MSG_MSG1="$ERR_UPDATE_CUSTOMIZE"                                                                                               
       LET           PNL_MSG_MSG2=UPDATE_FWKCONF.Errmsg                                                                                                 
       LET           PNL_KEY_OK="$HALT"                                                                                                                 
       PANEL         "HP_DISP_ERR2_OK",1                                                                                                                
