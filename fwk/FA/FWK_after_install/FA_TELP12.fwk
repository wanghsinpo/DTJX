$Type: Action
$Name: HP_PROBER_PRB_LOAD_RND_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- -------------- -------------------------------------------------------------------------------------------- - - --------------
 BODY                                                                                                                               
      IF             PRB_ONLINE==0                                                                                                  
       LET           RTN_PRB_LOAD_RND="Wafer_ready"                                                                                 
       RETURN                                                                                                                       
      END IF                                                                                                                        
                                                                                                                                    
      PROBER_SLOAD_I Cassno=PRB_LOAD_CASS,Slotno=PRB_LOAD_SLOT                                                        ### AUTOMATION
                                                                                                                                    
      LET            RTN_PRB_LOAD_RND="Prober_abort"                                                                                
                                                                                                                                    
      IF             PROBER_SLOAD_I.Status=="ERROR"                                                                   ### AUTOMATION
       IF            PROBER_SLOAD_I.Error=="#TIMEOUT_SRQ"                                                             ### AUTOMATION
        ACTION       "HP_PROBER_PRB_QUERY_READY_TELP12"                                                                             
        LET          RTN_PRB_LOAD_RND=RTN_PRB_QUERY_READY                                                                           
        IF           RTN_PRB_LOAD_RND=="Prober_abort"                                                                               
         LOG_PROBER  Session=SYSTEM.SESSIONID,Message="PRB_LOAD_RND: PRB_QUERY_READY Error " + PROBER_READY.Error                   
        END IF                                                                                                                      
        RETURN                                                                                                                      
       END IF                                                                                                                       
       IF            PROBER_SLOAD_I.Error=="#ALIGN_REJECT"                                                            ### AUTOMATION
        LET          RTN_PRB_LOAD_RND="Align_reject"                                                                                
       END IF                                                                                                                       
       IF            PROBER_SLOAD_I.Error=="#ALIGN_ASSIST"                                                            ### AUTOMATION
        LET          RTN_PRB_LOAD_RND="Align_assist"                                                                                
       END IF                                                                                                                       
       IF            PROBER_SLOAD_I.Error=="#NOWAFER"                                                                 ### AUTOMATION
        LET          RTN_PRB_LOAD_RND="No_wafer"                                                                                    
       END IF                                                                                                                       
      END IF                                                                                                                        
                                                                                                                                    
      IF             PROBER_SLOAD_I.Status=="END"                                                                     ### AUTOMATION
       LET           RTN_PRB_LOAD_RND="Lot_end"                                                                                     
      END IF                                                                                                                        
                                                                                                                                    
      IF             PROBER_SLOAD_I.Status=="OK"                                                                      ### AUTOMATION
       LET           RTN_PRB_LOAD_RND="Wafer_ready"                                                                                 
      END IF                                                                                                                        
                                                                                                                                    
      IF             RTN_PRB_LOAD_RND=="Prober_abort"                                                                               
       LOG_PROBER    Session=SYSTEM.SESSIONID,Message="PRB_LOAD_RND: PROBER_SLOAD Error " + PROBER_SLOAD_I.Error      ### AUTOMATION
      END IF                                                                                                                        
