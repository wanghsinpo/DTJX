        END IF                                                                                                                             
       ELSE                                                                                                                                
        LET           RTN_PRB_GET_CHKSLOT="No_cass"                                                                                        
        LET           TST_SLOT_CURRENT=0                                                                                                   
       END IF                                                                                                                              
      ELSE                                                                                                                                 
       LET            RTN_PRB_GET_CHKSLOT="Prober_abort"                                                                                   
       LOG_PROBER     Session=SYSTEM.SESSIONID,Message="PRB_GET_CHKSLOT: PROBER_CASSINFO Error " + PROBER_CASSINFO.Error                   
      END IF                                                                                                                               
