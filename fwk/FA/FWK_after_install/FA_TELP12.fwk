$Type: Action
$Name: FA_RESELECT_SLOT
$Vers: 1
$Desc: Desc: This is for RETEST. Select again.\nDate: '01/Jan/08 : Initial revision\n
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$------------ ---------------- ----------------------------------------------- - - ---------------------------------
 BODY                                                                                                               
 Set_slotmap                                                                                                        
              LET              FA_RTN_RESELECT_SLOT="NOWAFER"                      Initialize selected wafer        
              LET              TST_SLOT_CURRENT=1                                  Initialize current slot          
                                                                                                                    
              LET              TMP_SLOT_POS=0                                      ### FA3.0                        
              WHILE            TMP_SLOT_POS<PRB_SLOT_TOTAL_ALL                     ### FA3.0                        
                LET            TMP_SLOT_POS=TMP_SLOT_POS+1                         ### FA3.0                        
                LET            TST_SLOT_SELECT[TMP_SLOT_POS]=0                     ### FA3.0                        
                LET            TST_SLOT_LOT_INDEX[TMP_SLOT_POS]=0                  ### FA3.0                        
              END WHILE                                                            ### FA3.0                        
              IF               FA_WAF_RESELECT_AUTO == 0                           ### FA3.0                        
                GOTO           Normal                                              ### FA3.0                        
              END IF                                                               ### FA3.0                        
 ReSelect                                                                          ### FA3.0                        
                                                                                   ### FA3.0                        
              LET              FA_TST_SLOT_TOTAL=0                                 ### FA3.0                        
              LET              TMP_SLOT_POS=0                                      ### FA3.0                        
              SET_TAG_STRING   "PNL_TEST_CASS_STATE","$YET"                        ### FA3.0                        
              WHILE            TMP_SLOT_POS<PRB_SLOT_TOTAL_ALL                     ### FA3.0                        
                LET            TMP_SLOT_POS=TMP_SLOT_POS+1                         ### FA3.0                        
                READ_INTEGER   Format="%d",String=FA_WAF_LIST[TMP_SLOT_POS][1]                                      
                LET            FA_TMP_WAFNUMBER=READ_INTEGER.Value                                                  
                IF             (FA_TMP_WAFNUMBER>=1)AND(FA_TMP_WAFNUMBER<=25)      ### FA3.0                        
                  IF           PRB_SLOT_VALID[FA_TMP_WAFNUMBER]==1                 ### FA3.0                        
                    LET        FA_TST_SLOT_TOTAL=FA_TST_SLOT_TOTAL+1               ### FA3.0                        
                    IF         TST_SLOT_SELECT[FA_TMP_WAFNUMBER]==0                ### FA3.0                        
                      LET      TST_SLOT_SELECT[FA_TMP_WAFNUMBER]=1                 ### FA3.0                        
                      LET      TST_SLOT_LOT_INDEX[FA_TMP_WAFNUMBER]=1              ### FA3.0                        
                      GOTO     NextWafer                                           ### FA3.0                        
                    ELSE                                                           ### FA3.0                        
                      GOTO     AutoReselect                                        ### FA3.0                        
                    END IF                                                         ### FA3.0                        
                  END IF                                                           ### FA3.0                        
 AutoReselect                                                                      ### FA3.0                        
                  LET          TMP_SLOT_POS2=0                                     ### FA3.0                        
                  WHILE        TMP_SLOT_POS2<PRB_SLOT_TOTAL_ALL                    ### FA3.0                        
                    LET        TMP_SLOT_POS2=TMP_SLOT_POS2+1                       ### FA3.0                        
                    LET        TMP_SLOT_POS3=FA_TMP_WAFNUMBER+TMP_SLOT_POS2        ### FA3.0                        
                    IF         (1<=TMP_SLOT_POS3)AND(TMP_SLOT_POS3<=25)            ### FA3.0                        
                      IF       PRB_SLOT_VALID[TMP_SLOT_POS3]==1                    ### FA3.0                        
                        IF     TST_SLOT_SELECT[TMP_SLOT_POS3]==0                   ### FA3.0                        
                          LET  TST_SLOT_SELECT[TMP_SLOT_POS3]=1                    ### FA3.0                        
                          LET  TST_SLOT_LOT_INDEX[TMP_SLOT_POS3]=1                 ### FA3.0                        
                          GOTO NextWafer                                           ### FA3.0                        
                        END IF                                                     ### FA3.0                        
                      END IF                                                       ### FA3.0                        
                    END IF                                                         ### FA3.0                        
                    LET        TMP_SLOT_POS3=FA_TMP_WAFNUMBER-TMP_SLOT_POS2        ### FA3.0                        
                    IF         (1<=TMP_SLOT_POS3)AND(TMP_SLOT_POS3<=25)            ### FA3.0                        
                      IF       PRB_SLOT_VALID[TMP_SLOT_POS3]==1                    ### FA3.0                        
                        IF     TST_SLOT_SELECT[TMP_SLOT_POS3]==0                   ### FA3.0                        
                          LET  TST_SLOT_SELECT[TMP_SLOT_POS3]=1                    ### FA3.0                        
                          LET  TST_SLOT_LOT_INDEX[TMP_SLOT_POS3]=1                 ### FA3.0                        
                          GOTO NextWafer                                           ### FA3.0                        
                        END IF                                                     ### FA3.0                        
                      END IF                                                       ### FA3.0                        
                    END IF                                                         ### FA3.0                        
                                                                                   ### FA3.0                        
                  END WHILE                                                        ### FA3.0                        
                END IF                                                             ### FA3.0                        
 NextWafer                                                                         ### FA3.0                        
              END WHILE                                                            ### FA3.0                        
              RETURN                                                               ### FA3.0                        
 Normal                                                                            ### FA3.0                        
                                                                                                                    
              LET              FA_TST_SLOT_TOTAL=0                                 ### FA3.0                        
              LET              TMP_SLOT_POS=0                                      Initialize current slot number   
              SET_TAG_STRING   "PNL_TEST_CASS_STATE","$YET"                        Initialize all of slot status    
              WHILE            TMP_SLOT_POS<PRB_SLOT_TOTAL_ALL                     Current slot number < Total slots
                LET            TMP_SLOT_POS=TMP_SLOT_POS+1                         Increment slot number            
                READ_INTEGER   Format="%d",String=FA_WAF_LIST[TMP_SLOT_POS][1]                                      
                LET            FA_TMP_WAFNUMBER=READ_INTEGER.Value                                                  
                IF             (FA_TMP_WAFNUMBER>=1)AND(FA_TMP_WAFNUMBER<=25)      ### FA3.0                        
                  IF           PRB_SLOT_VALID[TMP_SLOT_POS]==1                     ### FA3.0                        
                    IF         TST_SLOT_SELECT[FA_TMP_WAFNUMBER]==0                ### FA3.0                        
                      LET      FA_TST_SLOT_TOTAL=FA_TST_SLOT_TOTAL+1               ### FA3.0                        
                      LET      TST_SLOT_SELECT[FA_TMP_WAFNUMBER]=1                 ### FA3.0                        
                      LET      TST_SLOT_LOT_INDEX[FA_TMP_WAFNUMBER]=1              ### FA3.0                        
                    ELSE                                                           ### FA3.0                        
                      LET      TST_SLOT_SELECT[FA_TMP_WAFNUMBER]=0                 ### FA3.0                        
                      LET      TST_SLOT_LOT_INDEX[FA_TMP_WAFNUMBER]=0              ### FA3.0                        
                    END IF                                                         ### FA3.0                        
                  END IF                                                           ### FA3.0                        
                END IF                                                                                              
              END WHILE                                                                                             
