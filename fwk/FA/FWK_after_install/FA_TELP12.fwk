$Type: Action
$Name: HP_GET_RECIPE_SINGLE
$Vers: 1
$Desc: Get test recipe.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- -------- ------------------------------------------------------------ - - -
 BODY                                                                            
      IF       TST_TYPE_USE=="1L1C"                                              
       LET     PNL_KEYCASS_LABEL="$CASSID_RECIPE"                                
      ELSE                                                                       
       IF      TST_RCP_INDEX==1                                                  
        LET    PNL_KEYCASS_LABEL="$CASSID1"                                      
       ELSE                                                                      
        LET    PNL_KEYCASS_LABEL="$CASSID2"                                      
       END IF                                                                    
      END IF                                                                     
                                                                                 
      IF       DSP_CASSID==1                                                     
       PANEL   "HP_SET_KEY_CASS"                                                 
       PANEL   "",0                                                              
                                                                                 
       IF      PANEL.RETURN=="MAIN"                                              
        LET    RTN_PLG_GET_RECIPE="Main"                                         
        RETURN                                                                   
       END IF                                                                    
      END IF                                                                     
      ACTION   "HP_TEST_QUERY_RECIPE_1L1C"                                       
                                                                                 
      IF       RTN_TEST_QUERY_RECIPE=="Error"                                    
       LET     PNL_TTL_LABEL="$TTL_WARNING"                                      
       LET     PNL_MSG_MSG1=RTN_TEST_QUERY_RECIPE_ERRMSG                         
       IF      UIF_SET_RECIPE==0                                                 
        LET    PNL_KEY_OK="$OK"                                                  
        PANEL  "HP_DISP_ERR1_OK"                                                 
        PANEL  "HP_DISP_ERR1_OK",0                                               
       ELSE                                                                      
        PANEL  "HP_DISP_ERR1_RETRY_NEXT"                                         
        PANEL  "HP_DISP_ERR1_RETRY_NEXT",0                                       
       END IF                                                                    
       IF      (PANEL.RETURN=="RETRY") OR (UIF_SET_RECIPE==0)                    
        LET    RTN_PLG_GET_RECIPE="Retry"                                        
       ELSE                                                                      
        LET    RTN_PLG_GET_RECIPE="Continue"                                     
       END IF                                                                    
       RETURN                                                                    
      END IF                                                                     
                                                                                 
      IF       TST_RCP_TPL<>""                                                   
       LET     TST_RCP_TPL = DIR_TESTPLAN + "/" + TST_RCP_TPL + ".tpl"           
      END IF                                                                     
      IF       TST_RCP_LIMIT<>""                                                 
       LET     TST_RCP_LIMIT = DIR_LIMITFILE + "/" + TST_RCP_LIMIT + ".lim"      
      END IF                                                                     
                                                                                 
      LET      RTN_PLG_GET_RECIPE="Continue"                                     
$---- -------- ------------------------------------------------------------ - - -

$Type: Action
$Name: HP_JDG_DIE_AT_DIE
$Vers: 1
$Desc: Judge current die.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- --------- --------------------------------------------------------------------------------------- - - -
 BODY                                                                                                        
      IF        SYSTEM.REJECT<>""                                                                            
       LET      DIE.SYS_DIEPASS=0                                                                            
      ELSE                                                                                                   
       IF       JDG_DIE_YLD_ITEM==0                                                                          
        LET     DIE.SYS_DIEPASS=1                                                                            
       ELSE                                                                                                  
        JDG_DIE Tgt_yld=JDG_DIE_YLD_ITEM,Last=1,Critical=JDG_DIE_CRITIC_FLAG,Ignore=JDG_DIE_IGNORE_FLAG      
        IF      JDG_DIE.Jdg<>0                                                                               
         LET    DIE.SYS_DIEPASS=1                                                                            
        ELSE                                                                                                 
         LET    DIE.SYS_DIEPASS=0                                                                            
        END IF                                                                                               
       END IF                                                                                                
      END IF                                                                                                 
$---- --------- --------------------------------------------------------------------------------------- - - -

