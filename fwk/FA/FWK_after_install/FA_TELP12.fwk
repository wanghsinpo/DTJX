       LOG_SYSTEM    Session=SYSTEM.SESSIONID,Message="FA_TRANSFER_FWK: UPDATE_FWKCONF Error " + UPDATE_FWKCONF.Errmsg + "(" + FIL_CUSTOMIZE + ")"      
       IF            TST_TESTER_UPBYOTHER=0                                                                                                             
        TESTER_STOP                                                                                                                                     
       END IF                                                                                                                                           
       STOP                                                                                                                                             
      END IF                                                                                                                                            
                                                                                                                                                        
      EXECUTE        "/usr/bin/chmod " + FIL_PERM_CUSTOMIZE + " " + FIL_CUSTOMIZE                                                                       
      EXECUTE        "/usr/bin/chown " + FIL_OWNER_CUSTOMIZE + ":" + FIL_GROUP_CUSTOMIZE + " " + FIL_CUSTOMIZE                                          
                                                                                                                                                        
      LET            FIL_CUSTOMIZE2=SYSTEM.FRAMEWORKPATH + "/" + SYSTEM.FRAMEWORK + ".fa"                                                               
                                                                                                                                                        
