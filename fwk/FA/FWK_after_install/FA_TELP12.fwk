                   GOTO         Ready_next_wafer                                                                                           
                  END IF                                                                                                                   
                  IF            RTN_PRB_QUERY_READY=="Align_assist"                                                                        
                   GOTO         Query_assist                                                                                               
                  END IF                                                                                                                   
                  IF            RTN_PRB_QUERY_READY=="Align_reject"                                                                        
                   GOTO         Reject_wafer                                                                                               
                  END IF                                                                                                                   
                  LET           RTN_LOAD_ONE="Prober_abort"                                                           "Prober_abort"       
                  RETURN                                                                                                                   
 Ready_next_wafer                                                                                                                          
                  LET           RTN_LOAD_ONE="Wafer_ready"                                                                                 
