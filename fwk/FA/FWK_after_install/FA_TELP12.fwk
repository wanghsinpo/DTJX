$Name: FA_CHECK_MULTIMEAS
$Vers: 1
$Desc: Desc: Check files for test plan and limit file for Multiple MEAS Lib\n### FA 3.3. All of lines of this action specs is new for FA 3.3.\n
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- -------------- ---------------------------------------------------------------- - - ----------------------------
 BODY                                                                                                                 
      LET            FA_STATUS=""                                                                                     
      LET            FA_STATUSNO=""                                                       ##PPBadcompileCode was added
                                                                                                                      
      FA_CHK_MEASLIB Revcont=FA_REV_CONTROL,Lib=FA_TMP_MEAS_MNAME,Libnum=FA_MEAS_INPP                                 
      IF             FA_CHK_MEASLIB.Status <> 0                                                                       
        LET          FA_STATUS="ERROR"                                                                                
        PROBER_LOGWR Message="[Error:Meas Lib check error]"+FA_CHK_MEASLIB.Error          For debugging               
        IF           FA_CHK_MEASLIB.Status==-1                                            Duplicated with Prober Lib  
         LET         FA_STATUSNO="510:Duplicated with Prober library"                     #              |            
        END IF                                                                                                        
        IF           FA_CHK_MEASLIB.Status==-2                                            Duplicated with Tester Lib  
         LET         FA_STATUSNO="511:Duplicated with Tester library"                     #              |            
        END IF                                                                                                        
        IF           FA_CHK_MEASLIB.Status==-3                                            Duplicated with Utility Lib 
         LET         FA_STATUSNO="512:Duplicated with Utility library"                    #              |            
        END IF                                                                                                        
        IF           FA_CHK_MEASLIB.Status==-4                                            Cannot read *.lib           
         LET         FA_STATUSNO="513:No read permission for *.lib file"                  #              |            
        END IF                                                                                                        
        IF           FA_CHK_MEASLIB.Status==-5                                            Cannot read *.sl            
         LET         FA_STATUSNO="514:No read permission for *.sl file"                   #              |            
        END IF                                                                                                        
        RETURN                                                                                                        
      END IF                                                                                                          
