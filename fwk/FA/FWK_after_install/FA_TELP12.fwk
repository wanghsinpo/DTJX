                LET           RTN_PRB_REALIGN="Wafer_ready"                                                                                       
                WAIT          3                                                                                                                   
                RETURN                                                                                                                            
               END IF                                                                                                                             
                                                                                                                                                  
               PROBER_ALIGN_I                                                                                               ### AUTOMATION        
                                                                                                                                                  
 ErrorHandling                                                                                                                                    
                LET           TMP_PRB_STATUS=PROBER_ALIGN_I.Status                                                          ### AUTOMATION        
                LET           TMP_PRB_ERROR=PROBER_ALIGN_I.Error                                                            ### AUTOMATION        
                                                                                                                                                  
                LET           RTN_PRB_REALIGN="Prober_abort"                                                                                      
                                                                                                                                                  
