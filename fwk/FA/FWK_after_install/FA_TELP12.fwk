$Type: Action
$Name: FA_WAIT_ABORT
$Vers: 1
$Desc: Desc: Error handling when the error happen\nDate: '01/Jan/06 Initial revision
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$------------ ------------ -------------------------------------------------------------------------------------------------------------------------------- - - --------------------------
 BODY                                                                                                                                                                                     
              IF           (FA_EQUIP_STATE=="ABORTING") OR (FA_EQUIP_STATE=="STOPPING") OR (FA_EQUIP_STATE=="PJABORTING") OR (FA_EQUIP_STATE=="CJABORTING")     ### FA3.0                 
                RETURN                                                                                                                                                                    
              END IF                                                                                                                                                                      
                                                                                                                                                                                          
              PANEL        "FA_DISP_ERR2_NONE",2                                                                                                                Show error panel          
                                                                                                                                                                                          
 Send_alarmID IF           FA_ALID<>""                                                                                                                          If error cause from Tester
                ACTION     "FA_SEND_ALARM"                                                                                                                                                
                LET        FA_PAUSE_ON="ON"                                                                                                                     ### FA3.0                 
                LET        FA_ALARM_ON="ON"                                                                                                                     ### FA3.0                 
              END IF                                                                                                                                                                      
                                                                                                                                                                                          
 Repeat_next  ACTION       "FA_CHECK_MAIN_COMMAND"                                                                                                                                        
              IF           FA_GEM_COMMAND=="STOP"                                                                                                               Stop command              
                ACTION     "FA_SEND_ACK"                                                                                                                                                  
                PANEL      "FA_DISP_ERR2_NONE",0                                                                                                                Close error panel         
                LET        PNL_MSG_MSG2=""                                                                                                                                                
                RETURN                                                                                                                                                                    
              END IF                                                                                                                                                                      
              IF           FA_GEM_COMMAND=="ABORT"                                                                                                              Abort command             
                ACTION     "FA_SEND_ACK"                                                                                                                                                  
                PANEL      "FA_DISP_ERR2_NONE",0                                                                                                                Close error panel         
                LET        PNL_MSG_MSG2=""                                                                                                                                                
                RETURN                                                                                                                                                                    
              END IF                                                                                                                                                                      
              IF           FA_GEM_COMMAND=="PJABORT"                                                                                                            ### FA3.0                 
                ACTION     "FA_SEND_ACK"                                                                                                                        ### FA3.0                 
                IF         FA_PORTNUMBER==FA_RESERVED_PJAPORTNUMBER                                                                                             ### FA3.0                 
                  REJECT   LOT                                                                                                                                  ### FA3.0                 
                  LET      FA_EQUIP_STATE="PJABORTING"                                                                                                          ### FA3.0                 
                  LET      PRB_ABORT_ALL=1                                                                                                                      ### FA3.0                 
                ELSE                                                                                                                                            ### FA3.0                 
                  LET      FA_PJABORT_DIFPORT=1                                                                                                                 ### FA3.0                 
                  LET      FA_TMP_PJABORTID=FA_RESERVED_PJAPORTNUMBER                                                                                           ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
                IF         FA_RSV_PORTNUMBER==1                                                                                                                 ### FA3.0                 
                  LET      FA_RESERVED_PPSELECT1="OFF"                                                                                                          ### FA3.0                 
                  LET      FA_RESERVED_START1="OFF"                                                                                                             ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
                IF         FA_RSV_PORTNUMBER==2                                                                                                                 ### FA3.0                 
                  LET      FA_RESERVED_PPSELECT2="OFF"                                                                                                          ### FA3.0                 
                  LET      FA_RESERVED_START2="OFF"                                                                                                             ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
                IF         FA_PAUSE_ON=="ON"                                                                                                                    ### FA3.0                 
                  REJECT   LOT                                                                                                                                  ### FA3.0                 
                  LET      FA_EQUIP_STATE="PJABORTING"                                                                                                          ### FA3.0                 
                  LET      PRB_ABORT_ALL=1                                                                                                                      ### FA3.0                 
                  LET      FA_PAUSE_ON="OFF"                                                                                                                    ### FA3.0                 
                  IF       FA_ALARM_ON=="ON"                                                                                                                    ### FA3.0                 
                    LET    FA_ALARM_ON="OFF"                                                                                                                    ### FA3.0                 
                    LET    FA_RESERVED_PPSELECT1="OFF"                                                                                                          ### FA3.0                 
                    LET    FA_RESERVED_PPSELECT2="OFF"                                                                                                          ### FA3.0                 
                    LET    FA_RESERVED_START1="OFF"                                                                                                             ### FA3.0                 
                    LET    FA_RESERVED_START2="OFF"                                                                                                             ### FA3.0                 
                    PANEL  "FA_DISP_ERR2_NONE",0                                                                                                                ### FA3.0                 
                    LET    FA_EQUIP_STATE="ABORTING"                                                                                                            ### FA3.0                 
                    RETURN                                                                                                                                      ### FA3.0                 
                  END IF                                                                                                                                        ### FA3.0                 
                  ACTION   "FA_SEND_RESUMED"                                                                                                                    ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
#               ACTION     "FA_SEND_PJABORTED"                                                                                                                  ### FA3.0                 
                REJECT     LOT                                                                                                                                  ### FA3.0                 
                RETURN                                                                                                                                          ### FA3.0                 
              END IF                                                                                                                                            ### FA3.0                 
              IF           FA_GEM_COMMAND=="CJABORT"                                                                                                            ### FA3.0                 
                ACTION     "FA_SEND_ACK"                                                                                                                        ### FA3.0                 
                IF         FA_PORTNUMBER==FA_RESERVED_PJAPORTNUMBER                                                                                             ### FA3.0                 
                  REJECT   LOT                                                                                                                                  ### FA3.0                 
                  LET      FA_EQUIP_STATE="CJABORTING"                                                                                                          ### FA3.0                 
                  LET      PRB_ABORT_ALL=1                                                                                                                      ### FA3.0                 
                ELSE                                                                                                                                            ### FA3.0                 
                  LET      FA_PJABORT_DIFPORT=1                                                                                                                 ### FA3.0                 
                  LET      FA_TMP_PJABORTID=FA_RESERVED_PJAPORTNUMBER                                                                                           ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
                IF         FA_RSV_PORTNUMBER==1                                                                                                                 ### FA3.0                 
                  LET      FA_RESERVED_PPSELECT1="OFF"                                                                                                          ### FA3.0                 
                  LET      FA_RESERVED_START1="OFF"                                                                                                             ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
                IF         FA_RSV_PORTNUMBER==2                                                                                                                 ### FA3.0                 
                  LET      FA_RESERVED_PPSELECT2="OFF"                                                                                                          ### FA3.0                 
                  LET      FA_RESERVED_START2="OFF"                                                                                                             ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
                IF         FA_PAUSE_ON=="ON"                                                                                                                    ### FA3.0                 
                  REJECT   LOT                                                                                                                                  ### FA3.0                 
                  LET      FA_EQUIP_STATE="CJABORTING"                                                                                                          ### FA3.0                 
                  LET      PRB_ABORT_ALL=1                                                                                                                      ### FA3.0                 
                  LET      FA_PAUSE_ON="OFF"                                                                                                                    ### FA3.0                 
                  IF       FA_ALARM_ON=="ON"                                                                                                                    ### FA3.0                 
                    LET    FA_ALARM_ON="OFF"                                                                                                                    ### FA3.0                 
                    LET    FA_RESERVED_PPSELECT1="OFF"                                                                                                          ### FA3.0                 
                    LET    FA_RESERVED_PPSELECT2="OFF"                                                                                                          ### FA3.0                 
                    LET    FA_RESERVED_START1="OFF"                                                                                                             ### FA3.0                 
                    LET    FA_RESERVED_START2="OFF"                                                                                                             ### FA3.0                 
                    PANEL  "FA_DISP_ERR2_NONE",0                                                                                                                ### FA3.0                 
                    LET    FA_EQUIP_STATE="ABORTING"                                                                                                            ### FA3.0                 
                    RETURN                                                                                                                                      ### FA3.0                 
                  END IF                                                                                                                                        ### FA3.0                 
                  ACTION   "FA_SEND_RESUMED"                                                                                                                    ### FA3.0                 
                END IF                                                                                                                                          ### FA3.0                 
#               ACTION     "FA_SEND_PJABORTED"                                                                                                                  ### FA3.0                 
                LET        FA_PJABORT="ON"                                                                                                                      ### FA3.0                 
                RETURN                                                                                                                                          ### FA3.0                 
              END IF                                                                                                                                            ### FA3.0                 
              IF           FA_GEM_COMMAND=="AlarmCleared"                                                                                                       ### FA3.0                 
                LET        FA_GEM_COMMAND=""                                                                                                                    Abort command             
                PANEL      "FA_DISP_ERR2_NONE",0                                                                                                                ### FA3.0                 
                LET        FA_ALARM_ON="OFF"                                                                                                                    ### FA3.0                 
              END IF                                                                                                                                            ### FA3.0                 
              IF           FA_GEM_COMMAND<>""                                                                                                                   Another command           
                ACTION     "FA_SEND_NACK"                                                                                                                                                 
              END IF                                                                                                                                                                      
              WAIT         Wait=2                                                                                                                                                         
 Until        GOTO         Repeat_next                                                                                                                          Loop to check command     
