$Type: Action
$Name: HP_PROBER_PAUSE_TELP12
$Vers: 1
$Desc: Description for this Panel spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- -------- ---------------------------------- - - -
 BODY                                                  
      ACTION   "HP_PROBER_PRB_PAUSE_TELP12"            
                                                       
      IF       RTN_PRB_PAUSE=="Wafer_cont"             
       ACTION  "HP_PROBER_PRB_HOME_TELP12"             
       IF      RTN_PRB_HOME=="Wafer_end"               
        LET    PRB_ABORT_WAFER=-1                      
        REJECT WAFER                                   
       END IF                                          
       IF      RTN_PRB_HOME=="Prober_abort"            
        LET    PRB_ABORT_ALL=-1                        
        REJECT LOT                                     
       END IF                                          
      ELSE                                             
       IF      RTN_PRB_PAUSE=="Wafer_end"              
        LET    PRB_ABORT_WAFER=-1                      
        REJECT WAFER                                   
       ELSE                                            
        LET    PRB_ABORT_ALL=-1                        
        REJECT LOT                                     
       END IF                                          
      END IF                                           
                                                       
      IF       PRB_ABORT_ALL==-1                       
       LET     PNL_TTL_LABEL="$TTL_PROBER_ERROR"       
       LET     PNL_MSG_MSG1="$TELP12_FATAL_ERROR"      
       LET     PNL_KEY_OK="$OK"                        
       PANEL   "HP_DISP_ERR1_OK",2                     
       PANEL   "HP_DISP_ERR1_OK",0                     
      END IF                                           
