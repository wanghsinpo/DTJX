$Type: Action
$Name: FA_PRB_SETCMAP_ODR_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$----------- -------------- ---------------------------------------- - - ---------
 BODY                                                                             
             IF             PRB_ONLINE==0                                         
               LET          FA_RTN_PRB_SETCMAP="OK"                               
               RETURN                                                             
             END IF                                                               
             LET            TMP_I=0                                               
             WHILE          TMP_I<25                                              
               LET          TMP_I=TMP_I+1                                         
               READ_INTEGER Format="%d",String=FA_WAF_LIST[TMP_I][1]              
               LET          FA_SLOTODR[TMP_I]=READ_INTEGER.Value                  
             END WHILE                                                            
             IF             FA_WAF_RESELECT_AUTO == 1                    ### FA3.0
               LET          TMP_I=0                                      ### FA3.0
               LET          TMP_SLOT_POS=0                               ### FA3.0
               WHILE        TMP_SLOT_POS<PRB_SLOT_TOTAL_ALL              ### FA3.0
                 LET        TMP_SLOT_POS=TMP_SLOT_POS+1                  ### FA3.0
                 IF         TST_SLOT_SELECT[TMP_SLOT_POS]==1             ### FA3.0
                   LET      TMP_I=TMP_I+1                                ### FA3.0
                   LET      FA_SLOTODR[TMP_I]=TMP_SLOT_POS               ### FA3.0
                 END IF                                                  ### FA3.0
               END WHILE                                                 ### FA3.0
             END IF                                                      ### FA3.0
 Prober_home                                                                      
             PROBER_CMAPODR Cassno=FA_PORTNUMBER,Slotstat=FA_SLOTODR              
                                                                                  
             IF             PROBER_CMAPODR.Status=="OK"                           
               LET          FA_RTN_PRB_SETCMAP="OK"                               
               RETURN                                                             
             END IF                                                               
                                                                                  
             IF             PROBER_CMAPODR.Status=="Error"                        
               LET          FA_RTN_PRB_SETCMAP="Error"                            
             END IF                                                               
