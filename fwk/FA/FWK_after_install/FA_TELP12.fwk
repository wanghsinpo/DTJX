                  IF            TMP_CASS_SEL[TMP_SLOT_POS+5]>0                                                       
                   LET          TST_SLOT_SELECT[TMP_SLOT_POS]=1                                                      
                   LET          PRB_SLOT_SELECT[TMP_SLOT_POS]=PRB_UNPROBED                          PRB_UNPROBED:3   
                  ELSE                                                                                               
                   LET          PRB_SLOT_SELECT[TMP_SLOT_POS]=PRB_SKIP                              PRB_SKIP:1       
                  END IF                                                                                             
                  LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                                          
                 UNTIL          TMP_SLOT_POS>50                                                                      
                 LET            PRB_CASSNO=2                                                                         
                 ACTION         "HP_PROBER_SETCMAP"                                                                  
                END IF                                                                                               
                                                                                                                     
 Verify_waferid                                                                                                      
                ACTION          "HP_TEST_VERIFY_WAFERID_1OR2"                                                        
