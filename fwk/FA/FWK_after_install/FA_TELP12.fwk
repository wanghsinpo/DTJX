$Type: Action
$Name: FA_CHECK_STATUSBYTE
$Vers: 1
$Desc: Desc: Statusbyte checker.\nDate: '01/Jan/08 Initial revision
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ---------- ---------------------------------- - - --------------------------------------
 BODY                                                                                         
      LET        RTN_PRB_QUERY_READY="Wafer_ready"      It means no statuebyte                
      PROBER_SRQ Timeout=1                                                                    
                                                                                              
      IF         PROBER_SRQ.Statusbyte>0                                                      
        IF       PROBER_SRQ.Statusbyte==78              Modified in TEL                       
          LET    FA_ASSIST="DONE"                       Modified in TEL Assist SRQ is reached.
        END IF                                          Modified in TEL                       
        IF       PROBER_SRQ.Statusbyte==112             ### FA3.4                             
          LET    FA_START_OK=1                          ### FA3.4                             
        END IF                                          ### FA3.4                             
        LET      RTN_PRB_QUERY_READY="Prober_abort"                                           
      END IF                                                                                  
