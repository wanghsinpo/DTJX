$-------------- ---------- ----------------------------------------------------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                                                                 
                IF         PRB_ONLINE==0                                                                                                                                              
                 LET       RTN_PROBER_TEMPSET="Continue"                                                                                                                              
                 RETURN                                                                                                                                                               
                END IF                                                                                                                                                                
                LET        RTN_PROBER_TEMPSET="Continue"                                                                                                                              
                                                                                                                                                                                      
 WarningMessage                                                                                                                                                                       
                LET        PNL_TTL_LABEL="$TTL_TEMPSET"                                                                                                                               
                IF         WAFER.TMP_PRB_SUB_ALGN_AUTO[1]==PNL_CHOICE_ALIGN[1]                                                                                                        
                 LET       PNL_MSG_MSG1="$AUTO_REALIGN"                                                                                                                               
                ELSE                                                                                                                                                                  
                 LET       PNL_MSG_MSG1="$WARNING_TEMPSET"                                                                                                                            
                END IF                                                                                                                                                                
                LET        PNL_KEY_F1="$CONTINUE"                                                                                                                                     
                LET        PNL_KEY_F4="$CANCEL"                                                                                                                                       
                PANEL      "HP_DISP_MSG1_F1_F4",2                                                                                                                                     
                PANEL      "HP_DISP_MSG1_F1_F4",0                                                                                                                                     
                                                                                                                                                                                      
                IF         PANEL.RETURN=="F1"                                                                                                                                         
                 LET       PNL_MSG_MSG1="$EXEC_TEMPSET"                                                                                                                               
                 LET       PNL_KEY_F4="$ABORT_TEMPSET"                                                                                                                                
                 PANEL     "HP_PROBER_TEMPSET",2                                                                                                                                      
 SetTemperature                                                                                                                                                                       
                 ACTION    "HP_PROBER_PRB_TEMPSET_TELP12"                                                                                                                             
                 IF        RTN_PRB_TEMPSET<>"Wafer_ready"                                                                                                                             
                  LET      PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                                                          
                  LET      PNL_MSG_MSG1="$TEMPSET_FAIL"                                                                                                                               
                  LET      PNL_KEY_OK="$OK"                                                                                                                                           
                  PANEL    "HP_DISP_ERR1_OK"                                                                                                                                          
                  PANEL    "HP_DISP_ERR1_OK",0                                                                                                                                        
                  RETURN                                                                                                                                                              
                 END IF                                                                                                                                                               
 WaitTempset                                                                                                                                                                          
                 LET       HP_TEMPSET_FINISH=0                                                                                                                                        
                 WHILE     ((WAFER.TMP_CURRTMP-WAFER.PRB_SUB_TEMP>WAFER.PRB_SUB_TRLC) OR (WAFER.TMP_CURRTMP-WAFER.PRB_SUB_TEMP<-1*WAFER.PRB_SUB_TRLC)) AND (HP_TEMPSET_FINISH=0)      
                  WAIT     Wait=HP_TEMPMON_INTERVAL                                                                                                                                   
                                                                                                                                                                                      
                  ACTION   "HP_PROBER_PRB_TEMPMON_TELP12"                                                                                                                             
                  IF       RTN_PRB_TEMPMON<>"Wafer_ready"                                                                                                                             
