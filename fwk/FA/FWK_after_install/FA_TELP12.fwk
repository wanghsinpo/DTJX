       STOP                                                                                                                        
      END IF                                                                                                                       
                                                                                                                                   
      EXECUTE      "rmb /opt/SPECS/sys/bin/DIAG4062UX"                                                                             
                                                                                                                                   
      TESTER_START Mode=TST_TESTER_MODE                                                                                            
      IF           TESTER_START.Stat==-1                                                                                           
       LET         PNL_TTL_LABEL="$TTL_TESTER_ERROR"                                                                               
       LET         PNL_MSG_MSG1="$ERR_DIAG_TESTER_START"                                                                           
       LET         PNL_MSG_MSG2=TESTER_START.Message                                                                               
       LET         PNL_KEY_OK="$HALT"                                                                                              
       PANEL       "HP_DISP_ERR2_OK",1                                                                                             
       LOG_SYSTEM  Session=SYSTEM.SESSIONID,Message="HP_EXEC_DIAG_HP4062UX: TESTER_START(Mode) Error " + TESTER_START.Message      
       STOP                                                                                                                        
      END IF                                                                                                                       
                                                                                                                                   
      PANEL        "HP_DISP_MSG1_NONE",0                                                                                           
      PANEL        "HP_DISP_BLANK",0                                                                                               
