$Name: HP_JDG_DIE_AT_MODULE
$Vers: 1
$Desc: Judge current die.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- -------- -------------------------------------------------------------------------------- - - -
 BODY                                                                                                
      LET      RTN_JDG_DIE_AT_MODULE="Pass"                                                          
                                                                                                     
      IF       JDG_DIE_YLD_ITEM>0                                                                    
       JDG_DIE Tgt_yld=JDG_DIE_YLD_ITEM,Critical=JDG_DIE_CRITIC_FLAG,Ignore=JDG_DIE_IGNORE_FLAG      
       IF      JDG_DIE.Jdg==0                                                                        
        LET    RTN_JDG_DIE_AT_MODULE="Fail"                                                          
       END IF                                                                                        
      END IF                                                                                         
$---- -------- -------------------------------------------------------------------------------- - - -

$Type: Action
$Name: HP_JDG_LOT_AT_LOT
$Vers: 1
$Desc: Judge current lot.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------ ----------------------------------------------------------------------- - - ------------------------------------------------------------------------------------
 BODY                                                                                                                                                                        
      LET    TMP_LOT_YIELD=JDG_LOT_WAFER_PASS                                            Necessary to avoid calculating PASS/(PASS+FAIL).                                    
      IF     (JDG_LOT_WAFER_PASS+JDG_LOT_WAFER_FAIL)==0                                  ### FA3.0                                                                           
        LET  TMP_LOT_YIELD=0                                                             ### FA3.0                                                                           
      ELSE                                                                               ### FA3.0                                                                           
        LET  TMP_LOT_YIELD=TMP_LOT_YIELD/(JDG_LOT_WAFER_PASS+JDG_LOT_WAFER_FAIL)*100     (This should result 0 or 1 because always PASS <= (PASS+FAIL) and both are INTEGER.)
      END IF                                                                             ### FA3.0                                                                           
      IF     TMP_LOT_YIELD<JDG_LOT_YLD_WAFER                                                                                                                                 
       LET   LOT.SYS_LOTPASS=0                                                                                                                                               
      ELSE                                                                                                                                                                   
       LET   LOT.SYS_LOTPASS=1                                                                                                                                               
      END IF                                                                                                                                                                 
$---- ------ ----------------------------------------------------------------------- - - ------------------------------------------------------------------------------------

$Type: Action
$Name: HP_JDG_LOT_AT_WAFER
$Vers: 1
$Desc: Judge current wafer.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------ ------------------------------------------------------- - - ------------------------------------------------------------------------
 BODY                                                                                                                                            
      LET    TMP_LOT_YIELD=JDG_LOT_WAFER_FAIL                            Necessary to avoid calculating JDG_LOT_WAFER_FAIL/JDG_LOT_WAFER_VALID.  
      LET    TMP_LOT_YIELD=100-TMP_LOT_YIELD/JDG_LOT_WAFER_VALID*100     (This should result 0 because always FAIL < VALID and both are INTEGER.)
      IF     TMP_LOT_YIELD<JDG_LOT_YLD_WAFER                                                                                                     
       LET   RTN_JDG_LOT_AT_WAFER="Fail"                                                                                                         
      ELSE                                                                                                                                       
       LET   RTN_JDG_LOT_AT_WAFER="Pass"                                                                                                         
      END IF                                                                                                                                     
$---- ------ ------------------------------------------------------- - - ------------------------------------------------------------------------

$Type: Action
$Name: HP_JDG_MODULE_AT_MODULE
$Vers: 1
$Desc: Judge current module.\n  Out: RTN_JDG_MODULE_AT_MODULE\t\t"Pass"=pass, "Fail"=fail\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------- ------------------------------- - - -
 BODY                                              
      LET     RTN_JDG_MODULE_AT_MODULE="Pass"      
                                                   
      JDG_MOD Ignore=JDG_MODULE_IGNORE_FLAG        
      IF      JDG_MOD.Jdg==0                       
       LET    RTN_JDG_MODULE_AT_MODULE="Fail"      
      END IF                                       
