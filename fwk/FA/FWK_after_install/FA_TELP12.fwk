      IF             UPDATE_FWKCONF.Errno<>0                                                                                                            
       LET           PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                                                  
       LET           PNL_MSG_MSG1="$ERR_UPDATE_CUSTOMIZE"                                                                                               
       LET           PNL_MSG_MSG2=UPDATE_FWKCONF.Errmsg                                                                                                 
       LET           PNL_KEY_OK="$HALT"                                                                                                                 
       PANEL         "HP_DISP_ERR2_OK",1                                                                                                                
       PANEL         "HP_DISP_ERR2_OK",0                                                                                                                
       LOG_SYSTEM    Session=SYSTEM.SESSIONID,Message="FA_TRANSFER_FWK: UPDATE_FWKCONF Error " + UPDATE_FWKCONF.Errmsg + "(" + FIL_CUSTOMIZE + ")"      
       IF            TST_TESTER_UPBYOTHER=0                                                                                                             
