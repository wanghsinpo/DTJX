$Type: Action
$Name: FA_CHECK_MAIN_COMMAND
$Vers: 1
$Desc: Desc: Check PPSELECT or Start message from the HOST\nDate: '00/Dec/18 : Initial revision\n      '00/Dec/20 : Start & PPSELECT command  are  supported.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$-------------------------- -------------------- ---------------------------------------------------------------------------------------------------------------------------------------------------------- - - ------------------------------------------------------------
 BODY                                                                                                                                                                                                                                                                       
 Get_command                                                                                                                                                                                                                                                                
                            ACTION               "FA_GEM_RECEIVE_COMMAND"                                                                                                                                                                                                   
                            IF                   FA_GEM_COMMAND==""                                                                                                                                                                                                         
                              RETURN                                                                                                                                                                                                                                        
                            END IF                                                                                                                                                                                                                                          
 Case_start                                                                                                                                                                                                                                                                 
                            IF                   FA_GEM_COMMAND=="START"                                                                                                                                        START command                                               
                              IF                 FA_START_NACK_FLG==1                                                                                                                                           ### FA3.0                                                   
                                ACTION           "FA_SEND_NACK"                                                                                                                                                 ### FA3.0                                                   
                                RETURN                                                                                                                                                                          ### FA3.0                                                   
                              END IF                                                                                                                                                                            ### FA3.0                                                   
                              ACTION             "FA_SEND_ACK"                                                                                                                                                                                                              
                              IF                 FA_START_PORTNUMBER=="1"                                                                                                                                                                                                   
                                LET              FA_RESERVED_START1="ON"                                                                                                                                                                                                    
                              ELSE                                                                                                                                                                                                                                          
                                LET              FA_RESERVED_START2="ON"                                                                                                                                                                                                    
                              END IF                                                                                                                                                                                                                                        
                              IF                 PRB_HPIB_ADRS==2500                                                                                                                                            for debugging                                               
                                IF               FA_START_PORTNUMBER=="1"                                                                                                                                       for debugging                                               
                                  EXECUTE        "touch /tmp/fapt/p1"                                                                                                                                           for debugging                                               
                                ELSE                                                                                                                                                                            for debugging                                               
                                  EXECUTE        "touch /tmp/fapt/p2"                                                                                                                                           for debugging                                               
                                END IF                                                                                                                                                                          for debugging                                               
                              END IF                                                                                                                                                                            for debugging                                               
                              RETURN                                                                                                                                                                                                                                        
                            END IF                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                            
 Case_ppselect                                                                                                                                                                                                                                                              
                            IF                   FA_GEM_COMMAND=="PP-SELECT"                                                                                                                                                                                                
                              ACTION             "FA_SEND_ACK"                                                                                                                                                                                                              
                              LET                FA_START_NACK_FLG=0                                                                                                                                            ### FA3.0                                                   
                              LET                FA_PPSELECT_FLG=1                                                                                                                                              ### FA3.0                                                   
                              LET                FA_DIELABEL_TEST_ON=0                                                                                                                                          ### FA3.0                                                   
                              LET                FA_DIETYPE_TEST_ON=0                                                                                                                                           ### FA3.0                                                   
                              LET                FA_MODLABEL_TEST_ON=0                                                                                                                                          ### FA3.0                                                   
                              LET                FA_DIE_OFFSET=0                                                                                                                                                ### FA3.0                                                   
                              LET                FA_WAF_OFFSET=0                                                                                                                                                ### FA3.0                                                   
                              IF                 FA_USE_RCP_KEY=="ON"                                                                                                                                           In case recipe from key code                                
#                               ACTION           "FA_SET_RECIPE_FROM_KEY"                                                                                                                                                                                                   
                                IF               FA_MULTIMEAS_MODE==2                                                                                                                                           ### FA3.3 Extract MEAS Libs from Recipe File                
                                  ACTION         "FA_SET_RECIPE_FROM_KEY3"                                                                                                                                      ### FA3.3                                                   
                                ELSE                                                                                                                                                                            ### FA3.3 Mease Libs Info comes from PP-Select cmd          
                                  ACTION         "FA_SET_RECIPE_FROM_KEY2"                                                                                                                                      ### FA3.0                                                   
                                END IF                                                                                                                                                                          ### FA3.3                                                   
                                IF               FA_USE_BUILD_FILES=="ON"                                                                                                                                       ### FA3.3                                                   
                                  IF             FA_TPL_BLDON<>"ON"                                                                                                                                             ### FA3.3                                                   
                                    PROBER_LOGWR Message="[Error: No spec names in Recipe File.]"                                                                                                               For debugging                                               
                                    LET          FA_STATUSNO="110:NO spec names in Recipe File"                                                                                                                 ### FA3.3                                                   
                                    ACTION       "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.3                                                   
                                    RETURN                                                                                                                                                                      ### FA3.3                                                   
                                  END IF                                                                                                                                                                        ### FA3.3                                                   
                                ELSE                                                                                                                                                                            ### FA3.3                                                   
                                  IF             FA_RSV_TPL_NAME[FA_RSV_PORTNUMBER]==""                                                                                                                         ### FA3.3                                                   
                                    PROBER_LOGWR Message="[Error: No TPL name in Recipe File.]"                                                                                                                 For debugging                                               
                                    LET          FA_STATUSNO="111:NO TPL name in Recipe File"                                                                                                                   ### FA3.3                                                   
                                    ACTION       "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.3                                                   
                                    RETURN                                                                                                                                                                      ### FA3.3                                                   
                                  END IF                                                                                                                                                                        ### FA3.3                                                   
                                END IF                                                                                                                                                                          ### FA3.3                                                   
                                IF               FA_STATUS=="ERROR"                                                                                                                                             ### FA3.0                                                   
                                  PROBER_LOGWR   Message="[Error: Recipe check error]"                                                                                                                          For debugging                                               
                                  ACTION         "FA_SEND_PPBADCOMPILE"                                                                                                                                                                                                     
                                  RETURN                                                                                                                                                                                                                                    
                                END IF                                                                                                                                                                                                                                      
                              ELSE                                                                                                                                                                                                                                          
                                IF               FA_USE_BUILD_FILES=="ON"                                                                                                                                       ### FA3.3                                                   
                                  IF             FA_TPL_BLDON<>"ON"                                                                                                                                             ### FA3.3                                                   
                                    PROBER_LOGWR Message="[Error: No spec names in Host message.]"                                                                                                              For debugging                                               
                                    LET          FA_STATUSNO="112:NO spec names in Host message"                                                                                                                ### FA3.3                                                   
                                    ACTION       "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.3                                                   
                                    RETURN                                                                                                                                                                      ### FA3.3                                                   
                                  END IF                                                                                                                                                                        ### FA3.3                                                   
                                ELSE                                                                                                                                                                            ### FA3.3                                                   
                                  IF             FA_RSV_TPL_NAME[FA_RSV_PORTNUMBER]==""                                                                                                                         ### FA3.3                                                   
                                    PROBER_LOGWR Message="[Error: No TPL name in Host message.]"                                                                                                                For debugging                                               
                                    LET          FA_STATUSNO="113:NO TPL name in Host message"                                                                                                                  ### FA3.3                                                   
                                    ACTION       "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.3                                                   
                                    RETURN                                                                                                                                                                      ### FA3.3                                                   
                                  END IF                                                                                                                                                                        ### FA3.3                                                   
                                END IF                                                                                                                                                                          ### FA3.3                                                   
                              END IF                                                                                                                                                                            ### FA3.3                                                   
                                                                                                                                                                                                                                                                            
                              IF                 FA_USE_RCP_KEY=="ON"                                                                                                                                           In case recipe from key code                                
                                IF               FA_RCPMETHOD==2                                                                                                                                                                                                            
                                  ACTION         "FA_SET_COND_FROM_KEY"                                                                                                                                                                                                     
                                END IF                                                                                                                                                                                                                                      
                              END IF                                                                                                                                                                                                                                        
                              IF                 FA_STATUS=="ERROR"                                                                                                                                                                                                         
                                PROBER_LOGWR     Message="[Error: Condition file check error]"                                                                                                                  For debugging                                               
                                ACTION           "FA_SEND_PPBADCOMPILE"                                                                                                                                                                                                     
                                RETURN                                                                                                                                                                                                                                      
                              END IF                                                                                                                                                                                                                                        
                              IF                 FA_MEASLOAD_MODE==0                                                                                                                                            ### FA3.3 Use SPECS 3.0 Facility                            
                                IF               FA_RSV_MEAS_NAME[FA_RSV_PORTNUMBER]<>""                                                                                                                        ### FA3.0                                                   
                                  ACTION         "FA_CHECK_MEAS"                                                                                                                                                ### FA3.0                                                   
                                  IF             FA_STATUS=="ERROR"                                                                                                                                             ### FA3.0                                                   
                                    PROBER_LOGWR Message="[Error: Measurement Lib File mode error]"                                                                                                             For debugging                                               
                                    ACTION       "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.0                                                   
                                    RETURN                                                                                                                                                                      ### FA3.0                                                   
                                  END IF                                                                                                                                                                        ### FA3.0                                                   
                                ELSE                                                                                                                                                                            ### FA3.0                                                   
                                  FA_CHK_REV     Session=SYSTEM.SESSIONID                                                                                                                                       ### FA3.0                                                   
                                  LET            FA_NOW_MEAS_DIR=FA_CHK_REV.Path                                                                                                                                ### FA3.0                                                   
                                END IF                                                                                                                                                                          ### FA3.0                                                   
                              ELSE                                                                                                                                                                              ### FA3.3 Use SPECS 3.1 Facility                            
                                ACTION           "FA_CHECK_MULTIMEAS"                                                                                                                                           ### FA3.3 Check Multi Meas Libs                             
                                IF               FA_STATUS=="ERROR"                                                                                                                                             ### FA3.3                                                   
                                  PROBER_LOGWR   Message="[Error: Measurement Lib File mode error]"                                                                                                             For debugging                                               
                                  ACTION         "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.3                                                   
                                  RETURN                                                                                                                                                                        ### FA3.3                                                   
                                END IF                                                                                                                                                                          ### FA3.3                                                   
                              END IF                                                                                                                                                                            ### FA3.3                                                   
                                                                                                                                                                                                                                                                            
                              IF                 FA_RSV_DEV_NAME[FA_RSV_PORTNUMBER]<>""                                                                                                                         ### FA3.0                                                   
                                ACTION           "FA_CHECK_DEV"                                                                                                                                                 ### FA3.0                                                   
                                IF               FA_STATUS=="ERROR"                                                                                                                                             ### FA3.0                                                   
                                  PROBER_LOGWR   Message="[Error: Device Lib File mode error]"                                                                                                                  For debugging                                               
                                  ACTION         "FA_SEND_PPBADCOMPILE"                                                                                                                                         ### FA3.0                                                   
                                  RETURN                                                                                                                                                                        ### FA3.0                                                   
                                END IF                                                                                                                                                                          ### FA3.0                                                   
                              ELSE                                                                                                                                                                              ### FA3.0                                                   
                                IF               FA_MEASLOAD_MODE==0                                                                                                                                            ### FA3.3 Use SPECS 3.0 Facility                            
                                  FA_CHK_REV     Session=SYSTEM.SESSIONID,Kinds="DEV"                                                                                                                           ### FA3.0                                                   
                                  LET            FA_NOW_DEV_NAME=FA_CHK_REV.Filename                                                                                                                            ### FA3.0                                                   
                                  IF             FA_NOW_DEV_NAME <> ""                                                                                                                                          ### FA3.0                                                   
                                    LET          FA_NOW_DEV_DIR=FA_CHK_REV.Path                                                                                                                                 ### FA3.0                                                   
                                  ELSE                                                                                                                                                                          ### FA3.0                                                   
                                    LET          FA_NOW_DEV_DIR=""                                                                                                                                              ### FA3.0                                                   
                                  END IF                                                                                                                                                                        ### FA3.0                                                   
                                ELSE                                                                                                                                                                            ### FA3.3                                                   
                                  FA_CHK_REV2    Session=SYSTEM.SESSIONID,Kinds="DEV"                                                                                                                           ### FA3.3                                                   
                                  LET            FA_NOW_DEV_NAME=FA_CHK_REV2.Filename                                                                                                                           ### FA3.3                                                   
                                  IF             FA_NOW_DEV_NAME <> ""                                                                                                                                          ### FA3.3                                                   
                                    LET          FA_NOW_DEV_DIR=FA_CHK_REV2.Path                                                                                                                                ### FA3.3                                                   
                                  ELSE                                                                                                                                                                          ### FA3.3                                                   
                                    LET          FA_NOW_DEV_DIR=""                                                                                                                                              ### FA3.3                                                   
                                  END IF                                                                                                                                                                        ### FA3.3                                                   
                                END IF                                                                                                                                                                          ### FA3.3                                                   
                              END IF                                                                                                                                                                            ### FA3.3                                                   
                                                                                                                                                                                                                                                                            
                              IF                 FA_USE_BUILD_FILES=="ON"                                                                                                                                       ### FA3.3                                                   
                                IF               FA_MEASLOAD_MODE==0                                                                                                                                            ### FA3.3 Use SPECS 3.0 Facility                            
                                  ACTION         "FA_GENERATE_FILES2"                                                                                                                                           ### FA3.0                                                   
                                ELSE                                                                                                                                                                            ### FA3.3                                                   
                                  ACTION         "FA_GENERATE_FILES3"                                                                                                                                           ### FA3.3                                                   
                                END IF                                                                                                                                                                          ### FA3.3                                                   
                              ELSE                                                                                                                                                                                                                                          
                                ACTION           "FA_CHECK_FILES"                                                                                                                                               test plan and limit file are used directly.                 
