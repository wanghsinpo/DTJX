$Type: Action
$Name: HP_PROBER_MOVRST
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$----------- ------- ----------------------------------------- - - -
 BODY                                                               
             IF      PRB_ONLINE==0                                  
              LET    PRB_MODX=WAFER.TMP_MODX                        
              LET    PRB_MODY=WAFER.TMP_MODY                        
              LET    WAFER.PRB_SUB_CURR_X=PRB_MODX                  
              LET    WAFER.PRB_SUB_CURR_Y=PRB_MODY                  
              RETURN                                                
             END IF                                                 
                                                                    
             LET     WAFER.PRB_SUB_MOVE_X=0                         
             LET     WAFER.PRB_SUB_MOVE_Y=0                         
 MoveHome                                                           
             ACTION  "HP_PROBER_PRB_HOME_"+PRB_MODEL                
                                                                    
             IF      RTN_PRB_HOME<>"Wafer_ready"                    
              LET    PRB_ABORT_ALL=-1                               
              REJECT LOT                                            
              GOTO   Fatal_error                                    
             END IF                                                 
 MoveBack                                                           
             LET     PRB_DIEX=WAFER.TMP_DIEX*WAFER.STEPX            
             LET     PRB_DIEY=WAFER.TMP_DIEY*WAFER.STEPY            
             LET     PRB_MODX=WAFER.TMP_MODX                        
             LET     PRB_MODY=WAFER.TMP_MODY                        
             LET     WAFER.PRB_SUB_CURR_X=PRB_MODX                  
             LET     WAFER.PRB_SUB_CURR_Y=PRB_MODY                  
                                                                    
             ACTION  "HP_PROBER_PRB_MOVE_OPT_" + PRB_MODEL          
                                                                    
             IF      RTN_PRB_MOVE<>"Module_ready"                   
              LET    PRB_ABORT_ALL=-1                               
              REJECT LOT                                            
              GOTO   Fatal_error                                    
             END IF                                                 
                                                                    
             RETURN                                                 
                                                                    
 Fatal_error                                                        
             LET     PNL_TTL_LABEL="$TTL_PROBER_ERROR"              
             LET     PNL_MSG_MSG1="$"+PRB_MODEL+"_FATAL_ERROR"      
             LET     PNL_KEY_OK="$OK"                               
             PANEL   "HP_DISP_ERR1_OK",2                            
             PANEL   "HP_DISP_ERR1_OK",0                            
