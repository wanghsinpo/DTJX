$Type: Action
$Name: HP_TEST_EXEC_PROBE
$Vers: 1
$Desc: Execute probing.\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---------- ------------- ------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                      
            IF            PRB_ONLINE==1                                                                                    
             PROBER_DOWN                                                                                                   
             IF           PROBER_DOWN.Status<>"OK"                                                                         
              LOG_PROBER  Session=SYSTEM.SESSIONID,Message="EXEC_PROBE_ZDOWN: PROBER_DOWN Error " + PROBER_DOWN.Error      
             END IF                                                                                                        
            END IF                                                                                                         
            LET           PNL_ZUP_DOWN="$Z_SEPARATE"                                                                       
            LET           PNL_COL_ZSTAT="LimeGreen"                                                                        
            PANEL         "HP_DISP_BLANK",2                                                                                
 Show_panel                                                                                                                
            LET           RTN_PRB_SEL_MODULE=1                                                                             
            LET           PRB_CURR_DIEX=PRB_SP_DIEX                                                                        
            LET           PRB_CURR_DIEY=PRB_SP_DIEY                                                                        
            LET           PRB_CURR_MODN=PRB_SP_MODN                                                                        
            PANEL         "HP_EXEC_PROBE",2                                                                                
                                                                                                                           
            REPEAT                                                                                                         
            UNTIL         RTN_PRB_SEL_MODULE=0                                                                             
                                                                                                                           
            IF            RTN_PRB_SEL_ACTION=="Go"                                                                         
             PANEL        "HP_EXEC_PROBE",0                                                                                
             GOTO         Show_panel                                                                                       
            END IF                                                                                                         
            ALGPANEL_STOP                                                                                                  
            PANEL         "HP_EXEC_PROBE",0                                                                                
            PANEL         "HP_DISP_BLANK",0                                                                                
            IF            PRB_ONLINE==1                                                                                    
             PROBER_UP                                                                                                     
             IF           PROBER_UP.Status<>"OK"                                                                           
              LOG_PROBER  Session=SYSTEM.SESSIONID,Message="EXEC_PROBE_ZUP: PROBER_UP Error " + PROBER_UP.Error            
             END IF                                                                                                        
            END IF                                                                                                         
            LET           PNL_ZUP_DOWN="$Z_CONTACT"                                                                        
            LET           PNL_COL_ZSTAT="Red"                                                                              
