$Type: Action
$Name: HP_SET_JDG_WAFER
$Vers: 1
$Desc: Return wafer level pass/fail string.\n  In : TMP_SLOT_POS (slot position)\n  Out: TMP_SLOT_JDG (judgement string)\n\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------- ------------------------------------------- - - -
 BODY                                                          
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]=="$PASS"       
       LET    TMP_SLOT_JDG="Pass"                              
       RETURN                                                  
      END IF                                                   
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]=="$FAIL"       
       LET    TMP_SLOT_JDG="Fail"                              
       RETURN                                                  
      END IF                                                   
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]=="$YET"        
       LET    TMP_SLOT_JDG=""                                  
       RETURN                                                  
      END IF                                                   
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]=="$SKIP"       
       LET    TMP_SLOT_JDG=""                                  
       RETURN                                                  
      END IF                                                   
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]=="$ALIGN"      
       LET    TMP_SLOT_JDG="Alignment"                         
       RETURN                                                  
      END IF                                                   
$---- ------- ------------------------------------------- - - -

