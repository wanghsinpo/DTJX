               LET            FA_LOTEND="OFF"                                                        
               GOTO           Lotend_proc                                                            
             END IF                                                                                  
                                                                                                     
             IF               FA_NOMOREU<>0                                                          
               LET            FA_NOMOREU=0                                                           
               GOTO           Wait_srq72                                                             
             END IF                                                                                  
             IF               PRB_RANDOM==0                           abort at processing            
               WAIT           Wait=5                                  abort at processing            
               PROBER_SENDCMD Command="B"                             PROBER_HOME abort at processing
             END IF                                                   abort at processing            
 Wait_srq75                                                           abort at processing            
             REPEAT                                                   abort at processing            
               PROBER_SRQ     Timeout=5                               abort at processing            
               IF             TMP_I>60                                abort at processing            
                 LET          RTN_PRB_QUERY_READY="Prober_abort"      abort at processing            
                 RETURN                                               abort at processing            
               END IF                                                 abort at processing            
               LET            TMP_I=TMP_I+1                           abort at processing            
             UNTIL            PROBER_SRQ.Statusbyte==75               abort at processing            
                                                                      abort at processing            
             IF               PRB_RANDOM==0                           0:Pipeline, 1:Random           
               WAIT           Wait=1                                                                 
             PROBER_SENDCMD   Command="U"                                                            
             ELSE                                                                                    
               WAIT                                                                                  
               WRITE_INTEGER  Value=PRB_LOAD_CASS                                                    
               LET            FA_RND_PNUM=WRITE_INTEGER.String        "1" or "2"                     
               WRITE_INTEGER  Format="%02d",Value=PRB_LOAD_SLOT                                      
               LET            FA_RND_SNUM=WRITE_INTEGER.String        "01"..."25"                    
               PROBER_SENDCMD Command="l"+FA_RND_PNUM+FA_RND_SNUM     "l101"..."l225"                
             END IF                                                                                  
                                                                                                     
 Wait_srq72                                                                                          
             REPEAT                                                                                  
               PROBER_SRQ     Timeout=5                               Check SRQ                      
               IF             TMP_I>60                                Wait for 3 minutes             
                 LET          RTN_PRB_QUERY_READY="Prober_abort"                                     
                 RETURN                                                                              
               END IF                                                                                
               LET            TMP_I=TMP_I+1                           Increment waiting time         
             UNTIL            PROBER_SRQ.Statusbyte==72                                              
 Lotend_proc                                                                                         
                                                                                                     
             IF               PRB_RANDOM==0                           0:Pipeline, 1:Random           
               ACTION         "FA_PRB_LOTEND_TELP12"                  Lot end command to prober      
