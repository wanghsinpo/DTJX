                 WRITE_INTEGER "%d",TST_SLOT_CURRENT                                                                                         
                 LOG_PROBER    Session=SYSTEM.SESSIONID,Message="ALIGNMENT ERROR: Slot " + WRITE_INTEGER.String                              
 Load_next_wafer                                                                                                                             
                 IF            FA_CONTROL_MODE<>"REMOTE"                                                            ### FA INSPECTION        
                  ACTION       "HP_SET_MODE_DISP"                                                                   ### FA INSPECTION        
                  IF           (TST_FLW_DETAILED==1) AND (TST_FLW_RNDLD==1)                                         ### FA INSPECTION        
                   LET         TST_SLOT_CURRENT=0                                                                   ### FA INSPECTION        
                  END IF                                                                                            ### FA INSPECTION        
                 END IF                                                                                             ### FA INSPECTION        
                 REPEAT                                                                                                                      
                  LET          TST_SLOT_CURRENT=TST_SLOT_CURRENT+1                                                                           
                  IF           TST_SLOT_CURRENT>PRB_SLOT_TOTAL_ALL                                                                           
                   ACTION      "HP_PROBER_PRB_LOTEND_RND_TELP12"                                                                             
                   LET         RTN_LOAD_ONE=RTN_PRB_LOTEND                                                                                   
                   RETURN                                                                                                                    
                  END IF                                                                                                                     
                 UNTIL         TST_SLOT_SELECT[TST_SLOT_CURRENT]=1                                                                           
                                                                                                                                             
                 IF            TST_SLOT_CURRENT<=PRB_SLOT_TOTAL_CASS                                                                         
                  IF           FA_PORTNUMBER=="1"                                                                   ### AUTOMATION           
                    LET        PRB_LOAD_CASS=1                                                                                               
