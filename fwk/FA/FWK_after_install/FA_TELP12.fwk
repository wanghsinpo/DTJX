$Type: Action
$Name: HP_PROBER_PRB_LOAD_PPL_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$--------- --------------- ------------------------------------------------------------------------------------------------------------------- - - ---------------------------------------------------
 BODY                                                                                                                                                                                                 
           IF              PRB_ONLINE==0                                                                                                                                                              
            LET            RTN_PRB_LOAD_PPL="Wafer_ready"                                                                                                                                             
            RETURN                                                                                                                                                                                    
           END IF                                                                                                                                                                                     
                                                                                                                                                                                                      
           IF              (FA_EQUIP_STATE=="ABORTING") OR (FA_EQUIP_STATE=="STOPPING") OR (FA_EQUIP_STATE=="CJABORTING")                          ### FA3.0                                          
             LET           RTN_PRB_LOAD_PPL="Lot_end"                                                                                              ### AUTOMATION                                     
             RETURN                                                                                                                                ### AUTOMATION                                     
           END IF                                                                                                                                  ### AUTOMATION                                     
                                                                                                                                                                                                      
           ACTION          "FA_CHECK_MESSAGE"                                                                                                      ### AUTOMATION Check prober_alarm                  
           IF              (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP") OR (FA_GEM_COMMAND=="PJABORT") OR (FA_GEM_COMMAND=="CJABORT")     ### FA3.0                                          
             PROBER_LOGWR  Message="ABORT 044"                                                                                                     For debugging                                      
             LET           RTN_PRB_LOAD_PPL="Prober_abort"                                                                                         ### AUTOMATION                                     
             RETURN                                                                                                                                ### AUTOMATION                                     
           END IF                                                                                                                                  ### AUTOMATION                                     
                                                                                                                                                                                                      
           ACTION          "FA_CHECK_STATUSBYTE"                                                                                                   ### AUTOMATION Check statusbyte                    
           IF              RTN_PRB_QUERY_READY=="Prober_abort"                                                                                     ### AUTOMATION Something error happen              
             LET           PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                       ### AUTOMATION                                     
             LET           PNL_MSG_MSG1="$TELP12_FATAL_ERROR"                                                                                      ### AUTOMATION                                     
             LET           PNL_MSG_MSG2="HP_PROBER_PRB_LOAD_PPL_TELP12"                                                                            ### AUTOMATION                                     
             PROBER_LOGWR  Message="RESUME_ABORT 017"                                                                                              For debugging                                      
             LET           FA_ALID="7000"                                                                                                          ### AUTOMATION                                     
             LET           FA_ALTX="CustomAlarm"                                                                                                   ### AUTOMATION                                     
             LET           FA_ALMSG="Invalid Status Byte."                                                                                         ### AUTOMATION                                     
             ACTION        "FA_WAIT_ABORT"                                                                                                         ### AUTOMATION Wait for abort/resume               
             IF            (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP") OR (FA_GEM_COMMAND=="PJABORT") OR (FA_GEM_COMMAND=="CJABORT")     ### FA3.0                                          
               LET         RTN_PRB_LOAD_PPL="Prober_abort"                                                                                         ### AUTOMATION                                     
               RETURN                                                                                                                              ### AUTOMATION                                     
             END IF                                                                                                                                ### AUTOMATION                                     
           END IF                                                                                                                                  ### AUTOMATION                                     
           IF              TST_SLOT_REJECT==0                                                                                                                                                         
            PROBER_LOAD_I                                                                                                                          ### AUTOMATION                                     
            LET            TMP_PRB_STATUS=PROBER_LOAD_I.Status                                                                                     ### AUTOMATION                                     
            LET            TMP_PRB_ERROR=PROBER_LOAD_I.Error                                                                                       ### AUTOMATION                                     
            IF             TMP_PRB_STATUS=="END"                                                                                                   ### AUTOMATION                                     
              LET          FA_LOTEND="ON"                                                                                                          ### AUTOMATION                                     
            END IF                                                                                                                                 ### AUTOMATION                                     
           ELSE                                                                                                                                                                                       
            PROBER_REJECT                                                                                                                                                                             
            LET            TMP_PRB_STATUS=PROBER_REJECT.Status                                                                                                                                        
            LET            TMP_PRB_ERROR=PROBER_REJECT.Error                                                                                                                                          
           END IF                                                                                                                                                                                     
                                                                                                                                                                                                      
           LET             FA_TIMER=1                                                                                                              ### AUTOMATION                                     
 Check_msg                                                                                                                                         ### AUTOMATION                                     
#          ACTION          "FA_CHECK_MESSAGE"                                                                                                      ### AUTOMATION Check prober_alarm                  
#          IF              FA_GEM_COMMAND=="RESUME"                                                                                                ### AUTOMATION   Resume or Abort after prober alarm
#            PROBER_LOGWR  Message="RESUME 018"                                                                                                    For debugging                                      
#          END IF                                                                                                                                  ### AUTOMATION                                     
#          IF              (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP") OR (FA_GEM_COMMAND=="PJABORT") OR (FA_GEM_COMMAND=="CJABORT")     ### FA3.0                                          
#            PROBER_LOGWR  Message="ABORT 018"                                                                                                     For debugging                                      
#            LET           RTN_PRB_LOAD_PPL="Prober_abort"                                                                                         ### AUTOMATION                                     
#            LET           FA_NOMOREU=1                                                                                                            ### AUTOMATION                                     
#            RETURN                                                                                                                                ### AUTOMATION                                     
#          END IF                                                                                                                                  ### AUTOMATION                                     
           IF              TMP_PRB_ERROR=="#TIMEOUT_SRQ"                                                                                           ### AUTOMATION Only check srq after load command   
             PROBER_LOAD_S                                                                                                                         ### AUTOMATION                                     
#            IF            FA_TIMER>60                                                                                                             ### AUTOMATION                                     
#              LET         RTN_PRB_LOAD_PPL="Prober_abort"                                                                                         ### AUTOMATION                                     
#              RETURN                                                                                                                              ### AUTOMATION                                     
#            END IF                                                                                                                                ### AUTOMATION                                     
             LET           FA_TIMER=FA_TIMER+1                                                                                                     ### AUTOMATION                                     
             LET           TMP_PRB_STATUS=PROBER_LOAD_S.Status                                                                                     ### AUTOMATION                                     
             LET           TMP_PRB_ERROR=PROBER_LOAD_S.Error                                                                                       ### AUTOMATION                                     
             GOTO          Check_msg                                                                                                               ### AUTOMATION                                     
           END IF                                                                                                                                  ### AUTOMATION                                     
 Check_stb                                                                                                                                         ### AUTOMATION                                     
           LET             RTN_PRB_LOAD_PPL="Prober_abort"                                                                                                                                            
                                                                                                                                                                                                      
           IF              TMP_PRB_STATUS=="ERROR"                                                                                                                                                    
            IF             TMP_PRB_ERROR=="#TIMEOUT_SRQ"                                                                                                                                              
             ACTION        "HP_PROBER_PRB_QUERY_READY_TELP12"                                                                                                                                         
             LET           RTN_PRB_LOAD_PPL=RTN_PRB_QUERY_READY                                                                                                                                       
             IF            RTN_PRB_LOAD_PPL=="Prober_abort"                                                                                                                                           
              LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PRB_QUERY_READY Error " + PROBER_READY.Error                                                                               
             END IF                                                                                                                                                                                   
             RETURN                                                                                                                                                                                   
            END IF                                                                                                                                                                                    
            IF             TMP_PRB_ERROR=="#ALIGN_REJECT"                                                                                                                                             
             LET           FA_LOTCANCEL_BYPRB=1                                                                                                    ### FA3.2                                          
             LET           RTN_PRB_LOAD_PPL="Align_reject"                                                                                                                                            
            END IF                                                                                                                                                                                    
            IF             TMP_PRB_ERROR=="#ALIGN_ASSIST"                                                                                                                                             
             LET           RTN_PRB_LOAD_PPL="Align_assist"                                                                                                                                            
            END IF                                                                                                                                                                                    
           END IF                                                                                                                                                                                     
                                                                                                                                                                                                      
           IF              TMP_PRB_STATUS=="END"                                                                                                                                                      
            LET            RTN_PRB_LOAD_PPL="Lot_end"                                                                                                                                                 
            IF             FA_LOTCANCEL_BYPRB==1                                                                                                   ### FA3.2                                          
              ACTION       "FA_SEND_PJABORTED"                                                                                                     ### FA3.2                                          
            END IF                                                                                                                                                                                    
           END IF                                                                                                                                                                                     
                                                                                                                                                                                                      
           IF              TMP_PRB_STATUS=="OK"                                                                                                                                                       
            LET            RTN_PRB_LOAD_PPL="Wafer_ready"                                                                                                                                             
           END IF                                                                                                                                                                                     
                                                                                                                                                                                                      
           IF              RTN_PRB_LOAD_PPL=="Prober_abort"                                                                                                                                           
            IF             TST_SLOT_REJECT==0                                                                                                                                                         
             LOG_PROBER    Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PROBER_LOAD Error " + TMP_PRB_ERROR                                                                                        
            ELSE                                                                                                                                                                                      
             LOG_PROBER    Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PROBER_REJECT Error " + TMP_PRB_ERROR                                                                                      
            END IF                                                                                                                                                                                    
           END IF                                                                                                                                                                                     
