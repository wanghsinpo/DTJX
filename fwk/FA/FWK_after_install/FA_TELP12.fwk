       LET    TMP_RPT_LOTYLD_ITEM[1]="$OFF"      
      END IF                                     
      IF      RPT_LOTYLD_DIE_USE==1              
       LET    TMP_RPT_LOTYLD_DIE[1]="$ON"        
      ELSE                                       
       LET    TMP_RPT_LOTYLD_DIE[1]="$OFF"       
      END IF                                     
      IF      RPT_WAFSTAT_USE==1                 
       LET    TMP_RPT_WAFSTAT[1]="$ON"           
      ELSE                                       
       LET    TMP_RPT_WAFSTAT[1]="$OFF"          
      END IF                                     
      IF      RPT_WAFPF_USE==1                   
       LET    TMP_RPT_WAFPF[1]="$ON"             
      ELSE                                       
       LET    TMP_RPT_WAFPF[1]="$OFF"            
      END IF                                     
                                                 
      PANEL   "HP_SET_RPTENV",2                  
                                                 
      IF      PANEL.RETURN=="CANCEL"             
       PANEL  "HP_SET_RPTENV",0                  
       RETURN                                    
      END IF                                     
                                                 
      IF      TMP_RPT_LOTSTAT[1]=="$ON"          
       LET    RPT_LOTSTAT_USE=1                  
      ELSE                                       
       LET    RPT_LOTSTAT_USE=0                  
      END IF                                     
      IF      TMP_RPT_LOTYLD_ITEM[1]=="$ON"      
       LET    RPT_LOTYLD_ITEM_USE=1              
      ELSE                                       
       LET    RPT_LOTYLD_ITEM_USE=0              
      END IF                                     
      IF      TMP_RPT_LOTYLD_DIE[1]=="$ON"       
       LET    RPT_LOTYLD_DIE_USE=1               
      ELSE                                       
       LET    RPT_LOTYLD_DIE_USE=0               
      END IF                                     
      IF      TMP_RPT_WAFSTAT[1]=="$ON"          
       LET    RPT_WAFSTAT_USE=1                  
      ELSE                                       
       LET    RPT_WAFSTAT_USE=0                  
      END IF                                     
      IF      TMP_RPT_WAFPF[1]=="$ON"            
       LET    RPT_WAFPF_USE=1                    
      ELSE                                       
       LET    RPT_WAFPF_USE=0                    
      END IF                                     
                                                 
      ACTION  "HP_SET_TAG_REPORT"                
                                                 
      PANEL   "HP_SET_RPTENV",0                  
$---- ------- ----------------------------- - - -

