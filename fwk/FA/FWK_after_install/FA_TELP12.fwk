$Type: Action
$Name: HP_TEST_UNSELECT_WAFER_MULTI
$Vers: 1
$Desc: Unselect wafers.\n  SPECS2.5: Moved from HP_TEST_SEL_WAFER_G8_MULTI.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- --------- ------------------------------ - - -
 BODY                                               
       LET      TMP_SLOT_POS=1                      
       REPEAT                                       
        IF      TMP_SLOTA_SEL[TMP_SLOT_POS]==2      
         LET    TMP_SLOTA_SEL[TMP_SLOT_POS]=-2      
        END IF                                      
        IF      TMP_SLOTB_SEL[TMP_SLOT_POS]==2      
         LET    TMP_SLOTB_SEL[TMP_SLOT_POS]=-2      
        END IF                                      
        LET     TMP_SLOT_POS=TMP_SLOT_POS+1         
       UNTIL    TMP_SLOT_POS>10                     
       REPEAT                                       
        IF      TMP_SLOTA_SEL[TMP_SLOT_POS]==2      
         LET    TMP_SLOTA_SEL[TMP_SLOT_POS]=-2      
        END IF                                      
        LET     TMP_SLOT_POS=TMP_SLOT_POS+1         
       UNTIL    TMP_SLOT_POS>15                     
                                                    
       IF       PRB_CASS_NUM=="$DOUBLE"             
        LET     TMP_SLOT_POS=1                      
        REPEAT                                      
         IF     TMP_SLOTC_SEL[TMP_SLOT_POS]==2      
          LET   TMP_SLOTC_SEL[TMP_SLOT_POS]=-2      
         END IF                                     
         IF     TMP_SLOTD_SEL[TMP_SLOT_POS]==2      
          LET   TMP_SLOTD_SEL[TMP_SLOT_POS]=-2      
         END IF                                     
         LET    TMP_SLOT_POS=TMP_SLOT_POS+1         
        UNTIL   TMP_SLOT_POS>10                     
        REPEAT                                      
         IF     TMP_SLOTC_SEL[TMP_SLOT_POS]==2      
          LET   TMP_SLOTC_SEL[TMP_SLOT_POS]=-2      
         END IF                                     
         LET    TMP_SLOT_POS=TMP_SLOT_POS+1         
        UNTIL   TMP_SLOT_POS>15                     
       END IF                                       
