$Type: Action
$Name: HP_EXEC_AUTOPROBE_GO
$Vers: 1
$Desc: Probe selected failed module.\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$-------------- ----------- ----------------------------------------- - - ------------------------------------
 BODY                                                                                                         
 Calc_typeindex                                                                                               
                LET         PRB_AT_TYPI=1                                                                     
                WHILE       PRB_AT_TYPI<=DIE.TYPETOTAL                                                        
                 IF         DIE.DEFINELIST[PRB_AT_TYPI]==PRB_AT_TYPN                                          
                  GOTO      Probe_selected                                                                    
                 END IF                                                                                       
                 LET        PRB_AT_TYPI=PRB_AT_TYPI+1                                                         
                END WHILE                                                                                     
                RETURN                                                                                        
 Probe_selected                                                                                               
                GET_LOC_MOD Typeindex=PRB_AT_TYPI,Modname=PRB_AT_MODN                                         
                IF          GET_LOC_MOD.Stat==-1                                                              
                 RETURN                                                                                       
                END IF                                                                                        
                                                                                                              
                LET         PNL_TTL_LABEL="$TTL_PROBING"                                                      
                LET         PNL_MSG_MSG1="$PROBING"                                                           
                PANEL       "HP_DISP_MSG1_NONE",2                                                             
                                                                                                              
                LET         PRB_DIEX=PRB_AT_DIEX*WAFER.STEPX                                                  
                LET         PRB_DIEY=PRB_AT_DIEY*WAFER.STEPY                                                  
                LET         PRB_MODX=GET_LOC_MOD.X                                                            
                LET         PRB_MODY=GET_LOC_MOD.Y                                                            
                                                                                                              
                ACTION      "HP_PROBER_PRB_MOVE_OPT_" + PRB_MODEL                                             
                                                                                                              
                PANEL       "HP_DISP_MSG1_NONE",0                                                             
                                                                                                              
                ACTION      "HP_PROBER_ZUP"                               Especially for EG, Z-up is necessary
                LET         PNL_ZUP_DOWN="$Z_CONTACT"                                                         
                LET         PNL_COL_ZSTAT="Red"                                                               
                LET         PRB_CURR_DIEX=PRB_AT_DIEX                                                         
                LET         PRB_CURR_DIEY=PRB_AT_DIEY                                                         
                LET         PRB_CURR_MODN=PRB_AT_MODN                                                         
                PANEL       "HP_EXEC_AUTOPROBE"                                                               
