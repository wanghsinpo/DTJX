                  WRITE_INTEGER "%d",TST_SLOT_CURRENT                                                                                      
                  LOG_PROBER    Session=SYSTEM.SESSIONID,Message="ALIGNMENT ERROR: Slot " + WRITE_INTEGER.String                           
 Load_next_wafer                                                                                                                           
                  ACTION        "HP_PROBER_PRB_LOAD_PPL_TELP12"                                                                            
                                                                                                                                           
                  IF            TST_SLOT_REJECT==1                                                                    Reject only 1st wafer
                   LET          TST_SLOT_REJECT=0                                                                                          
                  END IF                                                                                                                   
                                                                                                                                           
                  IF            RTN_PRB_LOAD_PPL=="Align_reject"                                                                           
                   GOTO         Reject_wafer                                                                                               
                  END IF                                                                                                                   
                  IF            RTN_PRB_LOAD_PPL=="Wafer_ready"                                                                            
                   GOTO         Ready_next_wafer                                                                                           
                  END IF                                                                                                                   
                  IF            RTN_PRB_LOAD_PPL=="Lot_end"                                                                                
                   LET          RTN_LOAD_ONE="Lot_end"                                                                                     
                   RETURN                                                                                                                  
                  END IF                                                                                                                   
                  IF            RTN_PRB_LOAD_PPL<>"Align_assist"                                                      "Prober_abort"       
