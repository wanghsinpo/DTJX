$Type: Action
$Name: DIE_BEGIN
$Vers: 1
$Desc: ACTION called before each die-test.\n  SPECS2.5: Update judgement flags.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------- ----------------- ---------------------------------------------------------------------------------------------------------------------- - - --------------------------------------------------
 BODY                                                                                                                                                                                                         
               LET               TST_WAFER_ENABLE_PAUSE=1                                                                                                                                                     
 Exec_hook                                                                                                                                                                                                    
               LET               DIE.PRB_OFFSETX=PRB_OFFSETX                                                                                                Can overwite these                                
               LET               DIE.PRB_OFFSETY=PRB_OFFSETY                                                                                                in plug action.                                   
               ACTION            "PLG_DIE_BEGIN"                                                                                                                                                              
 Init_tag                                                                                                                                                                                                     
               LET               TST_DIE_DONE=TST_DIE_DONE+1                                                                                                                                                  
               LET               TST_MODULE_DONE=-1                                                                                                                                                           
               LET               DIE.SYS_DIEEND="Terminate"                                                                                                 Replace if error occured                          
               LET               DIE.JDG_RUNTIME=JDG_DIE_RUNTIME                                                                                                                                              
               LET               TST_MODULE_NUM=0                                                                                                                                                             
               LET               TST_MODULE_TOTAL=MODULE.TOTAL                                                                                                                                                
               LET               TST_DIE_LAST=DIE.DEFINELIST[DIE.TYPENUMBER]                                                                                                                                  
               LET               TST_DIE_LASTX=DIE.INDEXX+DIE.PRB_OFFSETX                                                                                                                                     
               LET               TST_DIE_LASTY=DIE.INDEXY+DIE.PRB_OFFSETY                                                                                                                                     
               LET               TST_REJECT_USER=0                                                                                                                                                            
 Update_result                                                                                                                                                                                                
               IF                TST_DIE_NAME<>DIE.NAME                                                                                                                                                       
                LET              TST_DIE_NUM=TST_DIE_NUM+1                                                                                                                                                    
                WHILE            TST_DIE_TOTAL>=TST_DIE_NUM                                                                                                 Skip untested dice in                             
                 SET_JDG_DIE     Typeindex=TST_TYPE_NUM,Dieindex=TST_DIE_NUM,Judge=PNL_TEST_WAFER_SKP                                                       previous die type.                                
                 LET             TST_DIE_NUM=TST_DIE_NUM+1                                                                                                                                                    
                END WHILE                                                                                                                                                                                     
                                                                                                                                                                                                              
                LET              TST_TYPE_NUM=TST_TYPE_NUM+1                                                                                                                                                  
                WHILE            DIE.TYPENUMBER>TST_TYPE_NUM                                                                                                Skip untested dice in                             
                 SET_JDG_DIE_TYP Typeindex=TST_TYPE_NUM,Judge=PNL_TEST_WAFER_SKP                                                                            previous die type.                                
                 LET             TST_TYPE_NUM=TST_TYPE_NUM+1                                                                                                                                                  
                END WHILE                                                                                                                                                                                     
                                                                                                                                                                                                              
                LET              TST_DIE_NAME=DIE.NAME                                                                                                                                                        
                LET              TST_DIE_TOTAL=DIE.TOTAL                                                                                                                                                      
                LET              TST_DIE_NUM=0                                                                                                                                                                
                LET              TST_TYPE_NUM=DIE.TYPENUMBER                                                                                                                                                  
               END IF                                                                                                                                                                                         
                                                                                                                                                                                                              
               IF                DIE.NUMBER>TST_DIE_NUM                                                                                                     DIE.NUMBER=TST_DIE_NUM, if RETEST DIE was applied.
                LET              TST_DIE_NUM=TST_DIE_NUM+1                                                                                                                                                    
                WHILE            DIE.NUMBER>TST_DIE_NUM                                                                                                     Rejected dice                                     
                 SET_JDG_DIE     Typeindex=DIE.TYPENUMBER,Dieindex=TST_DIE_NUM,Judge=PNL_TEST_WAFER_SKP,Offsetx=DIE.PRB_OFFSETX,Offsety=DIE.PRB_OFFSETY                                                       
                 LET             TST_DIE_NUM=TST_DIE_NUM+1                                                                                                                                                    
                END WHILE                                                                                                                                                                                     
               END IF                                                                                                                                                                                         
 Set_current                                                                                                                                                                                                  
               SET_JDG_DIE       Typeindex=DIE.TYPENUMBER,Dieindex=DIE.NUMBER,Judge=PNL_TEST_WAFER_NOW,Offsetx=DIE.PRB_OFFSETX,Offsety=DIE.PRB_OFFSETY                                                        
 Check_time                                                                                                                                                                                                   
               CHECK_TIME                                                                                                                                   Update timer                                      
               LET               TIM_DIE_BEGIN=CHECK_TIME.Time                                                                                                                                                
               IF                TST_DIE_DONE>0                                                                                                                                                               
                LET              TIM_DIE=(TIM_DIE_BEGIN-TIM_WAFER_BEGIN)/TST_DIE_DONE                                                                                                                         
               ELSE                                                                                                                                                                                           
                LET              TIM_DIE=0                                                                                                                                                                    
               END IF                                                                                                                                                                                         
               STRING_TIME       Time=TIM_DIE_BEGIN-TIM_ALL_BEGIN                                                                                                                                             
               LET               PNL_TEST_TIME_SPEND=STRING_TIME.Symbol                                                                                                                                       
               IF                TST_WAFER_DONE>0                                                                                                                                                             
                LET              TIM_REMAIN=TIM_WAFER*TST_WAFER_NUM-TIM_DIE*TST_DIE_DONE                                                                                                                      
                IF               TIM_REMAIN>0                                                                                                                                                                 
                 STRING_TIME     Time=TIM_REMAIN                                                                                                                                                              
                 LET             PNL_TEST_TIME_REMAIN=STRING_TIME.Symbol                                                                                                                                      
                END IF                                                                                                                                                                                        
               ELSE                                                                                                                                                                                           
                IF               TST_DIE_DONE>0                                                                                                                                                               
                 LET             TIM_REMAIN=TIM_DIE*DIE.TOTAL*DIE.TYPETOTAL*TST_WAFER_NUM-TIM_DIE*TST_DIE_DONE                                                                                                
                 IF              TIM_REMAIN>0                                                                                                                                                                 
                  STRING_TIME    Time=TIM_REMAIN                                                                                                                                                              
                  LET            PNL_TEST_TIME_REMAIN=STRING_TIME.Symbol                                                                                                                                      
                 END IF                                                                                                                                                                                       
                END IF                                                                                                                                                                                        
               END IF                                                                                                                                                                                         
                                                                                                                                                                                                              
               UPDATE            "HP_DISP_TEST_RUN",UPD_TEST_DIE_BEGIN                                                                                                                                        
$------------- ----------------- ---------------------------------------------------------------------------------------------------------------------- - - --------------------------------------------------

$Type: Action
$Name: DIE_END
$Vers: 1
$Desc: ACTION called after each die-test.\n  SPECS2.5: Added auto-probing function on failure.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------ ------------- --------------------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                                  
 Exec_hook                                                                                                                                             
              ACTION        "PLG_DIE_END"                                                                                                              
              IF            SYSTEM.RETEST="DIE"                                                                                                        
               IF           ENV_CLEARONRETEST="TRUE"                                                                                                   
                SET_JDG_DIE Typeindex=DIE.TYPENUMBER,Dieindex=DIE.NUMBER,Judge=PNL_TEST_WAFER_YET,Offsetx=DIE.PRB_OFFSETX,Offsety=DIE.PRB_OFFSETY      
               END IF                                                                                                                                  
               RETURN                                                                                                                                  
              END IF                                                                                                                                   
 Set_tag                                                                                                                                               
              LET           WAFER.SYS_DIECOUNT=WAFER.SYS_DIECOUNT+1                                                                                    
                                                                                                                                                       
              IF            SYSTEM.REJECT<>""                                                                                                          
               LET          DIE.SYS_DIEEND="Reject"                                                                                                    
              ELSE                                                                                                                                     
               LET          DIE.SYS_DIEEND="Complete"                                                                                                  
              END IF                                                                                                                                   
 Judge_die                                                                                                                                             
              ACTION        "HP_JDG_DIE_AT_DIE"                                                                                                        
 Update_panel                                                                                                                                          
              IF            DIE.SYS_DIEPASS=1                                                                                                          
               LET          TMP_WAFER_JUDGE=PNL_TEST_WAFER_PSS                                                                                         
              ELSE                                                                                                                                     
               LET          TMP_WAFER_JUDGE=PNL_TEST_WAFER_FIL                                                                                         
              END IF                                                                                                                                   
              SET_JDG_DIE   Typeindex=DIE.TYPENUMBER,Dieindex=TST_DIE_NUM,Judge=TMP_WAFER_JUDGE,Offsetx=DIE.PRB_OFFSETX,Offsety=DIE.PRB_OFFSETY        
              UPDATE        "HP_DISP_TEST_RUN",UPD_TEST_DIE_END                                                                                        
 Judge_wafer                                                                                                                                           
              IF            WAFER.JDG_RUNTIME=1                                                                                                        
               ACTION       "HP_JDG_WAFER_AT_DIE"                                                                                                      
               IF           RTN_JDG_WAFER_AT_DIE="Fail"                                                                                                
                GOTO        Wafer_fail                                                                                                                 
               END IF                                                                                                                                  
              END IF                                                                                                                                   
                                                                                                                                                       
              IF            PRB_ABORT_ALL<>0                                                                                                           
               RETURN                                                                                                                                  
              END IF                                                                                                                                   
              IF            PRB_ABORT_WAFER<>0                                                                                                         
               RETURN                                                                                                                                  
              END IF                                                                                                                                   
                                                                                                                                                       
              IF            DIE.JDG_RUNTIME=0                                                                                                          
               IF           DIE.SYS_DIEPASS=0                                                                                                          
