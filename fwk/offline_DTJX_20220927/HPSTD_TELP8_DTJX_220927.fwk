$Type: Action
$Name: HP_PROBER_PRB_LOAD_PPL_TELP8
$Vers: 1
$Desc: Description for this Action spec.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- -------------- -------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                  
      IF             PRB_ONLINE=0                                                                                      
       LET           RTN_PRB_LOAD_PPL="Wafer_ready"                                                                    
       RETURN                                                                                                          
      END IF                                                                                                           
                                                                                                                       
      IF             TST_SLOT_REJECT=0                                                                                 
       PROBER_LOAD                                                                                                     
       LET           TMP_PRB_STATUS=PROBER_LOAD.Status                                                                 
       LET           TMP_PRB_ERROR=PROBER_LOAD.Error                                                                   
      ELSE                                                                                                             
       PROBER_REJECT                                                                                                   
       LET           TMP_PRB_STATUS=PROBER_REJECT.Status                                                               
       LET           TMP_PRB_ERROR=PROBER_REJECT.Error                                                                 
      END IF                                                                                                           
                                                                                                                       
      LET            RTN_PRB_LOAD_PPL="Prober_abort"                                                                   
                                                                                                                       
      IF             TMP_PRB_STATUS="ERROR"                                                                            
       IF            TMP_PRB_ERROR="#TIMEOUT_SRQ"                                                                      
        ACTION       "HP_PROBER_PRB_QUERY_READY_TELP8"                                                                 
        LET          RTN_PRB_LOAD_PPL=RTN_PRB_QUERY_READY                                                              
        IF           RTN_PRB_LOAD_PPL="Prober_abort"                                                                   
         LOG_PROBER  Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PRB_QUERY_READY Error " + PROBER_READY.Error      
        END IF                                                                                                         
        RETURN                                                                                                         
       END IF                                                                                                          
       IF            TMP_PRB_ERROR="#ALIGN_REJECT"                                                                     
        LET          RTN_PRB_LOAD_PPL="Align_reject"                                                                   
       END IF                                                                                                          
       IF            TMP_PRB_ERROR="#ALIGN_ASSIST"                                                                     
        LET          RTN_PRB_LOAD_PPL="Align_assist"                                                                   
       END IF                                                                                                          
      END IF                                                                                                           
                                                                                                                       
      IF             TMP_PRB_STATUS="END"                                                                              
       LET           RTN_PRB_LOAD_PPL="Lot_end"                                                                        
      END IF                                                                                                           
                                                                                                                       
      IF             TMP_PRB_STATUS="OK"                                                                               
       LET           RTN_PRB_LOAD_PPL="Wafer_ready"                                                                    
      END IF                                                                                                           
                                                                                                                       
      IF             RTN_PRB_LOAD_PPL="Prober_abort"                                                                   
       IF            TST_SLOT_REJECT=0                                                                                 
        LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PROBER_LOAD Error " + TMP_PRB_ERROR               
       ELSE                                                                                                            
        LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PROBER_REJECT Error " + TMP_PRB_ERROR             
       END IF                                                                                                          
      END IF                                                                                                           
