             LET                SYS_DATAFILE=SAVE_TMPDATA.Pubname                                                                                                                                                                         
                                                                                                                                                                                                                                          
             IF                 TST_LOT_FIRST=1                                                                                                                                                                                           
              IF                TST_LOT_DATA_FIRST[TST_LOT_CURRENT]=""                                                                                                                                                                    
               LET              TST_LOT_DATA_FIRST[TST_LOT_CURRENT]=SYS_DATAFILE                                                                                                                                                          
              END IF                                                                                                                                                                                                                      
             END IF                                                                                                                                                                                                                       
                                                                                                                                                                                                                                          
             IF                 TST_FLW_DETAILED=1                                                                                                                                                                                        
              IF                RTN_TEST_REPLACE_DATA=0                                                                                                                                                                                   
               IF               RPT_REMOVE_UNUSED=1                                                                                                                                                                                       
                IF              SYS_DATAFILE<>TST_LOT_DATA_FIRST[TST_LOT_CURRENT]                                                                                                                                                         
                 EXECUTE        "rm -f "+TST_LOT_DATA_DIR[TST_LOT_CURRENT]+"/"+SYS_DATAFILE+"."+ENV_ADTFMT                                                                                                                                
                 LET            TST_SLOT_DATA[TST_SLOT_LAST]=2                                                                                                                                                                            
                END IF                                                                                                                                                                                                                    
               END IF                                                                                                                                                                                                                     
               LET              RTN_TEST_REPLACE_DATA=1                                                                                                                                                                                   
               GOTO             Log_lot_end                                                                                                                                                                                               
              END IF                                                                                                                                                                                                                      
                                                                                                                                                                                                                                          
              IF                TST_LOT_FIRST=1                                                                                                                                                                                           
               IF               SYS_DATAFILE<>TST_LOT_DATA_FIRST[TST_LOT_CURRENT]                                                                                                                                                         
                IF              TST_FLW_RNDLD_PREV=1                                                                                                                                                                                      
                 MERGE_DATAFILE Original=TST_LOT_DATA_DIR[TST_LOT_CURRENT]+"/"+TST_LOT_DATA_FIRST[TST_LOT_CURRENT],New=TST_LOT_DATA_DIR[TST_LOT_CURRENT]+"/"+SYS_DATAFILE,Format=ENV_ADTFMT,Numwaf=TMP_WAFER_NUM,Waferid=TMP_WAFERID      
                ELSE                                                                                                                                                                                                                      
                 MERGE_DATAFILE Original=TST_LOT_DATA_DIR[TST_LOT_CURRENT]+"/"+TST_LOT_DATA_FIRST[TST_LOT_CURRENT],New=TST_LOT_DATA_DIR[TST_LOT_CURRENT]+"/"+SYS_DATAFILE,Format=ENV_ADTFMT                                               
                END IF                                                                                                                                                                                                                    
                                                                                                                                                                                                                                          
                IF              MERGE_DATAFILE.Stat=-1                                                                                                                                                                                    
                 LET            PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                                                                                                                         
                 LET            PNL_MSG_MSG1="$ERR_MERGE_DATAFILE"                                                                                                                                                                        
                 LET            PNL_MSG_MSG2=MERGE_DATAFILE.Error                                                                                                                                                                         
                 LET            PNL_KEY_OK="$HALT"                                                                                                                                                                                        
                 PANEL          "HP_DISP_ERR2_OK",2                                                                                                                                                                                       
                 PANEL          "HP_DISP_ERR2_OK",0                                                                                                                                                                                       
                 LOG_SYSTEM     Session=SYSTEM.SESSIONID,Message="TEST_DISP_RESULT: MERGE_DATAFILE Error " + MERGE_DATAFILE.Error                                                                                                         
                 TESTER_STOP                                                                                                                                                                                                              
                 ACTION         "PLG_FWK_END"                                                                                                                                                                                             
                 STOP                                                                                                                                                                                                                     
                END IF                                                                                                                                                                                                                    
                                                                                                                                                                                                                                          
                EXECUTE         "rm -f "+TST_LOT_DATA_DIR[TST_LOT_CURRENT]+"/"+SYS_DATAFILE+"."+ENV_ADTFMT                                                                                                                                
                LET             TST_SLOT_DATA[TST_SLOT_LAST]=2                                                                                                                                                                            
               END IF                                                                                                                                                                                                                     
              END IF                                                                                                                                                                                                                      
             END IF                                                                                                                                                                                                                       
                                                                                                                                                                                                                                          
 Log_lot_end                                                                                                                                                                                                                              
             LOG_LOT_END        Session=SYSTEM.SESSIONID,State=TST_LOT_STATE,Result=TST_LOT_RESULT,Datadir=SYS_DATADIR,Dataname=SYS_DATAFILE+"."+ENV_ADTFMT,Time=SYS_TPLEND                                                               
                                                                                                                                                                                                                                          
             ACTION             "PLG_TPL_END"                                                                                                                                                                                             
