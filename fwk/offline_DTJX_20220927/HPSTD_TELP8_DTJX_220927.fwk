 BODY                                                                                                                                             
               IF             PRB_ONLINE=0                                                                                  Commented out for test
                LET           RTN_PRB_REALIGN="Wafer_ready"                                                                                       
                WAIT          3                                                                                                                   
                RETURN                                                                                                                            
               END IF                                                                                                                             
                                                                                                                                                  
               PROBER_ALIGN                                                                                                                       
                                                                                                                                                  
 ErrorHandling                                                                                                                                    
                LET           TMP_PRB_STATUS=PROBER_ALIGN.Status                                                                                  
                LET           TMP_PRB_ERROR=PROBER_ALIGN.Error                                                                                    
                                                                                                                                                  
                LET           RTN_PRB_REALIGN="Prober_abort"                                                                                      
                                                                                                                                                  
                IF            TMP_PRB_STATUS="ERROR"                                                                                              
                 IF           TMP_PRB_ERROR="#TIMEOUT_SRQ"                                                                                        
                  ACTION      "HP_PROBER_PRB_QUERY_READY_TELP8"                                                                                   
                  LET         RTN_PRB_REALIGN=RTN_PRB_QUERY_READY                                                                                 
                  IF          RTN_PRB_REALIGN="Prober_abort"                                                                                      
                   LOG_PROBER Session=SYSTEM.SESSIONID,Message="PRB_ALIGN: PRB_QUERY_READY Error " + PROBER_READY.Error                           
                  END IF                                                                                                                          
                  RETURN                                                                                                                          
                 END IF                                                                                                                           
                 IF           TMP_PRB_ERROR="#ALIGN_REJECT"                                                                                       
                  LET         RTN_PRB_REALIGN="Align_reject"                                                                                      
                 END IF                                                                                                                           
                 IF           TMP_PRB_ERROR="#ALIGN_ASSIST"                                                                                       
                  LET         RTN_PRB_REALIGN="Align_assist"                                                                                      
                 END IF                                                                                                                           
                END IF                                                                                                                            
                                                                                                                                                  
                IF            TMP_PRB_STATUS="END"                                                                                                
                 LET          RTN_PRB_REALIGN="Lot_end"                                                                                           
                END IF                                                                                                                            
                                                                                                                                                  
