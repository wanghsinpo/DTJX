            LET         CTM_WAF_PASS=CTM_WAF_PASS+1                                                                   
          END IF                                                                                                      
          LET           I=I+1                                                                                         
        END WHILE                                                                                                     
        CTM_TIME_CAL    Start_time=LOT.CTM_TESTSTART,End_time=LOT.CTM_TESTEND                                         
        LET             CTM_LOT_TIME=CTM_TIME_CAL.Timespend                                                           
        LET             CTM_WAF_TIME=CTM_LOT_TIME/CTM_WAF_COUNT                                                       
      END IF                                                                                                          
                                                                                                                      
      LET               TST_TESTER_MODE=1                                                             JL-ooc          
