$Type: Action
$Name: CTM_ERR_HANDLE
$Vers: 1
$Desc: Description for this Action spec.
$Date: 07/11/2012
$Time: 06:23:26
$User: wat
$----------- --------------- ------------------------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                                       
             LET             CTM_REKEY_RECIPE=1                                                                                                             
             LET             CTM_DETAIL_MSG=""                                                                                                              
             LET             PNL_MSG_MSG1=""                                                                                                                
                                                                                                                                                            
 tplbld_err  IF              CTM_ERR_ID=1                                                                                                                   
               LET           CTM_NUM_MSG=CTM_TPLBLD.Ndetail                                                                                                 
               PRINT_STRING  Value="TPLBLD Error:"+CTM_TPLBLD.Error                                                                                         
               LET           CTM_DETAIL_MSG=""                                                                                                              
               LET           I=1                                                                                                                            
               WHILE         I <= CTM_NUM_MSG                                                                                                               
                LET          CTM_MSG[I]=CTM_TPLBLD.Detail[I]                                                                                                
                PRINT_STRING Value=CTM_MSG[I]                                                                                                               
                LET          CTM_DETAIL_MSG=CTM_DETAIL_MSG+CTM_MSG[I]+"\n"                                                                                  
                LET          I = I + 1                                                                                                                      
               END WHILE                                                                                                                                    
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
 tplfmt_err  IF              CTM_ERR_ID=2                                                                                                                   
               PRINT_STRING  Value="TPLFMT Error:"+CTM_TPLFMT.Error                                                                                         
               LET           CTM_DETAIL_MSG=CTM_TPLFMT.Error                                                                                                
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
 limbld_err  IF              CTM_ERR_ID=3                                                                                                                   
               LET           CTM_NUM_MSG=CTM_LIMBLD.Ndetail                                                                                                 
               PRINT_STRING  Value="LIMBLD Error:"+CTM_LIMBLD.Error                                                                                         
               LET           CTM_DETAIL_MSG=""                                                                                                              
               LET           I=1                                                                                                                            
               WHILE         I <= CTM_NUM_MSG                                                                                                               
                LET          CTM_MSG[I]=CTM_LIMBLD.Detail[I]                                                                                                
                PRINT_STRING Value=CTM_MSG[I]                                                                                                               
                LET          CTM_DETAIL_MSG=CTM_DETAIL_MSG+CTM_MSG[I]+"\n"                                                                                  
                LET          I = I + 1                                                                                                                      
               END WHILE                                                                                                                                    
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
 limfmt_err  IF              CTM_ERR_ID=4                                                                                                                   
               PRINT_STRING  Value="LIMFMT Error:"+CTM_LIMFMT.Error                                                                                         
               LET           CTM_DETAIL_MSG=CTM_LIMFMT.Error                                                                                                
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
 Card_issue  IF              CTM_ERR_ID=5                                                                                                                   
               IF            CTM_PRBSPEC<>CTM_PCIL_ACCESS.Pcard_spec                                                                                        
                 LET         CTM_DETAIL_MSG="There are 2 reasons for this error \n 1. MA used wrong card \n 2. Call engineer to check PIL's Card_type"      
               END IF                                                                                                                                       
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
             IF              CTM_ERR_ID=6                                                                                                                   
               IF            CTM_PCIL_ACCESS.Err="MISS"                                                                                                     
                 LET         CTM_DETAIL_MSG="Can NOT find "+CTM_PCIL+", check if it is lost !"                                                              
               END IF                                                                                                                                       
               IF            CTM_PCIL_ACCESS.Err="NOT_FOUND"                                                                                                
                 LET         CTM_DETAIL_MSG="The card id can NOT be recognized in "+CTM_PCIL                                                                
               END IF                                                                                                                                       
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
 PIL_issue   IF              CTM_ERR_ID=7                                                                                                                   
               IF            CTM_PIL_ACCESS.Status=1                                                                                                        
                LET          CTM_DETAIL_MSG="Two or more recipe are found by name: "+TST_RCP_PROCESS                                                        
               END IF                                                                                                                                       
               IF            CTM_PIL_ACCESS.Status=-3                                                                                                       
                LET          CTM_DETAIL_MSG="Can NOT find the Product ID in "+CTM_ROOT_DIR+"/ETC/"+CTM_PIL                                                  
               END IF                                                                                                                                       
               IF            CTM_PIL_ACCESS.Status=-2                                                                                                       
                LET          CTM_DETAIL_MSG="Can NOT find the file: "+CTM_ROOT_DIR+"/ETC/"+CTM_PIL                                                          
               END IF                                                                                                                                       
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
 Keyin_issue IF              CTM_ERR_ID=8                                                                                                                   
                LET          CTM_DETAIL_MSG="Please fill in LOT ID, Prodct ID, and Operator ID..."                                                          
               GOTO          Fatal_Error                                                                                                                    
             END IF                                                                                                                                         
                                                                                                                                                            
             RETURN                                                                                                                                         
 Fatal_Error                                                                                                                                                
             LET             PNL_MSG_MSG1=CTM_MSG_ARY[CTM_ERR_ID]+"\n\n"+CTM_DETAIL_MSG                                                                     
