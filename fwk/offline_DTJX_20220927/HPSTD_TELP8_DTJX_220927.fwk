$Type: Action
$Name: HP_SEL_CONFIG_FIL
$Vers: 1
$Desc: Set file and directory configuration.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$----------- ----------- ---------------------------------------------------------------------------------- - - -
 BODY                                                                                                            
             LET         TMP_DIR_TPL=DIR_TESTPLAN                                                                
             LET         TMP_DIR_LIMIT=DIR_LIMITFILE                                                             
             LET         TMP_DIR_DATA=DIR_DATAFILE                                                               
             LET         TMP_FILE_RECIPE=FIL_RECIPE                                                              
                                                                                                                 
             IF          DIR_TESTPLAN_MOV=1                                                                      
              LET        TMP_DIR_TPL_MOV[1]="$ON"                                                                
             ELSE                                                                                                
              LET        TMP_DIR_TPL_MOV[1]="$OFF"                                                               
             END IF                                                                                              
             IF          DIR_LIMITFILE_MOV=1                                                                     
              LET        TMP_DIR_LIMIT_MOV[1]="$ON"                                                              
             ELSE                                                                                                
              LET        TMP_DIR_LIMIT_MOV[1]="$OFF"                                                             
             END IF                                                                                              
                                                                                                                 
             PANEL       "HP_DISP_BLANK",2                                                                       
             PANEL       "HP_SET_FILENV",2                                                                       
                                                                                                                 
             IF          PANEL.RETURN="CANCEL"                                                                   
              PANEL      "HP_SET_FILENV",0                                                                       
              PANEL      "HP_DISP_BLANK",0                                                                       
              RETURN                                                                                             
             END IF                                                                                              
                                                                                                                 
             FORMAT_PATH Original=TMP_DIR_TPL                                                                    
             LET         TMP_DIR_TPL=FORMAT_PATH.Path                                                            
             VERIFY_FILE Filename=TMP_DIR_TPL,Type="Directory"                                                   
             IF          (VERIFY_FILE.Readable=0) OR (VERIFY_FILE.Executable=0)                                  
              GOTO       Error_found                                                                             
             END IF                                                                                              
                                                                                                                 
             FORMAT_PATH Original=TMP_DIR_LIMIT                                                                  
             LET         TMP_DIR_LIMIT=FORMAT_PATH.Path                                                          
             VERIFY_FILE Filename=TMP_DIR_LIMIT,Type="Directory"                                                 
             IF          (VERIFY_FILE.Readable=0) OR (VERIFY_FILE.Executable=0)                                  
              GOTO       Error_found                                                                             
             END IF                                                                                              
                                                                                                                 
             FORMAT_PATH Original=TMP_DIR_DATA                                                                   
             LET         TMP_DIR_DATA=FORMAT_PATH.Path                                                           
             VERIFY_FILE Filename=TMP_DIR_DATA,Type="Directory"                                                  
             IF          (VERIFY_FILE.Readable=0) OR (VERIFY_FILE.Writable=0) OR (VERIFY_FILE.Executable=0)      
              GOTO       Error_found                                                                             
             END IF                                                                                              
                                                                                                                 
             FORMAT_PATH Original=TMP_FILE_RECIPE                                                                
             LET         TMP_FILE_RECIPE=FORMAT_PATH.Path                                                        
             VERIFY_FILE Filename=TMP_FILE_RECIPE,Type="Normal"                                                  
             IF          VERIFY_FILE.Readable=0                                                                  
              GOTO       Error_found                                                                             
             END IF                                                                                              
                                                                                                                 
             LET         DIR_TESTPLAN=TMP_DIR_TPL                                                                
             LET         DIR_LIMITFILE=TMP_DIR_LIMIT                                                             
             LET         DIR_DATAFILE=TMP_DIR_DATA                                                               
             LET         FIL_RECIPE=TMP_FILE_RECIPE                                                              
                                                                                                                 
             IF          TMP_DIR_TPL_MOV[1]="$ON"                                                                
              LET        DIR_TESTPLAN_MOV=1                                                                      
             ELSE                                                                                                
              LET        DIR_TESTPLAN_MOV=0                                                                      
             END IF                                                                                              
             IF          TMP_DIR_LIMIT_MOV[1]="$ON"                                                              
              LET        DIR_LIMITFILE_MOV=1                                                                     
             ELSE                                                                                                
              LET        DIR_LIMITFILE_MOV=0                                                                     
             END IF                                                                                              
                                                                                                                 
             ACTION      "HP_SET_TAG_FILER"                                                                      
                                                                                                                 
             PANEL       "HP_SET_FILENV",0                                                                       
             PANEL       "HP_DISP_BLANK",0                                                                       
             RETURN                                                                                              
 Error_found                                                                                                     
             LET         PNL_TTL_LABEL="$TTL_CONFIG_FILE"                                                        
             LET         PNL_MSG_MSG1="$ERR_CONFIG_FILE"                                                         
             LET         PNL_KEY_OK="$OK"                                                                        
             PANEL       "HP_DISP_ERR1_OK",1                                                                     
             PANEL       "HP_DISP_ERR1_OK",0                                                                     
             PANEL       "HP_DISP_BLANK",0                                                                       
