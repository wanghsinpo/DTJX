$Type: Action
$Name: HP_PROBER_TEMPSET_TELP8
$Vers: 1
$Desc: Description for this Panel spec.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$-------------- ---------- ----------------------------------------------------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                                                                 
                IF         PRB_ONLINE=0                                                                                                                                               
                 LET       RTN_PROBER_TEMPSET="Continue"                                                                                                                              
                 RETURN                                                                                                                                                               
                END IF                                                                                                                                                                
                LET        RTN_PROBER_TEMPSET="Continue"                                                                                                                              
                                                                                                                                                                                      
 WarningMessage                                                                                                                                                                       
                LET        PNL_TTL_LABEL="$TTL_TEMPSET"                                                                                                                               
                IF         WAFER.TMP_PRB_SUB_ALGN_AUTO[1]=PNL_CHOICE_ALIGN[1]                                                                                                         
                 LET       PNL_MSG_MSG1="$AUTO_REALIGN"                                                                                                                               
                ELSE                                                                                                                                                                  
                 LET       PNL_MSG_MSG1="$WARNING_TEMPSET"                                                                                                                            
                END IF                                                                                                                                                                
                LET        PNL_KEY_F1="$CONTINUE"                                                                                                                                     
                LET        PNL_KEY_F4="$CANCEL"                                                                                                                                       
                PANEL      "HP_DISP_MSG1_F1_F4",2                                                                                                                                     
                PANEL      "HP_DISP_MSG1_F1_F4",0                                                                                                                                     
                                                                                                                                                                                      
                IF         PANEL.RETURN="F1"                                                                                                                                          
                 LET       PNL_MSG_MSG1="$EXEC_TEMPSET"                                                                                                                               
                 LET       PNL_KEY_F4="$ABORT_TEMPSET"                                                                                                                                
                 PANEL     "HP_PROBER_TEMPSET",2                                                                                                                                      
 SetTemperature                                                                                                                                                                       
                 ACTION    "HP_PROBER_PRB_TEMPSET_TELP8"                                                                                                                              
                 IF        RTN_PRB_TEMPSET<>"Wafer_ready"                                                                                                                             
                  LET      PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                                                          
                  LET      PNL_MSG_MSG1="$TEMPSET_FAIL"                                                                                                                               
                  LET      PNL_KEY_OK="$OK"                                                                                                                                           
                  PANEL    "HP_DISP_ERR1_OK"                                                                                                                                          
                  PANEL    "HP_DISP_ERR1_OK",0                                                                                                                                        
                  RETURN                                                                                                                                                              
                 END IF                                                                                                                                                               
 WaitTempset                                                                                                                                                                          
                 LET       HP_TEMPSET_FINISH=0                                                                                                                                        
                 WHILE     ((WAFER.TMP_CURRTMP-WAFER.PRB_SUB_TEMP>WAFER.PRB_SUB_TRLC) OR (WAFER.TMP_CURRTMP-WAFER.PRB_SUB_TEMP<-1*WAFER.PRB_SUB_TRLC)) AND (HP_TEMPSET_FINISH=0)      
                  WAIT     Wait=HP_TEMPMON_INTERVAL                                                                                                                                   
                                                                                                                                                                                      
                  ACTION   "HP_PROBER_PRB_TEMPMON_TELP8"                                                                                                                              
                  IF       RTN_PRB_TEMPMON<>"Wafer_ready"                                                                                                                             
                   LET     PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                                                          
                   LET     PNL_MSG_MSG1="$TEMPMON_FAIL"                                                                                                                               
                   LET     PNL_KEY_OK="$OK"                                                                                                                                           
                   PANEL   "HP_DISP_ERR1_OK",2                                                                                                                                        
                   PANEL   "HP_DISP_ERR1_OK",0                                                                                                                                        
                   RETURN                                                                                                                                                             
                  ELSE                                                                                                                                                                
                   UPDATE  "HP_PROBER_TEMPSET"                                                                                                                                        
                  END IF                                                                                                                                                              
                                                                                                                                                                                      
                 END WHILE                                                                                                                                                            
                 PANEL     "HP_PROBER_TEMPSET",0                                                                                                                                      
                                                                                                                                                                                      
                 IF        HP_TEMPSET_FINISH=1                                                                                                                                        
                  LET      PNL_MSG_MSG1="$TEMPSET_ABORTED"                                                                                                                            
                 ELSE                                                                                                                                                                 
                  LET      PNL_MSG_MSG1="$FINISH_TEMPSET"                                                                                                                             
                 END IF                                                                                                                                                               
                 PANEL     "HP_DISP_MSG1_NONE",2                                                                                                                                      
                 WAIT      Wait=3                                                                                                                                                     
                 PANEL     "HP_DISP_MSG1_NONE",0                                                                                                                                      
                 IF        (WAFER.TMP_PRB_SUB_ALGN_AUTO)[1]=PNL_CHOICE_ALIGN[1]                                                                                                       
                  IF       WAFER.PRB_SUB_SOAK>0                                                                                                                                       
                   LET     PNL_MSG_MSG1="$WAIT_SOAK"                                                                                                                                  
                   PANEL   "HP_DISP_MSG1_NONE",2                                                                                                                                      
                   WAIT    Wait=WAFER.PRB_SUB_SOAK                                                                                                                                    
                   PANEL   "HP_DISP_MSG1_NONE",0                                                                                                                                      
                  END IF                                                                                                                                                              
                  ACTION   "HP_PROBER_REALIGN_TELP8"                                                                                                                                  
                 END IF                                                                                                                                                               
 CancelTempset                                                                                                                                                                        
                ELSE                                                                                                                                                                  
                 LET       RTN_PRB_TEMPSET="Wafer_ready"                                                                                                                              
                 LET       PNL_MSG_MSG1="$CANCEL_TEMPSET"                                                                                                                             
                 PANEL     "HP_DISP_MSG1_NONE",2                                                                                                                                      
                 WAIT      Wait=3                                                                                                                                                     
                 PANEL     "HP_DISP_MSG1_NONE",0                                                                                                                                      
                END IF                                                                                                                                                                
                                                                                                                                                                                      
                RETURN                                                                                                                                                                
