$Type: Action
$Name: HP_EXEC_AUTOPROBE_GO
$Vers: 1
$Desc: Probe selected failed module.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$-------------- ----------- ----------------------------------------- - - ------------------------------------
 BODY                                                                                                         
 Calc_typeindex                                                                                               
                LET         PRB_AT_TYPI=1                                                                     
                WHILE       PRB_AT_TYPI<=DIE.TYPETOTAL                                                        
                 IF         DIE.DEFINELIST[PRB_AT_TYPI]=PRB_AT_TYPN                                           
                  GOTO      Probe_selected                                                                    
                 END IF                                                                                       
                 LET        PRB_AT_TYPI=PRB_AT_TYPI+1                                                         
                END WHILE                                                                                     
                RETURN                                                                                        
 Probe_selected                                                                                               
                GET_LOC_MOD Typeindex=PRB_AT_TYPI,Modname=PRB_AT_MODN                                         
                IF          GET_LOC_MOD.Stat=-1                                                               
                 RETURN                                                                                       
                END IF                                                                                        
                                                                                                              
