$Desc: Set tag variables for option menus.\n  SPECS2.5: Moved some variables to other action spec.\n            Added some menus for detailed test control.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$-------- ------- ------------------------------------------------------ - - ---------------------------
 BODY                                                                                                   
          LET     MNU_EXEC_NUM=0                                                                        
                                                                                                        
          IF      UIF_SEL_DIE_PAUSE=1                                                                   
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM+1                                                           
           LET    MNU_EXEC_TTL[1]="$WAFER_SELECT"                                                       
           LET    MNU_EXEC_ACT[1]="HP_SEL_WAFTST_PAUSE"                                                 
          END IF                                                                                        
                                                                                                        
          IF      UIF_MODIFY_SEQ=1                                                                      
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM+1                                                           
           LET    MNU_EXEC_TTL[MNU_EXEC_NUM]="$RETEST_REJECT"                                           
           LET    MNU_EXEC_ACT[MNU_EXEC_NUM]="HP_SEL_RETEST_REJECT"                                     
          END IF                                                                                        
                                                                                                        
          IF      (MNU_OPT1_LABEL<>"") AND (MNU_OPT1_ACT<>"")                                           
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM+1                                                           
           LET    MNU_EXEC_TTL[MNU_EXEC_NUM]=MNU_OPT1_LABEL                                             
           LET    MNU_EXEC_ACT[MNU_EXEC_NUM]=MNU_OPT1_ACT                                               
          END IF                                                                                        
                                                                                                        
          IF      (MNU_OPT2_LABEL<>"") AND (MNU_OPT2_ACT<>"")                                           
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM+1                                                           
           LET    MNU_EXEC_TTL[MNU_EXEC_NUM]=MNU_OPT2_LABEL                                             
           LET    MNU_EXEC_ACT[MNU_EXEC_NUM]=MNU_OPT2_ACT                                               
          END IF                                                                                        
                                                                                                        
          IF      (MNU_OPT3_LABEL<>"") AND (MNU_OPT3_ACT<>"")                                           
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM+1                                                           
           LET    MNU_EXEC_TTL[MNU_EXEC_NUM]=MNU_OPT3_LABEL                                             
           LET    MNU_EXEC_ACT[MNU_EXEC_NUM]=MNU_OPT3_ACT                                               
          END IF                                                                                        
                                                                                                        
          IF      TST_FLW_DETAILED=1                                                                    
           IF     UIF_SEL_FLW_RUNTIME=1                                                                 
            LET   MNU_EXEC_NUM=MNU_EXEC_NUM + 1                              For Test Flow Control Panel
            LET   MNU_EXEC_TTL[MNU_EXEC_NUM]="$TEST_FLOW"                                               
            LET   MNU_EXEC_ACT[MNU_EXEC_NUM]="HP_SEL_CONFIG_FLW_RUNTIME"                                
           END IF                                                                                       
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM + 1                              For Breakpoint Panel       
           LET    MNU_EXEC_TTL[MNU_EXEC_NUM]="$TEST_BREAK"                                              
           LET    MNU_EXEC_ACT[MNU_EXEC_NUM]="HP_SEL_CONFIG_BRK"                                        
           LET    MNU_EXEC_NUM=MNU_EXEC_NUM + 1                              For Direct Execution       
           LET    MNU_EXEC_TTL[MNU_EXEC_NUM]="$EXEC_DIRECT"                                             
           LET    MNU_EXEC_ACT[MNU_EXEC_NUM]="HP_TEST_EXEC_DIRECT"                                      
          END IF                                                                                        
 End_menu                                                                                               
          IF      MNU_EXEC_NUM<>0                                                                       
           LET    UIF_PAUSE_TYPE="MENU"                                                                 
          ELSE                                                                                          
           LET    UIF_PAUSE_TYPE="NONE"                                                                 
          END IF                                                                                        
