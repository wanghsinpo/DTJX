$Type: Action
$Name: HP_PROBER_LOAD_ONE_RND_TELP8
$Vers: 1
$Desc: Description for this Action spec.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$--------------- ------------- -------------------------------------------------------------------------------- - - -------------------------
 BODY                                                                                                                                        
                 IF            PRB_ONLINE=0                                                                                                  
                  IF           (TST_FLW_DETAILED=1) AND (TST_FLW_RNDLD=1)                                                                    
                   LET         TST_SLOT_CURRENT=0                                                                                            
                  END IF                                                                                                                     
                  REPEAT                                                                                                                     
                  LET          TST_SLOT_CURRENT=TST_SLOT_CURRENT+1                                                                           
                   IF          TST_SLOT_CURRENT>PRB_SLOT_TOTAL_ALL                                                                           
                    LET        RTN_LOAD_ONE="Lot_end"                                                                                        
                    RETURN                                                                                                                   
                   END IF                                                                                                                    
                  UNTIL        TST_SLOT_SELECT[TST_SLOT_CURRENT]=1                                                                           
                  LET          RTN_LOAD_ONE="Wafer_ready"                                                                                    
                  RETURN                                                                                                                     
                 END IF                                                                                                                      
                                                                                                                                             
                 LET           PNL_TTL_LABEL="$TTL_PROBER_OPERATION"                                                                         
                 LET           PNL_MSG_MSG1="$TELP8_ALIGNMENT_ERROR"                                                                         
                 GOTO          Load_next_wafer                                                                                               
 Reject_wafer                                                                                                                                
                 ACTION        "HP_PROBER_PRB_GET_CHKSLOT_TELP8"                                                                             
                 IF            RTN_PRB_GET_CHKSLOT<>"Ready"                                                                                  
                  LET          RTN_LOAD_ONE="Prober_abort"                                                                                   
                  RETURN                                                                                                                     
                 END IF                                                                                                                      
                 WRITE_INTEGER "%d",TST_SLOT_CURRENT                                                                                         
                 LOG_PROBER    Session=SYSTEM.SESSIONID,Message="ALIGNMENT ERROR: Slot " + WRITE_INTEGER.String                              
 Load_next_wafer                                                                                                                             
                 IF            (TST_FLW_DETAILED=1) AND (TST_FLW_RNDLD=1)                                                                    
                  LET          TST_SLOT_CURRENT=0                                                                                            
                 END IF                                                                                                                      
                 REPEAT                                                                                                                      
                  LET          TST_SLOT_CURRENT=TST_SLOT_CURRENT+1                                                                           
                  IF           TST_SLOT_CURRENT>PRB_SLOT_TOTAL_ALL                                                                           
                   ACTION      "HP_PROBER_PRB_LOTEND_RND_TELP8"                                                                              
                   LET         RTN_LOAD_ONE=RTN_PRB_LOTEND                                                                                   
                   RETURN                                                                                                                    
                  END IF                                                                                                                     
                 UNTIL         TST_SLOT_SELECT[TST_SLOT_CURRENT]=1                                                                           
                                                                                                                                             
                 IF            TST_SLOT_CURRENT<=PRB_SLOT_TOTAL_CASS                                                                         
                  LET          PRB_LOAD_CASS=1                                                                                               
                  LET          PRB_LOAD_SLOT=TST_SLOT_CURRENT                                                                                
                 ELSE                                                                                                                        
                  LET          PRB_LOAD_CASS=2                                                                                               
                  LET          PRB_LOAD_SLOT=TST_SLOT_CURRENT-PRB_SLOT_TOTAL_CASS                                                            
                 END IF                                                                                                                      
                                                                                                                                             
                 ACTION        "HP_PROBER_PRB_LOAD_RND_TELP8"                                                                                
                                                                                                                                             
                 IF            RTN_PRB_LOAD_RND="Align_reject"                                                                               
                  GOTO         Reject_wafer                                                                                                  
                 END IF                                                                                                                      
                 IF            RTN_PRB_LOAD_RND="Align_assist"                                                                               
                  GOTO         Query_assist                                                                                                  
                 END IF                                                                                                                      
                 IF            RTN_PRB_LOAD_RND="Wafer_ready"                                                                                
                  LET          RTN_LOAD_ONE="Wafer_ready"                                                                                    
                 ELSE                                                                                                                        
                  IF           RTN_PRB_LOAD_RND="Lot_end"                                                                                    
                   LET         RTN_LOAD_ONE="Lot_end"                                                                                        
                  ELSE                                                                                                                       
                   LET         RTN_LOAD_ONE="Prober_abort"                                                          "No_wafer","Prober_abort"
                  END IF                                                                                                                     
                 END IF                                                                                                                      
                 RETURN                                                                                                                      
 Query_assist                                                                                                                                
                 PANEL         "HP_DISP_ERR1_NONE",2                                                                                         
                                                                                                                                             
                 ACTION        "HP_PROBER_PRB_QUERY_READY_TELP8"                                                                             
                                                                                                                                             
                 PANEL         "HP_DISP_ERR1_NONE",0                                                                                         
                                                                                                                                             
                 IF            RTN_PRB_QUERY_READY="Align_reject"                                                                            
                  GOTO         Reject_wafer                                                                                                  
                 END IF                                                                                                                      
                 IF            RTN_PRB_QUERY_READY="Align_assist"                                                                            
                  GOTO         Query_assist                                                                                                  
                 END IF                                                                                                                      
                 IF            RTN_PRB_QUERY_READY="Wafer_ready"                                                                             
                  LET          RTN_LOAD_ONE="Wafer_ready"                                                                                    
                 ELSE                                                                                                                        
                  LET          RTN_LOAD_ONE="Prober_abort"                                                                                   
                 END IF                                                                                                                      
