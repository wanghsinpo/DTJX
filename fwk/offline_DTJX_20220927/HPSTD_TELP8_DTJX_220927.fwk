$Type: Action
$Name: HP_SET_SLOT_OBO_G8_ML2C
$Vers: 1
$Desc: Set next wafer to load.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- --------------- -------------------------------- - - -
 BODY                                                       
      SET_TAG_INTEGER Name="TST_SLOT_SELECT"                
      LET             RTN_TEST_SEL_WAFER="Next"             
                                                            
      IF              PNL_MRCP_SEL2_SEL[1]="1-15"           
       LET            TMP_SLOT_POS=1                        
       REPEAT                                               
        IF            TMP_SLOTA_SEL[TMP_SLOT_POS]>0         
         LET          TST_SLOT_SELECT[TMP_SLOT_POS]=1       
         RETURN                                             
        END IF                                              
        LET           TMP_SLOT_POS=TMP_SLOT_POS+1           
       UNTIL          TMP_SLOT_POS>15                       
      ELSE                                                  
       IF             PNL_MRCP_SEL2_SEL[1]="16-25"          
        LET           TMP_SLOT_POS=16                       
        REPEAT                                              
         IF           TMP_SLOTB_SEL[TMP_SLOT_POS-15]>0      
          LET         TST_SLOT_SELECT[TMP_SLOT_POS]=1       
          RETURN                                            
         END IF                                             
        LET           TMP_SLOT_POS=TMP_SLOT_POS+1           
        UNTIL         TMP_SLOT_POS>25                       
       ELSE                                                 
        IF            PNL_MRCP_SEL2_SEL[1]="26-40"          
         LET          TMP_SLOT_POS=26                       
         REPEAT                                             
          IF          TMP_SLOTC_SEL[TMP_SLOT_POS-25]>0      
           LET        TST_SLOT_SELECT[TMP_SLOT_POS]=1       
           RETURN                                           
          END IF                                            
         LET          TMP_SLOT_POS=TMP_SLOT_POS+1           
         UNTIL        TMP_SLOT_POS>40                       
        ELSE                                                
         LET          TMP_SLOT_POS=41                       
         REPEAT                                             
          IF          TMP_SLOTD_SEL[TMP_SLOT_POS-40]>0      
           LET        TST_SLOT_SELECT[TMP_SLOT_POS]=1       
           RETURN                                           
          END IF                                            
         LET          TMP_SLOT_POS=TMP_SLOT_POS+1           
         UNTIL        TMP_SLOT_POS>50                       
        END IF                                              
       END IF                                               
      END IF                                                
                                                            
      LET             RTN_TEST_SEL_WAFER="Retry"            
