$Type: Action
$Name: HP_TEST_UPDATE_OFFSET
$Vers: 1
$Desc: Update offset parameter.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- ------ ------------------------------- - - -
 BODY                                             
      IF     WAFER.COORDINATE=1                   
       LET   PRB_DIR_X=+1                         
       LET   PRB_DIR_Y=+1                         
      END IF                                      
      IF     WAFER.COORDINATE=2                   
       LET   PRB_DIR_X=-1                         
       LET   PRB_DIR_Y=+1                         
      END IF                                      
      IF     WAFER.COORDINATE=3                   
       LET   PRB_DIR_X=-1                         
       LET   PRB_DIR_Y=-1                         
      END IF                                      
      IF     WAFER.COORDINATE=4                   
       LET   PRB_DIR_X=+1                         
       LET   PRB_DIR_Y=-1                         
      END IF                                      
                                                  
      IF     TMP_UIF_SEL_OFFSETX[1]="$STAY"       
       LET   PRB_OFFSETX=0                        
      END IF                                      
      IF     TMP_UIF_SEL_OFFSETX[1]="$RIGHT"      
       LET   PRB_OFFSETX=PRB_DIR_X                
      END IF                                      
      IF     TMP_UIF_SEL_OFFSETX[1]="$LEFT"       
       LET   PRB_OFFSETX=(-1)*PRB_DIR_X           
      END IF                                      
      IF     TMP_UIF_SEL_OFFSETY[1]="$STAY"       
       LET   PRB_OFFSETY=0                        
      END IF                                      
      IF     TMP_UIF_SEL_OFFSETY[1]="$UPPER"      
       LET   PRB_OFFSETY=PRB_DIR_Y                
      END IF                                      
      IF     TMP_UIF_SEL_OFFSETY[1]="$LOWER"      
       LET   PRB_OFFSETY=(-1)*PRB_DIR_Y           
      END IF                                      
$---- ------ ------------------------------- - - -

$Type: Action
$Name: HP_TEST_UPDATE_RESULT_WAFER_END
$Vers: 1
$Desc: Update die judgement result.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- --------------- ---------------------------------------------------------------------------------------------------------------------------- - - -----------------------------
 BODY                                                                                                                                                                               
      IF              SYSTEM.RETEST="WAFER"                                                                                                                                         
       LET            TMP_TYPE_NUM=1                                                                                                                                                
       WHILE          DIE.TYPENUMBER>=TMP_TYPE_NUM                                                                                                     For measured die type.       
        LET           TMP_DIE_NUM=1                                                                                                                                                 
        GET_JDG_DIE   Typeindex=TMP_TYPE_NUM,Dieindex=TMP_DIE_NUM                                                                                                                   
        WHILE         GET_JDG_DIE.Judge>=0                                                                                                             For all dice in the die type.
         IF           GET_JDG_DIE.Judge=PNL_TEST_WAFER_SKP                                                                                                                          
          SET_JDG_DIE Typeindex=TMP_TYPE_NUM,Dieindex=TMP_DIE_NUM,Judge=PNL_TEST_WAFER_YET,Offsetx=GET_JDG_DIE.Offsetx,Offsety=GET_JDG_DIE.Offsety                                  
         END IF                                                                                                                                                                     
         LET          TMP_DIE_NUM=TMP_DIE_NUM+1                                                                                                                                     
         GET_JDG_DIE  Typeindex=TMP_TYPE_NUM,Dieindex=TMP_DIE_NUM                                                                                                                   
        END WHILE                                                                                                                                                                   
        LET           TMP_TYPE_NUM=TMP_TYPE_NUM+1                                                                                                                                   
       END WHILE                                                                                                                                                                    
      ELSE                                                                                                                                                                          
       LET            TMP_TYPE_NUM=1                                                                                                                                                
       WHILE          DIE.TYPETOTAL>=TMP_TYPE_NUM                                                                                                                                   
        LET           TMP_DIE_NUM=1                                                                                                                                                 
        GET_JDG_DIE   Typeindex=TMP_TYPE_NUM,Dieindex=TMP_DIE_NUM                                                                                                                   
        WHILE         GET_JDG_DIE.Judge>=0                                                                                                                                          
         IF           GET_JDG_DIE.Judge=PNL_TEST_WAFER_YET                                                                                                                          
          SET_JDG_DIE Typeindex=TMP_TYPE_NUM,Dieindex=TMP_DIE_NUM,Judge=PNL_TEST_WAFER_SKP,Offsetx=PRB_OFFSETX,Offsety=PRB_OFFSETY                                                  
         END IF                                                                                                                                                                     
         LET          TMP_DIE_NUM=TMP_DIE_NUM+1                                                                                                                                     
         GET_JDG_DIE  Typeindex=TMP_TYPE_NUM,Dieindex=TMP_DIE_NUM                                                                                                                   
        END WHILE                                                                                                                                                                   
        LET           TMP_TYPE_NUM=TMP_TYPE_NUM+1                                                                                                                                   
       END WHILE                                                                                                                                                                    
      END IF                                                                                                                                                                        
                                                                                                                                                                                    
      UPDATE          "HP_DISP_TEST_RUN",UPD_TEST_WAFER_END                                                                                                                         
