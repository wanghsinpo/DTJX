$Type: Action
$Name: PLG_TEST_BEGIN
$Vers: 1
$Desc: 201209 sami add;Plug ACTION called before each test.
$Date: 09/23/2012
$Time: 05:03:55
$User: wat
$---- --------------------- --------------------------------------------------------------------------------- - - -
 BODY                                                                                                              
      IF                    MODULE.NUMBER>=0 AND CTM_MODULE_CHECK=0                                                
        LET                 I=1                                                                                    
        WHILE               I<=MODULE.TOTAL                                                                        
          IF                MODULE.DEFINELIST[I]<>""                                                               
            CTM_FILE_LOOKUP File=CTM_ROOT_DIR+CTM_RCP_DIR+"/DIE/"+CTM_DIESPEC+".die",Key=MODULE.DEFINELIST[I]      
            LET             CTM_MODULE_CHECK=1                                                                     
            IF              CTM_FILE_LOOKUP.Stat<>0                                                                
             LET            PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                      
             LET            PNL_MSG_MSG1="Module Not Found"                                                        
             LET            PNL_MSG_MSG2=MODULE.DEFINELIST[I]+" is not found in die file "+CTM_DIESPEC             
             LET            PNL_KEY_OK="OK"                                                                        
             PANEL          "HP_DISP_ERR2_OK",2                                                                    
             PANEL          "HP_DISP_ERR2_OK",0                                                                    
             ACTION         "PLG_BREAK_MODULE_BEGIN"                                                               
            END IF                                                                                                 
          END IF                                                                                                   
          LET               I=I+1                                                                                  
        END WHILE                                                                                                  
      END IF                                                                                                       
$---- --------------------- --------------------------------------------------------------------------------- - - -

