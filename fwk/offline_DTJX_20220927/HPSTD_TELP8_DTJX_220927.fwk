$Type: Action
$Name: HP_SET_TAG_LOOP_BEGIN
$Vers: 1
$Desc: Initialize tags on LOOP_BEGIN.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- -------- ---------------------------------------------------- - - ----------------
 BODY                                                                                   
      LET      TST_LOT_CURRENT=TST_SLOT_LOT_INDEX[TST_SLOT_CURRENT]                     
      LET      JDG_LOT_WAFER_VALID=0                                                    
      LET      TMP_SLOT_POS=TST_SLOT_CURRENT                                            
      LET      TST_WAFER_FIRST=1                                                        
      LET      TST_WAFER_ENABLE_PAUSE=0                                                 
                                                                                        
      REPEAT                                                                            
       IF      TST_SLOT_SELECT[TMP_SLOT_POS]=1                                          
        IF     TST_SLOT_LOT_INDEX[TMP_SLOT_POS]<>TST_LOT_CURRENT                        
         BREAK                                                                          
        END IF                                                                          
        LET    JDG_LOT_WAFER_VALID=JDG_LOT_WAFER_VALID+1                                
       END IF                                                                           
       LET     TMP_SLOT_POS=TMP_SLOT_POS+1                                              
      UNTIL    TMP_SLOT_POS>PRB_SLOT_TOTAL_ALL                                          
                                                                                        
      LET      JDG_LOT_WAFER_PASS=0                                                     
      LET      JDG_LOT_WAFER_FAIL=0                                                     
      LET      PRB_OFFSETX=0                                                            
      LET      PRB_OFFSETY=0                                                            
      LET      PRB_OFFSETX_LAST=0                                                       
      LET      PRB_OFFSETY_LAST=0                                                       
