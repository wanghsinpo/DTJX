$Type: Action
$Name: HP_TEST_SEL_WAFER_FIRST
$Vers: 1
$Desc: Select wafer to test (first time only).\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- -------- -------------------------------------------------------- - - -
 BODY                                                                        
      IF       TST_FLW_DETAILED=1                                            
       LET     TST_SFX="_P"                                                  
      ELSE                                                                   
       LET     PNL_LBL_BTN1="$TEST_START"                                    
       LET     PNL_LBL_BTN4="$END_TEST"                                      
       LET     TST_SFX=""                                                    
      END IF                                                                 
      LET      PRB_SEL_OBO=PRB_SEL_OBO_OFF                                   
      IF       TST_FLW_DETAILED=1                                            
       IF      PRB_RANDOM=1                                                  
        IF     TST_FLW_RNDLD=1                                               
         LET   PRB_SEL_OBO=PRB_SEL_OBO_ON                                    
        END IF                                                               
       END IF                                                                
      END IF                                                                 
                                                                             
      IF       TST_LOT_NUM="$MULTIPLE"                                       
       ACTION  "HP_TEST_SEL_WAFER_" + PRB_SEL_OBO + PRB_GENE + "_MULTI"      
      ELSE                                                                   
       ACTION  "HP_TEST_SEL_WAFER_" + PRB_SEL_OBO + PRB_GENE + "_1OR2"       
      END IF                                                                 
      IF       RTN_TEST_SEL_WAFER="Cancel"                                   
       ACTION  "HP_PROBER_LOAD_CANCEL_" + PRB_MODEL                          
      END IF                                                                 
                                                                             
      LET      TST_SFX=""                                                    
$---- -------- -------------------------------------------------------- - - -

