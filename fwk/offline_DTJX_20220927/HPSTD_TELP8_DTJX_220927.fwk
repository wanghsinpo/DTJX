 BODY                                                                                        
       IF         PRB_MODEL="TELP8"                                                          
        LET       PRB_GENE="G8"                                                              
       END IF                                                                                
       IF         PRB_MODEL="TSK90A"                                                         
        LET       PRB_GENE="G8"                                                              
       END IF                                                                                
       IF         PRB_MODEL="EG4080"                                                         
        LET       PRB_GENE="G8"                                                              
        IF        PRB_CASS_NUM="$DOUBLE"                                                     
         LET      PNL_MSG_MSG1="$ERR_DEF_MISMATCH_MODEL_CASS"                                
         GOTO     Error                                                                      
        END IF                                                                               
       END IF                                                                                
                                                                                             
       IF         PRB_GENE="G8"                                                              
        LET       PRB_SLOT_TOTAL_CASS=25                                                     
       END IF                                                                                
                                                                                             
       IF         PRB_CASS_NUM="$SINGLE"                                                     
        LET       PRB_SLOT_TOTAL_ALL=PRB_SLOT_TOTAL_CASS                                     
        IF        TST_LOT_NUM="$SINGLE"                                                      
         LET      TST_TYPE_USE="1L1C"                                                        
        ELSE                                                                                 
         IF       TST_LOT_NUM="$MULTIPLE"                                                    
          LET     TST_TYPE_USE="ML1C"                                                        
         ELSE                                                                                
          LET     PNL_MSG_MSG1="$ERR_DEF_MISMATCH_LOT_CASS"                                  
          GOTO    Error                                                                      
         END IF                                                                              
        END IF                                                                               
       ELSE                                                                                  
        LET       PRB_SLOT_TOTAL_ALL=PRB_SLOT_TOTAL_CASS*2                                   
        IF        TST_LOT_NUM="$SINGLE"                                                      
         LET      TST_TYPE_USE="1L2C"                                                        
        ELSE                                                                                 
         IF       TST_LOT_NUM="$DOUBLE"                                                      
          LET     TST_TYPE_USE="2L2C"                                                        
         ELSE                                                                                
          LET     TST_TYPE_USE="ML2C"                                                        
         END IF                                                                              
        END IF                                                                               
       END IF                                                                                
                                                                                             
       RETURN                                                                                
