$Type: Action
$Name: HP_SEL_SYSTEM_JOB
$Vers: 1
$Desc: Description for this Action spec.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---------- ------------ ---------------------------- - - -
 BODY                                                      
 Select_job IF           DSP_MAIN=1                        
            IF           UIF_MODIFY_CONFIG=1               
             PANEL       "HP_SEL_MAIN"                     
            ELSE                                           
             PANEL       "HP_SEL_MAIN_NO_CONFIG"           
            END IF                                         
                                                           
            IF           PANEL.RETURN="LOTTEST"            
             LET         RTN_SEL_SYSTEM_JOB="Test"         
            END IF                                         
            IF           PANEL.RETURN="UTILITY"            
             LET         RTN_SEL_SYSTEM_JOB="Utility"      
            END IF                                         
            IF           PANEL.RETURN="CONFIG"             
             LET         RTN_SEL_SYSTEM_JOB="Config"       
            END IF                                         
            IF           PANEL.RETURN="HALT"               
             TESTER_STOP                                   
             ACTION      "PLG_FWK_END"                     
             STOP                                          
            END IF                                         
            END IF                                         
$---------- ------------ ---------------------------- - - -

