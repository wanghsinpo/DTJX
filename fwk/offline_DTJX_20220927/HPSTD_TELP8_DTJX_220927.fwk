$Type: Action
$Name: HP_PROBER_BREAK_TELP8
$Vers: 1
$Desc: Set prober state pause and verify return value.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$----------- ------- --------------------------------- - - -
 BODY                                                       
             ACTION  "HP_PROBER_PRB_BREAK_TELP8"            
                                                            
             IF      RTN_PRB_PAUSE="Wafer_ready"            
              LET    PRB_ABORT_WAFER=1                      
              REJECT WAFER                                  
              RETURN                                        
             END IF                                         
             IF      RTN_PRB_PAUSE="Wafer_end"              
              LET    PRB_ABORT_WAFER=-1                     
              REJECT WAFER                                  
              RETURN                                        
             END IF                                         
             IF      RTN_PRB_PAUSE="Lot_end"                
              LET    PRB_ABORT_ALL=1                        
              REJECT LOT                                    
              RETURN                                        
             END IF                                         
             IF      RTN_PRB_PAUSE<>"Wafer_cont"            
              LET    PRB_ABORT_ALL=-1                       
              REJECT LOT                                    
              GOTO   Fatal_error                            
             END IF                                         
                                                            
             RETURN                                         
 Fatal_error                                                
             LET     PNL_TTL_LABEL="$TTL_PROBER_ERROR"      
             LET     PNL_MSG_MSG1="$TELP8_FATAL_ERROR"      
             LET     PNL_KEY_OK="$OK"                       
             PANEL   "HP_DISP_ERR1_OK",2                    
             PANEL   "HP_DISP_ERR1_OK",0                    
$----------- ------- --------------------------------- - - -

$Type: Action
$Name: HP_PROBER_CLEAN_TELP8
$Vers: 1
$Desc: Description for this Panel spec.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$-------------- -------- --------------------------------- - - -
 BODY                                                           
                LET      RTN_PROBER_CLEAN="Continue"            
 WarningMessage                                                 
                LET      PNL_TTL_LABEL="$TTL_TIPCLEAN"          
                LET      PNL_MSG_MSG1="$WARNING_TIPCLEAN"       
                LET      PNL_KEY_F1="$CONTINUE"                 
                LET      PNL_KEY_F4="$CANCEL"                   
                PANEL    "HP_DISP_MSG1_F1_F4",2                 
                PANEL    "HP_DISP_MSG1_F1_F4",0                 
                                                                
                IF       PANEL.RETURN="F1"                      
 Prober_clean                                                   
                 LET     PNL_MSG_MSG1="$EXEC_TIPCLEAN"          
                 PANEL   "HP_DISP_MSG1_NONE",2                  
                 ACTION  "HP_PROBER_PRB_CLEAN_TELP8"            
                 PANEL   "HP_DISP_MSG1_NONE",0                  
                 IF      RTN_PRB_CLEAN<>"Wafer_ready"           
                  LET    PRB_ABORT_ALL=-1                       
                  REJECT LOT                                    
                  GOTO   Fatal_error                            
                 END IF                                         
 CancelCleaning                                                 
                ELSE                                            
                 LET     RTN_PRB_CLEAN="Wafer_ready"            
#                LET     PNL_MSG_MSG1="$CANCEL_TIPCLEAN"        
#                PANEL   "HP_DISP_MSG1_NONE",2                  
#                WAIT    Wait=3                                 
#                PANEL   "HP_DISP_MSG1_NONE",0                  
                END IF                                          
                RETURN                                          
                                                                
 Fatal_error                                                    
                LET      PNL_TTL_LABEL="$TTL_PROBER_ERROR"      
                LET      PNL_MSG_MSG1="$TELP8_FATAL_ERROR"      
                LET      PNL_KEY_OK="$OK"                       
                PANEL    "HP_DISP_ERR1_OK",2                    
                PANEL    "HP_DISP_ERR1_OK",0                    
