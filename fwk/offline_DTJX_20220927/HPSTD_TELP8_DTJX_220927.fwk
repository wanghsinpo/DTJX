               IF      RTN_PRB_SET_FILE="Not_ready"               Returned STB=112(#CASSMAP_READY)
                LET    PNL_MSG_MSG1="$TELP8_SETUP_NOTREADY"                                       
                GOTO   Prober_error                                                               
               END IF                                                                             
                                                                                                  
              END IF                                                                              
 Get_cassmap                                                                                      
              ACTION   "HP_PROBER_PRB_GET_CASSMAP_TELP8"          Get cassette map from prober    
                                                                                                  
              IF       RTN_PRB_GET_CASSMAP="Prober_abort"                                         
               GOTO    Prober_fatal                                                               
              END IF                                                                              
               IF      RTN_PRB_GET_CASSMAP="Not_ready"            Returned STB=112(#CASSMAP_READY)
