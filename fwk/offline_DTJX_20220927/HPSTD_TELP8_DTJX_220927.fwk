$Type: Action
$Name: HP_PROBER_SEL_CONFIG_TELP8
$Vers: 1
$Desc: Description for this Action spec.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---------------- -------------- ----------------------------------------- - - -
 BODY                                                                           
                  LET            TMP_CASS_NUM[1]=PRB_CASS_NUM                   
                  LET            TMP_LOT_NUM[1]=TST_LOT_NUM                     
                  IF             PRB_RANDOM=1                                   
                   LET           TMP_PRB_RANDOM[1]="$ON"                        
                  ELSE                                                          
                   LET           TMP_PRB_RANDOM[1]="$OFF"                       
                  END IF                                                        
                  IF             PRB_REJECT=1                                   
                   LET           TMP_PRB_REJECT[1]="$ON"                        
                  ELSE                                                          
                   LET           TMP_PRB_REJECT[1]="$OFF"                       
                  END IF                                                        
                                                                                
                  IF             PRB_FILE_USE=1                                 
                   LET           TMP_PRB_FILE[1]="$ON"                          
                  ELSE                                                          
                   LET           TMP_PRB_FILE[1]="$OFF"                         
                  END IF                                                        
                                                                                
                  PANEL          "HP_DISP_BLANK",2                              
 Set_prbenv                                                                     
                  PANEL          "HP_PROBER_SET_TELP8",2                        
                                                                                
                  IF             PANEL.RETURN="CANCEL"                          
                   PANEL         "HP_PROBER_SET_TELP8",0                        
                   PANEL         "HP_DISP_BLANK",0                              
                   RETURN                                                       
                  END IF                                                        
                                                                                
                  IF             TMP_CASS_NUM[1]="$SINGLE"                      
                   LET           PRB_SLOT_TOTAL_ALL=PRB_SLOT_TOTAL_CASS         
                   IF            TMP_LOT_NUM[1]="$DOUBLE"                       
                    GOTO         Cas_lot_mismatch                               
                   ELSE                                                         
                    IF           TMP_LOT_NUM[1]="$SINGLE"                       
                     LET         TST_TYPE_USE="1L1C"                            
                    ELSE                                                        
                     LET         TST_TYPE_USE="ML1C"                            
                    END IF                                                      
                   END IF                                                       
                  ELSE                                                          
                   LET           PRB_SLOT_TOTAL_ALL=PRB_SLOT_TOTAL_CASS*2       
                   IF            TMP_LOT_NUM[1]="$DOUBLE"                       
                    LET          TST_TYPE_USE="2L2C"                            
                   ELSE                                                         
                    IF           TMP_LOT_NUM[1]="$SINGLE"                       
                     LET         TST_TYPE_USE="1L2C"                            
                    ELSE                                                        
                     LET         TST_TYPE_USE="ML2C"                            
                    END IF                                                      
                   END IF                                                       
                  END IF                                                        
                                                                                
                  LET            PRB_CASS_NUM=TMP_CASS_NUM[1]                   
                  LET            TST_LOT_NUM=TMP_LOT_NUM[1]                     
                                                                                
                  IF             TMP_PRB_RANDOM[1]="$ON"                        
                   LET           PRB_RANDOM=1                                   
                  ELSE                                                          
                   LET           PRB_RANDOM=0                                   
                  END IF                                                        
                  IF             TMP_PRB_REJECT[1]="$ON"                        
                   LET           PRB_REJECT=1                                   
                  ELSE                                                          
                   LET           PRB_REJECT=0                                   
                  END IF                                                        
                                                                                
                  IF             TMP_PRB_FILE[1]="$ON"                          
                   LET           PRB_FILE_USE=1                                 
                  ELSE                                                          
                   LET           PRB_FILE_USE=0                                 
                  END IF                                                        
                                                                                
                  IF             PRB_ONLINE=1                                   
                   PROBER_CONFIG Address=PRB_HPIB_ADRS                          
                  END IF                                                        
                                                                                
                  PANEL          "HP_PROBER_SET_TELP8",0                        
                  PANEL          "HP_DISP_BLANK",0                              
                  RETURN                                                        
 Cas_lot_mismatch                                                               
                  LET            PNL_TTL_LABEL="$TTL_CONFIG_PRB"                
                  LET            PNL_MSG_MSG1="$ERR_SET_MISMATCH_LOT_CASS"      
                  LET            PNL_KEY_OK="$OK"                               
                  PANEL          "HP_DISP_ERR1_OK",1                            
                  PANEL          "HP_DISP_ERR1_OK",0                            
                  GOTO           Set_prbenv                                     
