         LOG_PROBER Session=SYSTEM.SESSIONID,Message="PRB_LOAD_RND: PRB_QUERY_READY Error " + PROBER_READY.Error      
        END IF                                                                                                        
        RETURN                                                                                                        
       END IF                                                                                                         
       IF           PROBER_SLOAD.Error="#ALIGN_REJECT"                                                                
        LET         RTN_PRB_LOAD_RND="Align_reject"                                                                   
       END IF                                                                                                         
       IF           PROBER_SLOAD.Error="#ALIGN_ASSIST"                                                                
        LET         RTN_PRB_LOAD_RND="Align_assist"                                                                   
       END IF                                                                                                         
       IF           PROBER_SLOAD.Error="#NOWAFER"                                                                     
        LET         RTN_PRB_LOAD_RND="No_wafer"                                                                       
       END IF                                                                                                         
      END IF                                                                                                          
                                                                                                                      
      IF            PROBER_SLOAD.Status="END"                                                                         
       LET          RTN_PRB_LOAD_RND="Lot_end"                                                                        
      END IF                                                                                                          
                                                                                                                      
      IF            PROBER_SLOAD.Status="OK"                                                                          
       LET          RTN_PRB_LOAD_RND="Wafer_ready"                                                                    
      END IF                                                                                                          
                                                                                                                      
      IF            RTN_PRB_LOAD_RND="Prober_abort"                                                                   
       LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_LOAD_RND: PROBER_SLOAD Error " + PROBER_SLOAD.Error         
      END IF                                                                                                          
