$Type: Action
$Name: CTM_CARD_ID_LEAK
$Vers: 1
$Desc: CTM: Check probe card ID and leakage
$Date: 04/25/2020
$Time: 17:56:46
$User: wat
$---- ------------------- ---------------------------------------------------------------------------------------------------------------------------------------- - - -----------
 BODY                                                                                                                                                                             
      LET                 PNL_CARD_LEAK_INFO=""                                                                                                                        125        
#     LET                 CTM_PC_STOP_SEL=CTM_PC_STOP_TEST                                                                                                             201206,sami
      LET                 CTM_PC_STOP_SEL0=CTM_PC_STOP_SEL                                                                                                             201207,sami
      CTM_PCIL_ACCESS     Check=CTM_CHECK_PCID,Pcil_file=CTM_PCIL,Hi=CTM_PC_RM_HI,Lo=CTM_PC_RM_LO,Vf=CTM_PC_RM_VF,Dev=CTM_PC_DEV,Ip_file=CTM_RFID_IP                              
      LET                 CTM_PC_ID=CTM_PCIL_ACCESS.Pcard_type + CTM_PCIL_ACCESS.Pcard_id                                                                                         
                                                                                                                                                                                  
      IF                  CTM_PCIL_ACCESS.St="ERROR"                                                                                                                              
        LET               CTM_ERR_ID=6                                                                                                                                            
        ACTION            "CTM_ERR_HANDLE"                                                                                                                                        
        RETURN                                                                                                                                                                    
      ELSE                                                                                                                                                                        
        IF                CTM_ERR_ID=0                                                                                                                                            
          LET             PNL_MSG_MSG1=""                                                                                                                                         
        END IF                                                                                                                                                                    
      END IF                                                                                                                                                                      
                                                                                                                                                                                  
      IF                  (CTM_CHECK_PCLEAK=1) AND (TST_TESTER_MODE=1)                                                                                                            
        FILE_EXIST        CTM_ROOT_DIR + CTM_RCP_DIR +"/PRB/"+CTM_PCIL_ACCESS.Pcard_spec+".prb"                                                                                   
        IF                FILE_EXIST.Status="OK"                                                                                                                                  
          CTM_PC_LEAK     Vbias=CTM_PC_LEAK_VOL,Ifail=CTM_PC_LEAK_CRI,Pcard_type=CTM_PCIL_ACCESS.Pcard_spec,Pcard_spec_dir=CTM_ROOT_DIR+ CTM_RCP_DIR +"/PRB",Wt=.3                
            IF            CTM_PC_LEAK.Status="LEAK_PASS"                                                                                                                          
              LET         PNL_CARD_LEAK_INFO="Probe card leakage check Pass"                                                                                                      
            END IF                                                                                                                                                                
