$Type: Action
$Name: MODULE_BEGIN
$Vers: 1
$Desc: ACTION called before each module-test.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------- -------------- -------------------------------------------------------------------------------------------------------- - - -----
 BODY                                                                                                                                           
               LET            TST_WAFER_ENABLE_PAUSE=1                                                                                          
 Exec_hook                                                                                                                                      
               ACTION         "PLG_MODULE_BEGIN"                                                                                                
 Set_tag                                                                                                                                        
               LET            TST_MOD_LAST=MODULE.DEFINELIST[MODULE.NUMBER]                                                                     
               LET            TST_MODULE_DONE=TST_MODULE_DONE+1                                                                                 
               LET            TST_REJECT_USER=0                                                                                                 
 Update_result                                                                                                                                  
               CHECK_TIME                                                                                                                  Timer
               LET            TIM_MODULE_BEGIN=CHECK_TIME.Time                                                                                  
               IF             TST_MODULE_DONE>0                                                                                                 
                LET           TIM_MODULE=(TIM_MODULE_BEGIN-TIM_DIE_BEGIN)/TST_MODULE_DONE                                                       
               ELSE                                                                                                                             
                LET           TIM_MODULE=0                                                                                                      
               END IF                                                                                                                           
               STRING_TIME    Time=TIM_MODULE_BEGIN-TIM_ALL_BEGIN                                                                               
               LET            PNL_TEST_TIME_SPEND=STRING_TIME.Symbol                                                                            
               IF             TST_WAFER_DONE>0                                                                                                  
                LET           TIM_REMAIN=TIM_WAFER*TST_WAFER_NUM-TIM_DIE*TST_DIE_DONE-TIM_MODULE*TST_MODULE_DONE                                
                IF            TIM_REMAIN>0                                                                                                      
                 STRING_TIME  Time=TIM_REMAIN                                                                                                   
                 LET          PNL_TEST_TIME_REMAIN=STRING_TIME.Symbol                                                                           
                END IF                                                                                                                          
               ELSE                                                                                                                             
                IF            TST_DIE_DONE>0                                                                                                    
                 LET          TIM_REMAIN=TIM_DIE*DIE.TOTAL*DIE.TYPETOTAL*TST_WAFER_NUM-TIM_DIE*TST_DIE_DONE-TIM_MODULE*TST_MODULE_DONE          
                 IF           TIM_REMAIN>0                                                                                                      
                  STRING_TIME Time=TIM_REMAIN                                                                                                   
                  LET         PNL_TEST_TIME_REMAIN=STRING_TIME.Symbol                                                                           
                 END IF                                                                                                                         
                ELSE                                                                                                                            
                 LET          TIM_REMAIN=TIM_MODULE*MODULE.TOTAL*DIE.TOTAL*DIE.TYPETOTAL*TST_WAFER_NUM-TIM_MODULE*TST_MODULE_DONE               
                 IF           TIM_REMAIN>0                                                                                                      
                  STRING_TIME Time=TIM_REMAIN                                                                                                   
                  LET         PNL_TEST_TIME_REMAIN=STRING_TIME.Symbol                                                                           
                 END IF                                                                                                                         
                END IF                                                                                                                          
               END IF                                                                                                                           
                                                                                                                                                
               UPDATE         "HP_DISP_TEST_RUN",UPD_TEST_MODULE_BEGIN                                                                          
 Move_chuck                                                                                                                                     
               ACTION         "HP_PROBER_MOVE_" + PRB_MODEL                                                                                     
 Break_test                                                                                                                                     
               IF             TST_FLW_DETAILED=1                                                                                                
                IF            TST_BRK_AT_MODULEBEGIN                                                                                            
                 GET_BRK_MOD  Typeindex=DIE.TYPENUMBER,Dieindex=DIE.NUMBER,Modindex=MODULE.NUMBER                                               
                 IF           GET_BRK_MOD.Break=1                                                                                               
                  LET         PNL_TTL_LABEL="$TTL_BREAK_ON_POINT"                                                                               
                  IF          UIF_PRB_BZZR_ON_BREAK=1                                                                                           
                   LET        PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_BREAK_BZZR"                                                             
                   ACTION     "HP_PROBER_WAIT_CONT"                                                                                             
                   IF         RTN_PRB_WAIT_CONT<>"Wafer_cont"                                                                                   
                    RETURN                                                                                                                      
                   END IF                                                                                                                       
                  ELSE                                                                                                                          
                   LET        PNL_KEY_OK="$OK"                                                                                                  
                   LET        PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_BREAK"                                                                  
                   ACTION     "HP_PROBER_WAIT_NONE"                                                                                             
                  END IF                                                                                                                        
                  LET         PNL_TEST_PROG_STOP="$TEST_BREAK_MODBGN"                                                                           
                  ACTION      "PLG_BREAK_MODULE_BEGIN"                                                                                          
                 END IF                                                                                                                         
                END IF                                                                                                                          
               END IF                                                                                                                           
$------------- -------------- -------------------------------------------------------------------------------------------------------- - - -----

$Type: Action
$Name: MODULE_END
$Vers: 1
$Desc: ACTION called after each module-test.\n  SPECS2.5: Added auto-probing function on failure.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------ ------------- ------------------------------------------------------------------- - - -
 BODY                                                                                                
 Exec_hook                                                                                           
              ACTION        "PLG_MODULE_END"                                                         
              IF            SYSTEM.RETEST="MODULE"                                                   
               RETURN                                                                                
              END IF                                                                                 
 Update_panel                                                                                        
              UPDATE        "HP_DISP_TEST_RUN",UPD_TEST_MODULE_END                                   
 Judge_mod                                                                                           
              IF            TST_FLW_DETAILED=1                                                       
               IF           JDG_MODULE_RUNTIME=1                                                     
                ACTION      "HP_JDG_MODULE_AT_MODULE"                                                
                IF          RTN_JDG_MODULE_AT_MODULE="Fail"                                          
                 LET        PNL_TTL_LABEL="$TTL_BREAK_ON_FAIL"                                       
                 IF         UIF_PRB_BZZR_ON_MODULE_FAIL=1                                            
                  LET       PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_FAIL_BZZR"                     
                  ACTION    "HP_PROBER_WAIT_CONT"                                                    
                  IF        RTN_PRB_WAIT_CONT<>"Wafer_cont"                                          
                   RETURN                                                                            
                  END IF                                                                             
                 ELSE                                                                                
                  LET       PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_FAIL"                          
                  LET       PNL_KEY_OK="$OK"                                                         
                  ACTION    "HP_PROBER_WAIT_NONE"                                                    
                 END IF                                                                              
                 LET        PNL_TEST_PROG_STOP="$TEST_BREAK_MODFIL"                                  
                 ACTION     "PLG_BREAK_MODULE_FAIL"                                                  
                 GOTO       Judge_die                                                                
                END IF                                                                               
               END IF                                                                                
              END IF                                                                                 
 Break_test                                                                                          
              IF            TST_FLW_DETAILED=1                                                       
               IF           TST_BRK_AT_MODULEEND                                                     
                GET_BRK_MOD Typeindex=DIE.TYPENUMBER,Dieindex=DIE.NUMBER,Modindex=MODULE.NUMBER      
                IF          GET_BRK_MOD.Break=1                                                      
                 IF         SYSTEM.REJECT=""                                                         
                  IF        SYSTEM.RETEST=""                                                         
                   LET      PNL_TTL_LABEL="$TTL_BREAK_ON_POINT"                                      
                   IF       UIF_PRB_BZZR_ON_BREAK=1                                                  
                    LET     PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_BREAK_BZZR"                    
                    ACTION  "HP_PROBER_WAIT_CONT"                                                    
                    IF      RTN_PRB_WAIT_CONT<>"Wafer_cont"                                          
                     RETURN                                                                          
                    END IF                                                                           
                   ELSE                                                                              
                    LET     PNL_KEY_OK="$OK"                                                         
                    LET     PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_BREAK"                         
                    ACTION  "HP_PROBER_WAIT_NONE"                                                    
                   END IF                                                                            
                   LET      PNL_TEST_PROG_STOP="$TEST_BREAK_MODEND"                                  
                   ACTION   "PLG_BREAK_MODULE_END"                                                   
                  END IF                                                                             
                 END IF                                                                              
                END IF                                                                               
               END IF                                                                                
              END IF                                                                                 
 Judge_die                                                                                           
              IF            DIE.JDG_RUNTIME=1                                                        
               ACTION       "HP_JDG_DIE_AT_MODULE"                                                   
               IF           RTN_JDG_DIE_AT_MODULE="Fail"                                             
                GOTO        Die_fail                                                                 
               END IF                                                                                
              END IF                                                                                 
              RETURN                                                                                 
 Die_fail                                                                                            
              IF            JDG_DIE_TYPE="$JDG_DIE_REJECT"                                           
               LET          TST_WAFER_ENABLE_PAUSE=0                                                 
               REJECT       DIE                                                                      
               RETURN                                                                                
              END IF                                                                                 
                                                                                                     
              IF            PRB_ABORT_ALL<>0                                                         
               RETURN                                                                                
              END IF                                                                                 
              IF            PRB_ABORT_WAFER<>0                                                       
