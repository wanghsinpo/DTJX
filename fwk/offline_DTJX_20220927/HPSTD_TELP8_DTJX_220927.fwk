$Type: Action
$Name: HP_SET_JDG_WAFER
$Vers: 1
$Desc: Return wafer level pass/fail string.\n  In : TMP_SLOT_POS (slot position)\n  Out: TMP_SLOT_JDG (judgement string)\n\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- ------- ------------------------------------------ - - -
 BODY                                                         
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$PASS"       
       LET    TMP_SLOT_JDG="Pass"                             
       RETURN                                                 
      END IF                                                  
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$FAIL"       
       LET    TMP_SLOT_JDG="Fail"                             
       RETURN                                                 
      END IF                                                  
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$YET"        
       LET    TMP_SLOT_JDG=""                                 
       RETURN                                                 
      END IF                                                  
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$SKIP"       
       LET    TMP_SLOT_JDG=""                                 
       RETURN                                                 
      END IF                                                  
      IF      PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$ALIGN"      
       LET    TMP_SLOT_JDG="Alignment"                        
       RETURN                                                 
      END IF                                                  
$---- ------- ------------------------------------------ - - -

$Type: Action
$Name: HP_SET_MODE_DISP
$Vers: 1
$Desc: Set string of mode display in title bar.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- ------- ----------------------------------------- - - -
 BODY                                                        
      IF      TST_FLW_DETAILED=1                             
       LET    PNL_MAIN_MODE_MSG="$PNL_MAIN_MODE_MSG_DC"      
       LET    PNL_MAIN_MODE_COLOR=PNL_FG_RED                 
      ELSE                                                   
       IF     TST_FLW_DETAILED=0                             
        LET   PNL_MAIN_MODE_MSG="$PNL_MAIN_MODE_MSG_HT"      
        LET   PNL_MAIN_MODE_COLOR=PNL_FG_BLUEGREEN           
       ELSE                                                  
        LET   PNL_MAIN_MODE_MSG=""                           
        LET   PNL_MAIN_MODE_COLOR=""                         
       END IF                                                
      END IF                                                 
$---- ------- ----------------------------------------- - - -

$Type: Action
$Name: HP_SET_MODE_DISP_INIT
