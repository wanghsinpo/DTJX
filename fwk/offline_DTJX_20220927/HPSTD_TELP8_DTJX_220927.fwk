                 END IF                                                                                                                           
                END IF                                                                                                                            
                                                                                                                                                  
                IF            RTN_PRB_REALIGN="Prober_abort"                                                                                      
                 LOG_PROBER   Session=SYSTEM.SESSIONID,Message="PRB_LOAD_PPL: PROBER_LOAD Error " + PROBER_LOAD.Error                             
                END IF                                                                                                                            
                                                                                                                                                  
               RETURN                                                                                                                             
