$Desc: JOY: ask prober to load the image file record in PIL file
$Date: 01/21/2002
$Time: 09:41:06
$User: joy
$---- ---------------- -------------------------------------------------------------------------------------------- - - --------------------------------------------------------------------------
 BODY                                                                                                                                                                                             
      LET              RTN_PRB_SET_FILE="File_ready"                                                                    Initialize return parameter                                               
      IF               PRB_ONLINE=0                                                                                                                                                               
       RETURN                                                                                                                                                                                     
      END IF                                                                                                                                                                                      
                                                                                                                                                                                                  
      ACTION           "HP_PROBER_PRB_QUERY_READY_TELP8"                                                                                                                                          
      IF               RTN_PRB_QUERY_READY="Cassmap_ready"                                                              STB=112(#CASSMAP_READY) is returned instead of STB=82(#SETUP_READY)       
       LET             PNL_TTL_LABEL="$TTL_CONF_CONT"                                                                                                                                             
       LET             PNL_MSG_MSG1="$TELP8_WARNING_GPIB_MISSMATCH"                                                                                                                               
       LET             PNL_KEY_F1="$CONTINUE"                                                                                                                                                     
       LET             PNL_KEY_F4="$RESTART"                                                                                                                                                      
       PANEL           "HP_DISP_MSG1_F1_F4",2                                                                                                                                                     
       PANEL           "HP_DISP_MSG1_F1_F4",0                                                                                                                                                     
                                                                                                                                                                                                  
       IF              PANEL.RETURN="F4"                                                                                                                                                          
        LET            RTN_PRB_SET_FILE="Not_ready"                                                                     It's caused by the missmatch of GPIB setting between prober and framework 
        PROBER_CASSMAP                                                                                                  To restore the prober control (enable prober to accept any other command) 
        LOG_PROBER     Session=SYSTEM.SESSIONID,Message="PRB_SET_FILE: PRB_QUERY_READY Error " + PROBER_READY.Error     (It's handled as a kind of error, to be corrected and restarted.)         
       ELSE                                                                                                                                                                                       
        LET            RTN_PRB_SET_FILE="File_ready"                                                                    Accept the product file currently set on the prober (Just a warning level)
        LET            PNL_MSG_MSG1="$TELP8_ACCEPT_CURRFILE"                                                                                                                                      
        PANEL          "HP_DISP_MSG1_NONE",2                                                                                                                                                      
        WAIT           Wait=3                                                                                                                                                                     
        PANEL          "HP_DISP_MSG1_NONE",0                                                                                                                                                      
       END IF                                                                                                                                                                                     
       RETURN                                                                                                                                                                                     
      END IF                                                                                                                                                                                      
                                                                                                                                                                                                  
      IF               RTN_PRB_QUERY_READY<>"File_ready"                                                                                                                                          
       LET             RTN_PRB_SET_FILE="Prober_abort"                                                                                                                                            
       LOG_PROBER      Session=SYSTEM.SESSIONID,Message="PRB_SET_FILE: PRB_QUERY_READY Error " + PROBER_READY.Error                                                                               
       RETURN                                                                                                                                                                                     
      END IF                                                                                                                                                                                      
                                                                                                                                                                                                  
      LET              PNL_MSG_MSG1="$EXEC_PRBFILE_SETUP"                                                                                                                                         
      LET              PNL_TTL_LABEL="$TTL_PRBFILE_SETUP"                                                                                                                                         
      PANEL            "HP_DISP_MSG1_NONE",2                                                                                                                                                      
      PROBER_SETUP     Drive=PRB_FILE_DRIVE,Filename=PRB_FILE_NAME                                                                                                                                
      PANEL            "HP_DISP_MSG1_NONE",0                                                                                                                                                      
                                                                                                                                                                                                  
      LET              RTN_PRB_SET_FILE="Prober_abort"                                                                                                                                            
                                                                                                                                                                                                  
      IF               PROBER_SETUP.Status="OK"                                                                                                                                                   
       LET             RTN_PRB_SET_FILE="File_ready"                                                                                                                                              
      END IF                                                                                                                                                                                      
      IF               PROBER_SETUP.Status="ERROR"                                                                                                                                                
       IF              PROBER_SETUP.Error="#NOFILE"                                                                                                                                               
        LET            RTN_PRB_SET_FILE="No_file"                                                                                                                                                 
       END IF                                                                                                                                                                                     
      END IF                                                                                                                                                                                      
                                                                                                                                                                                                  
      IF               RTN_PRB_SET_FILE="Prober_abort"                                                                                                                                            
       LOG_PROBER      Session=SYSTEM.SESSIONID,Message="PRB_SET_FILE: PROBER_SETUP Error " + PROBER_SETUP.Error                                                                                  
      END IF                                                                                                                                                                                      
