$Type: Action
$Name: HP_SEL_CONFIG_REPLACE
$Vers: 1
$Desc: Replace Button callback for system configuration panel.\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- -------------- ------------------------------------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                                     
      IF             FIL_AUTHORIZE=1                                                                                                                      
       LET           PNL_TTL_LABEL="$TTL_CONFIG"                                                                                                          
       LET           PNL_MSG_MSG1="$BE_SU_FOR_REPLACE_CONFIG"                                                                                             
       PANEL         "HP_DISP_MSG1_NONE",1                                                                                                                
       CHECK_PASSWD  Name="root"                                                                                                                          
       IF            CHECK_PASSWD.Result=0                                                                                                                
        PANEL        "HP_DISP_MSG1_NONE",0                                                                                                                
        LET          RTN_DISP_SEL_CONFIG=0                                                                                                                
        LET          RTN_DISP_SEL_CONFIG_REDRAW=0                                                                                                         
        RETURN                                                                                                                                            
       END IF                                                                                                                                             
      ELSE                                                                                                                                                
       VERIFY_FILE   Filename=FIL_CUSTOMIZE,Type="Normal"                                                                                                 
       IF            VERIFY_FILE.Writable=0                                                                                                               
        LET          RTN_DISP_SEL_CONFIG=0                                                                                                                
        LET          RTN_DISP_SEL_CONFIG_REDRAW=0                                                                                                         
        RETURN                                                                                                                                            
       END IF                                                                                                                                             
       VERIFY_FILE   Filename=FIL_PNLCONF,Type="Normal"                                                                                                   
       IF            VERIFY_FILE.Writable=0                                                                                                               
        LET          RTN_DISP_SEL_CONFIG=0                                                                                                                
        LET          RTN_DISP_SEL_CONFIG_REDRAW=0                                                                                                         
        RETURN                                                                                                                                            
       END IF                                                                                                                                             
      END IF                                                                                                                                              
                                                                                                                                                          
      ATTRIB_FILE    Filename=FIL_CUSTOMIZE                                                                                                               
      LET            FIL_PERM_CUSTOMIZE=ATTRIB_FILE.Permission                                                                                            
      LET            FIL_OWNER_CUSTOMIZE=ATTRIB_FILE.Owner                                                                                                
      LET            FIL_GROUP_CUSTOMIZE=ATTRIB_FILE.Group                                                                                                
                                                                                                                                                          
      ATTRIB_FILE    Filename=FIL_PNLCONF                                                                                                                 
      LET            FIL_PERM_PNLCONF=ATTRIB_FILE.Permission                                                                                              
      LET            FIL_OWNER_PNLCONF=ATTRIB_FILE.Owner                                                                                                  
      LET            FIL_GROUP_PNLCONF=ATTRIB_FILE.Group                                                                                                  
                                                                                                                                                          
      UPDATE_FWKCONF File=FIL_CUSTOMIZE,Type=1                                                                                                            
                                                                                                                                                          
      IF             UPDATE_FWKCONF.Errno<>0                                                                                                              
       LET           PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                                                    
       LET           PNL_MSG_MSG1="$ERR_UPDATE_CUSTOMIZE"                                                                                                 
       LET           PNL_MSG_MSG2=UPDATE_FWKCONF.Errmsg                                                                                                   
       LET           PNL_KEY_OK="$HALT"                                                                                                                   
       PANEL         "HP_DISP_ERR2_OK",1                                                                                                                  
       PANEL         "HP_DISP_ERR2_OK",0                                                                                                                  
       LOG_SYSTEM    Session=SYSTEM.SESSIONID,Message="HP_SEL_CONFIG_JOB: UPDATE_FWKCONF Error " + UPDATE_FWKCONF.Errmsg + "(" + FIL_CUSTOMIZE + ")"      
       TESTER_STOP                                                                                                                                        
       ACTION        "PLG_FWK_END"                                                                                                                        
       STOP                                                                                                                                               
      END IF                                                                                                                                              
                                                                                                                                                          
      EXECUTE        "/usr/bin/chmod " + FIL_PERM_CUSTOMIZE + " " + FIL_CUSTOMIZE                                                                         
      EXECUTE        "/usr/bin/chown " + FIL_OWNER_CUSTOMIZE + ":" + FIL_GROUP_CUSTOMIZE + " " + FIL_CUSTOMIZE                                            
                                                                                                                                                          
      UPDATE_FWKCONF File=FIL_PNLCONF,Type=1                                                                                                              
                                                                                                                                                          
      IF             UPDATE_FWKCONF.Errno<>0                                                                                                              
       LET           PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                                                    
       LET           PNL_MSG_MSG1="$ERR_UPDATE_PNLCONF"                                                                                                   
       LET           PNL_MSG_MSG2=UPDATE_FWKCONF.Errmsg                                                                                                   
       LET           PNL_KEY_OK="$HALT"                                                                                                                   
       PANEL         "HP_DISP_ERR2_OK",1                                                                                                                  
       PANEL         "HP_DISP_ERR2_OK",0                                                                                                                  
       LOG_SYSTEM    Session=SYSTEM.SESSIONID,Message="HP_SEL_CONFIG_JOB: UPDATE_FWKCONF Error " + UPDATE_FWKCONF.Errmsg + "(" + FIL_PNLCONF + ")"        
       TESTER_STOP                                                                                                                                        
       ACTION        "PLG_FWK_END"                                                                                                                        
       STOP                                                                                                                                               
      END IF                                                                                                                                              
                                                                                                                                                          
      EXECUTE        "/usr/bin/chmod " + FIL_PERM_PNLCONF + " " + FIL_PNLCONF                                                                             
      EXECUTE        "/usr/bin/chown " + FIL_OWNER_PNLCONF + ":" + FIL_GROUP_PNLCONF + " " + FIL_PNLCONF                                                  
                                                                                                                                                          
      PANEL          "HP_DISP_MSG1_NONE",0                                                                                                                
      LET            RTN_DISP_SEL_CONFIG=0                                                                                                                
      LET            RTN_DISP_SEL_CONFIG_REDRAW=0                                                                                                         
