$Type: Action
$Name: PLG_WAFER_END
$Vers: 1
$Desc: JOY:to count the probe-contact times for each wafer loop
$Date: 04/25/2020
$Time: 17:57:45
$User: wat
$---- --------------- ---------------------------------------------------------------------------------------------------------------------------------------- - - ----------------------------------------------------------------
 BODY                                                                                                                                                                                                                              
      IF              CTM_DATAOUT_PER_WAFER==1                                                                                                                     201211,sami: add                                                
        WRITE_INTEGER Value=TST_SLOT_CURRENT                                                                                                                                                                                       
        DATADUMP2     SYS_DATADIR+"/"+TST_RCP_LOTID+"/",WRITE_INTEGER.String,1                                                                                                                                                     
      END IF                                                                                                                                                       201211,sami: add                                                
                                                                                                                                                                                                                                   
      IF              TST_TESTER_MODE=1                                                                                                                                                                                            
       IF             CTM_PC_COUNT=1                                                                                                                                                                                               
        CTM_PC_COUNT  Prb_adr=PRB_HPIB_ADRS                                                                                                                                                                                        
        LET           TMP[2]=CTM_PC_COUNT.Count                                                                                                                                                                                    
        LET           CTM_CONTACT=TMP[2]-TMP[1]                                                                                                                                                                                    
        CTM_PC_MGR    Pcil_mgr=CTM_PCIL_MGR,Pcard_type=CTM_PCIL_ACCESS.Pcard_type,Pcard_id=CTM_PCIL_ACCESS.Pcard_id,Count=CTM_CONTACT,Leak_pin=CTM_PC_LEAK.Msg     20200425, JL fix Pcard.spec to pcard.type for matching Card type
        IF            (CTM_PC_STOP_SEL0=1) AND (CTM_PC_MGR.St="1")                                                                                                 201206,sami                                                     
         LET          PNL_TTL_LABEL="ProbeCardAlarm"                                                                                                                                                                               
         LET          PNL_MSG_MSG1=CTM_PC_MGR.Err+"\n\nPlease press CONTINUE to continue the test!\n\nOr Press QUIT then OPTION to abort this test"                                                                                
         LET          PNL_CARD_ERR2_CONT="CONTINUE"                                                                                                                                                                                
         LET          PNL_CARD_ERR2_QUIT="QUIT"                                                                                                                                                                                    
         PANEL        "CTM_DISP_ERR2_OK",2                                                                                                                                                                                         
         PANEL        "CTM_DISP_ERR2_OK",0                                                                                                                                                                                         
          IF          PANEL.RETURN="CONTINUE"                                                                                                                                                                                      
           LET        CTM_PC_STOP_SEL0=0                                                                                                                                                                                           
          ELSE                                                                                                                                                                                                                     
           ACTION     "HP_TEST_BREAK_MENU"                                                                                                                                                                                         
          END IF                                                                                                                                                                                                                   
        END IF                                                                                                                                                     201206,sami                                                     
       END IF                                                                                                                                                                                                                      
      END IF                                                                                                                                                                                                                       
                                                                                                                                                                                                                                   
      SET_GUARD_OPEN  Yes=1                                                                                                                                        Paul 2013/08/30  set guard to be floating                       
