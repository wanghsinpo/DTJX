$Type: Action
$Name: HP_TEST_SET_RECIPE_MULTI_G8
$Vers: 1
$Desc: Set test recipe (multi lot).\n  SPECS2.5: Set default wafer ID.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------- ----------------- ------------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                                   
 Check_tpl                                                                                                                              
               IF                SYSTEM.TESTPLAN<>""                                                                                    
                LET              PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                      
                LET              PNL_MSG_MSG1="$ERR_TESTPLAN_LOADED"                                                                    
                LET              PNL_KEY_OK="$OK"                                                                                       
                PANEL            "HP_DISP_ERR1_OK"                                                                                      
                PANEL            "HP_DISP_ERR1_OK",0                                                                                    
                LET              RTN_TEST_SET_RECIPE="Main"                                                                             
                RETURN                                                                                                                  
               END IF                                                                                                                   
 Init_tag                                                                                                                               
               SET_TAG_INTEGER   "TMP_CASS_SEL",-3                                                                                      
               SET_TAG_STRING    "TMP_CASS_BODY",""                                                                                     
               SET_TAG_INTEGER   "TST_SLOT_SELECT",0                                                                                    
               SET_TAG_STRING    "TST_SLOT_WAFERID",""                                                                                  
               SET_TAG_STRING    "TMP_SLOT_LOTID",""                                                                                    
               SET_TAG_STRING    "TMP_SLOT_PROCESS",""                                                                                  
               SET_TAG_STRING    "TMP_SLOT_TPL",""                                                                                      
               SET_TAG_STRING    "TMP_SLOT_LIMIT",""                                                                                    
               LET               TST_RCP_LOTID=""                                                                                       
               LET               TST_RCP_PROCESS=""                                                                                     
               LET               TST_RCP_TPL=""                                                                                         
               LET               TST_RCP_LIMIT=""                                                                                       
               LET               TMP_I=26                                                                                               
               WHILE             TMP_I<=30                                                                                              
                LET              TMP_CASS_SEL[TMP_I]=-4                                                                                 
                LET              TMP_I=TMP_I+1                                                                                          
               END WHILE                                                                                                                
               LET               TMP_I=56                                                                                               
               WHILE             TMP_I<=60                                                                                              
                LET              TMP_CASS_SEL[TMP_I]=-4                                                                                 
                LET              TMP_I=TMP_I+1                                                                                          
               END WHILE                                                                                                                
               LET               TMP_I=1                                                                                                
               WHILE             TMP_I<=25                                                                                              
                LET              TMP_CASS_BODY[TMP_I][1]=TST_SLOT_WAFERID[TMP_I]                                                        
                LET              TMP_CASS_BODY[TMP_I+30][1]=TST_SLOT_WAFERID[TMP_I+25]                                                  
                LET              TMP_I=TMP_I+1                                                                                          
               END WHILE                                                                                                                
 Enter_recipe                                                                                                                           
               IF                PRB_CASS_NUM="$SINGLE"                                                                                 
                PANEL            "HP_SET_RECIPE_ML1C_G8"                                                                                
               ELSE                                                                                                                     
                PANEL            "HP_SET_RECIPE_ML2C_G8"                                                                                
               END IF                                                                                                                   
               PANEL             "",0                                                                                                   
                                                                                                                                        
               IF                PANEL.RETURN="MAIN"                                                                                    
                LET              RTN_TEST_SET_RECIPE="Main"                                                                             
                RETURN                                                                                                                  
               END IF                                                                                                                   
 Verify_recipe                                                                                                                          
               VERIFY_FILE       TST_RCP_TPL,"Normal"                                                                                   
               IF                VERIFY_FILE.Readable=0                                                                                 
                LET              PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                      
                LET              PNL_MSG_MSG1="$ERR_TPL_NOT_READABLE"                                                                   
                LET              PNL_KEY_OK="$RETRY"                                                                                    
                PANEL            "HP_DISP_ERR1_OK"                                                                                      
                PANEL            "HP_DISP_ERR1_OK",0                                                                                    
                GOTO             Enter_recipe                                                                                           
               END IF                                                                                                                   
                                                                                                                                        
               IF                TST_RCP_LIMIT<>""                                                                                      
                VERIFY_FILE      TST_RCP_LIMIT,"Normal"                                                                                 
                IF               VERIFY_FILE.Readable=0                                                                                 
                 LET             PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                      
                 LET             PNL_MSG_MSG1="$ERR_LIMIT_NOT_READABLE"                                                                 
                 LET             PNL_KEY_OK="$RETRY"                                                                                    
                 PANEL           "HP_DISP_ERR1_OK"                                                                                      
                 PANEL           "HP_DISP_ERR1_OK",0                                                                                    
                 GOTO            Enter_recipe                                                                                           
                END IF                                                                                                                  
               END IF                                                                                                                   
 CheckTPLvsLim                                                                                                                          
               ACTION            "PLG_TEST_CHECK_FILE"                                                                                  
               IF                RTN_TEST_CHKFILE<>"OK"                                                                                 
                GOTO             Enter_recipe                                                                                           
               END IF                                                                                                                   
                                                                                                                                        
               LET               TST_DATA_DIR=""                                                                                        
               LET               TST_DATA_NAME=""                                                                                       
               ACTION            "PLG_NAME_DATAFILE"                                                                                    
               IF                RTN_PLG_NAME_DATAFILE="Error"                                                                          
                GOTO             Enter_recipe                                                                                           
               END IF                                                                                                                   
 Set_slot                                                                                                                               
               LET               TMP_SLOT_POS=1                                                                                         
               REPEAT                                                                                                                   
                IF               TMP_CASS_SEL[TMP_SLOT_POS]>0                                                                           
                 LET             TST_SLOT_SELECT[TMP_SLOT_POS]=1                                                                        
                 LET             TST_SLOT_WAFERID[TMP_SLOT_POS]=TMP_CASS_BODY[TMP_SLOT_POS][1]                                          
                 IF              TST_SLOT_WAFERID[TMP_SLOT_POS]=""                                                                      
                  WRITE_INTEGER  Value=TMP_SLOT_POS                                                                                     
                  LET            TST_SLOT_WAFERID[TMP_SLOT_POS]=WRITE_INTEGER.String                                                    
                 END IF                                                                                                                 
                 LET             TMP_SLOT_LOTID[TMP_SLOT_POS]=TST_RCP_LOTID                                                             
                 LET             TMP_SLOT_PROCESS[TMP_SLOT_POS]=TST_RCP_PROCESS                                                         
                 STRING_LOOKUP   Src=TST_RCP_TPL,Pat=".tpl"                                                                             
                 WRITE_INTEGER   Format="%d",Value=STRING_LOOKUP.Pos-1                                                                  
                 WRITE_STRING    Format="%."+WRITE_INTEGER.String+"s",Value=TST_RCP_TPL                                                 
                 LET             TMP_SLOT_TPL[TMP_SLOT_POS]=WRITE_STRING.String                                                         
                 IF              TST_RCP_LIMIT<>""                                                                                      
                  STRING_LOOKUP  Src=TST_RCP_LIMIT,Pat=".lim"                                                                           
                  WRITE_INTEGER  Format="%d",Value=STRING_LOOKUP.Pos-1                                                                  
                  WRITE_STRING   Format="%."+WRITE_INTEGER.String+"s",Value=TST_RCP_LIMIT                                               
                  LET            TMP_SLOT_LIMIT[TMP_SLOT_POS]=WRITE_STRING.String                                                       
                 ELSE                                                                                                                   
                  LET            TMP_SLOT_LIMIT[TMP_SLOT_POS]=TST_RCP_LIMIT                                                             
                 END IF                                                                                                                 
                 LET             TMP_CASS_SEL[TMP_SLOT_POS]=-4                                                                          
                 LET             TMP_CASS_BODY[TMP_SLOT_POS][1]="FILLED"                                                                
                END IF                                                                                                                  
                LET              TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
               UNTIL             TMP_SLOT_POS>25                                                                                        
                                                                                                                                        
               IF                PRB_CASS_NUM="$DOUBLE"                                                                                 
                REPEAT                                                                                                                  
                 IF              TMP_CASS_SEL[TMP_SLOT_POS+5]>0                                                                         
                  LET            TST_SLOT_SELECT[TMP_SLOT_POS]=1                                                                        
                  LET            TST_SLOT_WAFERID[TMP_SLOT_POS]=TMP_CASS_BODY[TMP_SLOT_POS+5][1]                                        
                  IF             TST_SLOT_WAFERID[TMP_SLOT_POS]=""                                                                      
                   WRITE_INTEGER Value=TMP_SLOT_POS                                                                                     
                   LET           TST_SLOT_WAFERID[TMP_SLOT_POS]=WRITE_INTEGER.String                                                    
                  END IF                                                                                                                
                  LET            TMP_SLOT_LOTID[TMP_SLOT_POS]=TST_RCP_LOTID                                                             
                  LET            TMP_SLOT_PROCESS[TMP_SLOT_POS]=TST_RCP_PROCESS                                                         
                  STRING_LOOKUP  Src=TST_RCP_TPL,Pat=".tpl"                                                                             
                  WRITE_INTEGER  Format="%d",Value=STRING_LOOKUP.Pos-1                                                                  
                  WRITE_STRING   Format="%."+WRITE_INTEGER.String+"s",Value=TST_RCP_TPL                                                 
                  LET            TMP_SLOT_TPL[TMP_SLOT_POS]=WRITE_STRING.String                                                         
                  IF             TST_RCP_LIMIT<>""                                                                                      
                   STRING_LOOKUP Src=TST_RCP_LIMIT,Pat=".lim"                                                                           
                   WRITE_INTEGER Format="%d",Value=STRING_LOOKUP.Pos-1                                                                  
                   WRITE_STRING  Format="%."+WRITE_INTEGER.String+"s",Value=TST_RCP_LIMIT                                               
                   LET           TMP_SLOT_LIMIT[TMP_SLOT_POS]=WRITE_STRING.String                                                       
                  ELSE                                                                                                                  
                   LET           TMP_SLOT_LIMIT[TMP_SLOT_POS]=TST_RCP_LIMIT                                                             
                  END IF                                                                                                                
                  LET            TMP_CASS_SEL[TMP_SLOT_POS+5]=-4                                                                        
                  LET            TMP_CASS_BODY[TMP_SLOT_POS+5][1]="FILLED"                                                              
                 END IF                                                                                                                 
                 LET             TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                UNTIL            TMP_SLOT_POS>50                                                                                        
               END IF                                                                                                                   
                                                                                                                                        
               ACTION            "HP_TEST_VERIFY_WAFERID_MULTI"                                                                         
               IF                RTN_TEST_VERIFY_WAFERID="Error"                                                                        
                LET              PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                      
                LET              PNL_MSG_MSG1="$ERR_WAFERID"                                                                            
                LET              PNL_MSG_MSG2="\""+RTN_TEST_DUPLICATE_WAFERID+"\""                                                      
                LET              PNL_KEY_OK="$RETRY"                                                                                    
                PANEL            "HP_DISP_ERR2_OK"                                                                                      
                PANEL            "",0                                                                                                   
                LET              TMP_SLOT_POS=1                                                                                         
                REPEAT                                                                                                                  
                 IF              TMP_SLOT_LOTID[TMP_SLOT_POS]=TST_RCP_LOTID                                                             
                  IF             TMP_SLOT_PROCESS[TMP_SLOT_POS]=TST_RCP_PROCESS                                                         
                   IF            TMP_SLOT_TPL[TMP_SLOT_POS]+".tpl"=TST_RCP_TPL                                                          
                    IF           TMP_SLOT_LIMIT[TMP_SLOT_POS]+".lim"<>TST_RCP_LIMIT                                                     
                     IF          TMP_SLOT_LIMIT[TMP_SLOT_POS]<>TST_RCP_LIMIT                                                            
                      GOTO       Next_cass1                                                                                             
                     END IF                                                                                                             
                    END IF                                                                                                              
                    LET          TMP_CASS_BODY[TMP_SLOT_POS][1]=TST_SLOT_WAFERID[TMP_SLOT_POS]                                          
                    LET          TMP_CASS_SEL[TMP_SLOT_POS]=3                                                                           
                   END IF                                                                                                               
                  END IF                                                                                                                
                 END IF                                                                                                                 
 Next_cass1      LET             TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                UNTIL            TMP_SLOT_POS>25                                                                                        
                REPEAT                                                                                                                  
                 IF              TMP_SLOT_LOTID[TMP_SLOT_POS]=TST_RCP_LOTID                                                             
                  IF             TMP_SLOT_PROCESS[TMP_SLOT_POS]=TST_RCP_PROCESS                                                         
                   IF            TMP_SLOT_TPL[TMP_SLOT_POS]+".tpl"=TST_RCP_TPL                                                          
                    IF           TMP_SLOT_LIMIT[TMP_SLOT_POS]+".lim"<>TST_RCP_LIMIT                                                     
                     IF          TMP_SLOT_LIMIT[TMP_SLOT_POS]<>TST_RCP_LIMIT                                                            
                      GOTO       Next_cass2                                                                                             
                     END IF                                                                                                             
                    END IF                                                                                                              
                    LET          TMP_CASS_BODY[TMP_SLOT_POS+5][1]=TST_SLOT_WAFERID[TMP_SLOT_POS]                                        
                    LET          TMP_CASS_SEL[TMP_SLOT_POS+5]=3                                                                         
                   END IF                                                                                                               
                  END IF                                                                                                                
                 END IF                                                                                                                 
 Next_cass2      LET             TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                UNTIL            TMP_SLOT_POS>50                                                                                        
                GOTO             Enter_recipe                                                                                           
               END IF                                                                                                                   
                                                                                                                                        
               IF                PANEL.RETURN="MORE"                                                                                    
                GOTO             Enter_recipe                                                                                           
               END IF                                                                                                                   
 Scan_first                                                                                                                             
               LET               TMP_SLOT_POS=1                                                                                         
               REPEAT                                                                                                                   
                IF               TST_SLOT_SELECT[TMP_SLOT_POS]=1                                                                        
                 GOTO            Set_final                                                                                              
                END IF                                                                                                                  
                LET              TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
               UNTIL             TMP_SLOT_POS>PRB_SLOT_TOTAL_ALL                                                                        
                                                                                                                                        
               LET               PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                                                      
               LET               PNL_MSG_MSG1="$ERR_NO_RECIPE_ASSIGNED"                                                                 
               LET               PNL_KEY_OK="$RETRY"                                                                                    
               PANEL             "HP_DISP_ERR1_OK"                                                                                      
               PANEL             "HP_DISP_ERR1_OK",0                                                                                    
               GOTO              Enter_recipe                                                                                           
 Set_final                                                                                                                              
               LET               TST_SLOT_LOT_INDEX[TMP_SLOT_POS]=1                                                                     
               LET               TST_LOT_ID[1]=TMP_SLOT_LOTID[TMP_SLOT_POS]                                                             
               LET               TST_LOT_PROCESS[1]=TMP_SLOT_PROCESS[TMP_SLOT_POS]                                                      
               LET               TST_LOT_TPL[1]=TMP_SLOT_TPL[TMP_SLOT_POS]                                                              
               LET               TST_LOT_LIMIT[1]=TMP_SLOT_LIMIT[TMP_SLOT_POS]                                                          
                                                                                                                                        
               LET               TMP_LOT_INDEX=1                                                                                        
               LET               TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                                                                                                                                        
               WHILE             TMP_SLOT_POS<=PRB_SLOT_TOTAL_ALL                                                                       
                WHILE            TMP_SLOT_POS<=PRB_SLOT_TOTAL_ALL                                                                       
                 IF              TST_SLOT_SELECT[TMP_SLOT_POS]>0                                                                        
                  IF             TMP_SLOT_LOTID[TMP_SLOT_POS]=TST_LOT_ID[TMP_LOT_INDEX]                                                 
                   IF            TMP_SLOT_PROCESS[TMP_SLOT_POS]=TST_LOT_PROCESS[TMP_LOT_INDEX]                                          
                    IF           TMP_SLOT_TPL[TMP_SLOT_POS]=TST_LOT_TPL[TMP_LOT_INDEX]                                                  
                     IF          TMP_SLOT_LIMIT[TMP_SLOT_POS]=TST_LOT_LIMIT[TMP_LOT_INDEX]                                              
                      LET        TST_SLOT_LOT_INDEX[TMP_SLOT_POS]=TMP_LOT_INDEX                                                         
                      LET        TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                      BREAK                                                                                                             
                     END IF                                                                                                             
                    END IF                                                                                                              
                   END IF                                                                                                               
                  END IF                                                                                                                
                  LET            TMP_LOT_INDEX=TMP_LOT_INDEX+1                                                                          
                  LET            TST_SLOT_LOT_INDEX[TMP_SLOT_POS]=TMP_LOT_INDEX                                                         
                  LET            TST_LOT_PROCESS[TMP_LOT_INDEX]=TMP_SLOT_PROCESS[TMP_SLOT_POS]                                          
                  LET            TST_LOT_ID[TMP_LOT_INDEX]=TMP_SLOT_LOTID[TMP_SLOT_POS]                                                 
                  LET            TST_LOT_TPL[TMP_LOT_INDEX]=TMP_SLOT_TPL[TMP_SLOT_POS]                                                  
                  LET            TST_LOT_LIMIT[TMP_LOT_INDEX]=TMP_SLOT_LIMIT[TMP_SLOT_POS]                                              
                 END IF                                                                                                                 
                 LET             TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                END WHILE                                                                                                               
               END WHILE                                                                                                                
                                                                                                                                        
               LET               TST_LOT_TOTAL=TMP_LOT_INDEX                                                                            
               LET               TMP_LOT_INDEX=1                                                                                        
               REPEAT                                                                                                                   
                LET              TST_LOT_DATA_DIR[TMP_LOT_INDEX]=DIR_DATAFILE + "/" + TST_LOT_ID[TMP_LOT_INDEX]                         
                LET              TST_LOT_DATA_NAME[TMP_LOT_INDEX]=TST_LOT_ID[TMP_LOT_INDEX] + "_" + TST_LOT_PROCESS[TMP_LOT_INDEX]      
                LET              TMP_LOT_INDEX=TMP_LOT_INDEX+1                                                                          
               UNTIL             TMP_LOT_INDEX>TST_LOT_TOTAL                                                                            
                                                                                                                                        
               LET               TST_SLOT_SELECTABLE=TST_SLOT_SELECT                                                                    
               LET               RTN_TEST_SET_RECIPE="Next"                                                                             
