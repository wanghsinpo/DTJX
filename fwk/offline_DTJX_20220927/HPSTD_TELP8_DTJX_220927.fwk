$Type: Action
$Name: HP_SET_RECIPE_DOUBLE
$Vers: 1
$Desc: Set test recipe.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$---- ------- ----------------------------------- - - -
 BODY                                                  
      LET     PNL_RCP_LOT_LABEL="$LOTID_RECIPE"        
      LET     PNL_RCP_CAS1_LABEL="$CASSID_RECIPE"      
      LET     RTN_PLG_SET_RECIPE="Continue"            
                                                       
      PANEL   "HP_SET_RECIPE_1L2C"                     
      PANEL   "",0                                     
                                                       
      IF      PANEL.RETURN="CANCEL"                    
       LET    RTN_PLG_SET_RECIPE="Retry"               
       RETURN                                          
      END IF                                           
      IF      PANEL.RETURN="MAIN"                      
       LET    RTN_PLG_SET_RECIPE="Main"                
       RETURN                                          
      END IF                                           
$---- ------- ----------------------------------- - - -

$Type: Action
$Name: HP_SET_RECIPE_SINGLE
$Vers: 1
$Desc: JOY modify: 1. apply ctm1.cf\n\t    2. idetify probe-card ID\n\t    3. display probe-card ID before operator press "START"
$Date: 09/27/2022
$Time: 21:50:51
$User: wat
$------------ --------------- ---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------- - - ----------------------------------------------------------
 BODY                                                                                                                                                                                                                                                                                                                          
              IF              TST_TYPE_USE="1L1C"                                                                                                                                                                                                                                                                              
               LET            PNL_RCP_LOT_LABEL="$LOTID_RECIPE"                                                                                                                                                                                                                                                                
               LET            PNL_RCP_CAS1_LABEL="$CASSID_RECIPE"                                                                                                                                                                                                                                                              
              ELSE                                                                                                                                                                                                                                                                                                             
               IF             TST_RCP_INDEX=1                                                                                                                                                                                                                                                                                  
                LET           PNL_RCP_LOT_LABEL="$LOTID1"                                                                                                                                                                                                                                                                      
                LET           PNL_RCP_CAS1_LABEL="$CASSID1"                                                                                                                                                                                                                                                                    
