$Type: Action
$Name: HP_TEST_SEL_WAFER_NEXT
$Vers: 1
$Desc: Select next loaded wafer.\n  Output: RTN_TEST_SEL_WAFER\t\t"Next","Change","Cancel","Prev"\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------ --------- -------------------------------------------------------- - - -
 BODY                                                                                 
              IF        TST_LOT_FIRST=0                                               
               RETURN                                                                 
              END IF                                                                  
                                                                                      
              LET       RTN_TEST_SEL_WAFER="Next"                                     
              LET       PNL_LBL_BTN1="$CONTINUE"                                      
              LET       PNL_LBL_BTN4="$END_TEST"                                      
              LET       TST_SFX="_C"                                                  
                                                                                      
              IF        PRB_SEL_OBO=PRB_SEL_OBO_ON                                    
               IF       TST_FLW_RNDLD=0                                               
                ACTION  "HP_TEST_SEL_WAFER_RND2SQ_" + PRB_GENE                        
               END IF                                                                 
              ELSE                                                                    
               IF       TST_FLW_RNDLD=1                                               
                ACTION  "HP_TEST_SEL_WAFER_SQ2RND_" + PRB_GENE                        
               ELSE                                                                   
                IF      TST_SEL_WAFER_RUNTIME=0                                       
                 RETURN                                                               
                END IF                                                                
               END IF                                                                 
              END IF                                                                  
                                                                                      
 Select_wafer                                                                         
              LET       PNL_TTL_LABEL="$TTL_BREAK_ON_WAFEREND"                        
              IF        UIF_PRB_BZZR_YET=1                                            
               IF       UIF_PRB_BZZR_ON_NEXT_WAFER=1                                  
                LET     PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_WAFEREND_BZZR"      
                ACTION  "HP_PROBER_WAIT_CONT"                                         
                IF      RTN_PRB_WAIT_CONT<>"Wafer_cont"                               
                 RETURN                                                               
                END IF                                                                
               ELSE                                                                   
                LET     PNL_MSG_MSG1="$"+PRB_MODEL+"_WAIT_CONT_ON_WAFEREND"           
                LET     PNL_KEY_OK="$OK"                                              
                ACTION  "HP_PROBER_WAIT_NONE"                                         
               END IF                                                                 
               LET      UIF_PRB_BZZR_YET=0                                            
              END IF                                                                  
                                                                                      
              IF        TST_LOT_NUM="$MULTIPLE"                                       
               ACTION   "HP_TEST_SEL_WAFER_" + PRB_SEL_OBO + PRB_GENE + "_MULTI"      
              ELSE                                                                    
               ACTION   "HP_TEST_SEL_WAFER_" + PRB_SEL_OBO + PRB_GENE + "_1OR2"       
              END IF                                                                  
                                                                                      
              LET       TST_SEL_WAFER_RUNTIME=1                                       
$------------ --------- -------------------------------------------------------- - - -

$Type: Action
$Name: HP_TEST_SEL_WAFER_OBO_G8_1OR2
$Vers: 1
$Desc: Select wafers one by one (1 or 2 lots).\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$-------------- --------------- --------------------------------------------------------------- - - -
 BODY                                                                                                
                IF              (UIF_SEL_WAFER=0) AND (TST_FLW_DETAILED=0)                           
                 LET            RTN_TEST_SEL_WAFER="Next"                                            
                 RETURN                                                                              
                END IF                                                                               
 Set_default                                                                                         
                IF              UIF_MODIFY_WAFER=1                                                   
                 LET            TMP_SLOT_SEL=-3                                                      
                 LET            TMP_SLOT_UNSEL=-4                                                    
                ELSE                                                                                 
                 LET            TMP_SLOT_SEL=-2                                                      
                 LET            TMP_SLOT_UNSEL=-4                                                    
                END IF                                                                               
                                                                                                     
                LET             TMP_SLOT_POS=1                                                       
                SET_TAG_INTEGER Name="TMP_CASS_SEL",Value=TMP_SLOT_UNSEL                             
                SET_TAG_STRING  "TMP_CASS_BODY",""                                                   
                REPEAT                                                                               
                 IF             PRB_SLOT_VALID[TMP_SLOT_POS]=1                                       
                  LET           TMP_CASS_BODY[TMP_SLOT_POS][1]=TST_SLOT_WAFERID[TMP_SLOT_POS]        
                  ACTION        "HP_SET_JDG_WAFER"                                                   
                  LET           TMP_CASS_BODY[TMP_SLOT_POS][2]=TMP_SLOT_JDG                          
                  LET           TMP_CASS_SEL[TMP_SLOT_POS]=TMP_SLOT_SEL                              
                 END IF                                                                              
                 LET            TMP_SLOT_POS=TMP_SLOT_POS+1                                          
                UNTIL           TMP_SLOT_POS>25                                                      
                                                                                                     
                IF              PRB_CASS_NUM="$DOUBLE"                                               
                 REPEAT                                                                              
                  IF            PRB_SLOT_VALID[TMP_SLOT_POS]=1                                       
                   LET          TMP_CASS_BODY[TMP_SLOT_POS+5][1]=TST_SLOT_WAFERID[TMP_SLOT_POS]      
                   ACTION       "HP_SET_JDG_WAFER"                                                   
                   LET          TMP_CASS_BODY[TMP_SLOT_POS+5][2]=TMP_SLOT_JDG                        
                   LET          TMP_CASS_SEL[TMP_SLOT_POS+5]=TMP_SLOT_SEL                            
                  END IF                                                                             
                  LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                          
                 UNTIL          TMP_SLOT_POS>50                                                      
                END IF                                                                               
 Set_next                                                                                            
                LET             TMP_SLOT_POS=TST_SLOT_CURRENT                                        
                IF              TMP_SLOT_POS<=0                                                      
                 LET            TMP_SLOT_POS=1                                                       
                END IF                                                                               
                LET             TMP_I=0                                                              
                WHILE           TMP_SLOT_POS<=PRB_SLOT_TOTAL_ALL                                     
                 IF             PRB_SLOT_VALID[TMP_SLOT_POS]=1                                       
                  IF            PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$YET"                             
                   LET          TMP_I=1                                                              
                   BREAK                                                                             
                  END IF                                                                             
                 END IF                                                                              
                 LET            TMP_SLOT_POS=TMP_SLOT_POS+1                                          
                END WHILE                                                                            
                IF              TMP_I=1                                                              
                 IF             TMP_SLOT_POS<=25                                                     
                  LET           TMP_CASS_SEL[TMP_SLOT_POS]=TMP_CASS_SEL[TMP_SLOT_POS]*(-1)           
                 ELSE                                                                                
                  LET           TMP_CASS_SEL[TMP_SLOT_POS+5]=TMP_CASS_SEL[TMP_SLOT_POS+5]*(-1)       
                 END IF                                                                              
                END IF                                                                               
 Select_slot                                                                                         
                PANEL           "HP_SEL_SLOT_OBO_G8_" + TST_TYPE_USE + TST_SFX,2                     
                PANEL           "",0                                                                 
                IF              PANEL.RETURN="CANCEL"                                                
                 LET            RTN_TEST_SEL_WAFER="Cancel"                                          
                 RETURN                                                                              
                END IF                                                                               
                IF              PANEL.RETURN="PREV"                                                  
                 LET            RTN_TEST_SEL_WAFER="Prev"                                            
                 RETURN                                                                              
                END IF                                                                               
                IF              PANEL.RETURN="CHNGFLW"                                               
                 LET            RTN_TEST_SEL_WAFER="Change"                                          
                 RETURN                                                                              
                END IF                                                                               
 Set_selection                                                                                       
                SET_TAG_INTEGER Name="TST_SLOT_SELECT"                                               
                                                                                                     
                LET             TMP_SLOT_POS=1                                                       
                REPEAT                                                                               
                 IF             TMP_CASS_SEL[TMP_SLOT_POS]>0                                         
                  LET           TST_SLOT_SELECT[TMP_SLOT_POS]=1                                      
                  LET           TST_SLOT_WAFERID[TMP_SLOT_POS]=TMP_CASS_BODY[TMP_SLOT_POS][1]        
                  BREAK                                                                              
                 END IF                                                                              
                 LET            TMP_SLOT_POS=TMP_SLOT_POS+1                                          
                UNTIL           TMP_SLOT_POS>25                                                      
                                                                                                     
                IF              PRB_CASS_NUM="$DOUBLE"                                               
                 REPEAT                                                                              
                  IF            TMP_CASS_SEL[TMP_SLOT_POS+5]>0                                       
                   LET          TST_SLOT_SELECT[TMP_SLOT_POS]=1                                      
                   LET          TST_SLOT_WAFERID[TMP_SLOT_POS]=TMP_CASS_BODY[TMP_SLOT_POS][1]        
                   BREAK                                                                             
                  END IF                                                                             
                  LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                          
                 UNTIL          TMP_SLOT_POS>50                                                      
                END IF                                                                               
 Verify_waferid                                                                                      
                ACTION          "HP_TEST_VERIFY_WAFERID_1OR2"                                        
                IF              RTN_TEST_VERIFY_WAFERID="Error"                                      
                 LET            PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                    
                 LET            PNL_MSG_MSG1="$ERR_WAFERID"                                          
                 LET            PNL_MSG_MSG2="\""+RTN_TEST_DUPLICATE_WAFERID+"\""                    
                 LET            PNL_KEY_OK="$RETRY"                                                  
                 PANEL          "HP_DISP_ERR2_OK"                                                    
                 PANEL          "",0                                                                 
                 LET            TMP_SLOT_POS=1                                                       
                 GOTO           Select_slot                                                          
                END IF                                                                               
                                                                                                     
                LET             RTN_TEST_SEL_WAFER="Next"                                            
$-------------- --------------- --------------------------------------------------------------- - - -

$Type: Action
$Name: HP_TEST_SEL_WAFER_OBO_G8_MULTI
$Vers: 1
$Desc: Select wafers one by one (multi lots).\n  SPECS2.5: Initial revision.
$Date: 09/01/2000
$Time: 09:00:00
$User: specs
$------------- --------------- ------------------------------------------------------ - - -
 BODY                                                                                      
               SET_TAG_INTEGER "TMP_SLOTA_SEL",-4                                          
               SET_TAG_INTEGER "TMP_SLOTB_SEL",-4                                          
               SET_TAG_INTEGER "TMP_SLOTC_SEL",-4                                          
               SET_TAG_INTEGER "TMP_SLOTD_SEL",-4                                          
               SET_TAG_STRING  "TMP_SLOTA_RCP",""                                          
               SET_TAG_STRING  "TMP_SLOTB_RCP",""                                          
               SET_TAG_STRING  "TMP_SLOTC_RCP",""                                          
               SET_TAG_STRING  "TMP_SLOTD_RCP",""                                          
 Set_default                                                                               
               LET             TMP_SLOT_POS=1                                              
               SET_TAG_INTEGER "TST_SLOT_SELECT",0                                         
               REPEAT                                                                      
                IF             TST_SLOT_SELECTABLE[TMP_SLOT_POS]=1                         
                 IF            PRB_SLOT_VALID[TMP_SLOT_POS]=1                              
                  LET          TMP_LOT_INDEX=TST_SLOT_LOT_INDEX[TMP_SLOT_POS]              
                  LET          TMP_I=TMP_SLOT_POS                                          
                  LET          TMP_SLOTA_SEL[TMP_I]=-2                                     
                  IF           PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$YET"                    
                   LET         TMP_SLOTA_SEL[TMP_I]=+2                                     
                  END IF                                                                   
                  LET          TMP_SLOTA_RCP[TMP_I][1]=TST_LOT_ID[TMP_LOT_INDEX]           
                  LET          TMP_SLOTA_RCP[TMP_I][2]=TST_SLOT_WAFERID[TMP_SLOT_POS]      
                  LET          TMP_SLOTA_RCP[TMP_I][3]=TST_LOT_PROCESS[TMP_LOT_INDEX]      
                  LET          TMP_SLOTA_RCP[TMP_I][4]=TST_LOT_TPL[TMP_LOT_INDEX]          
                  LET          TMP_SLOTA_RCP[TMP_I][5]=TST_LOT_LIMIT[TMP_LOT_INDEX]        
                  ACTION       "HP_SET_JDG_WAFER"                                          
                  LET          TMP_SLOTA_RCP[TMP_I][6]=TMP_SLOT_JDG                        
                 END IF                                                                    
                END IF                                                                     
                LET            TMP_SLOT_POS=TMP_SLOT_POS+1                                 
               UNTIL           TMP_SLOT_POS>15                                             
                                                                                           
               REPEAT                                                                      
                IF             TST_SLOT_SELECTABLE[TMP_SLOT_POS]=1                         
                 IF            PRB_SLOT_VALID[TMP_SLOT_POS]=1                              
                  LET          TMP_LOT_INDEX=TST_SLOT_LOT_INDEX[TMP_SLOT_POS]              
                  LET          TMP_I=TMP_SLOT_POS-15                                       
                  LET          TMP_SLOTB_SEL[TMP_I]=-2                                     
                  IF           PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$YET"                    
                   LET         TMP_SLOTB_SEL[TMP_I]=+2                                     
                  END IF                                                                   
                  LET          TMP_SLOTB_RCP[TMP_I][1]=TST_LOT_ID[TMP_LOT_INDEX]           
                  LET          TMP_SLOTB_RCP[TMP_I][2]=TST_SLOT_WAFERID[TMP_SLOT_POS]      
                  LET          TMP_SLOTB_RCP[TMP_I][3]=TST_LOT_PROCESS[TMP_LOT_INDEX]      
                  LET          TMP_SLOTB_RCP[TMP_I][4]=TST_LOT_TPL[TMP_LOT_INDEX]          
                  LET          TMP_SLOTB_RCP[TMP_I][5]=TST_LOT_LIMIT[TMP_LOT_INDEX]        
                  ACTION       "HP_SET_JDG_WAFER"                                          
                  LET          TMP_SLOTB_RCP[TMP_I][6]=TMP_SLOT_JDG                        
                 END IF                                                                    
                END IF                                                                     
                LET            TMP_SLOT_POS=TMP_SLOT_POS+1                                 
               UNTIL           TMP_SLOT_POS>25                                             
                                                                                           
               IF              PRB_CASS_NUM="$DOUBLE"                                      
                REPEAT                                                                     
                 IF            TST_SLOT_SELECTABLE[TMP_SLOT_POS]=1                         
                  IF           PRB_SLOT_VALID[TMP_SLOT_POS]=1                              
                   LET         TMP_LOT_INDEX=TST_SLOT_LOT_INDEX[TMP_SLOT_POS]              
                   LET         TMP_I=TMP_SLOT_POS-25                                       
                   LET         TMP_SLOTC_SEL[TMP_I]=-2                                     
                   IF          PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$YET"                    
                    LET        TMP_SLOTC_SEL[TMP_I]=+2                                     
                   END IF                                                                  
                   LET         TMP_SLOTC_RCP[TMP_I][1]=TST_LOT_ID[TMP_LOT_INDEX]           
                   LET         TMP_SLOTC_RCP[TMP_I][2]=TST_SLOT_WAFERID[TMP_SLOT_POS]      
                   LET         TMP_SLOTC_RCP[TMP_I][3]=TST_LOT_PROCESS[TMP_LOT_INDEX]      
                   LET         TMP_SLOTC_RCP[TMP_I][4]=TST_LOT_TPL[TMP_LOT_INDEX]          
                   LET         TMP_SLOTC_RCP[TMP_I][5]=TST_LOT_LIMIT[TMP_LOT_INDEX]        
                   ACTION      "HP_SET_JDG_WAFER"                                          
                   LET         TMP_SLOTC_RCP[TMP_I][6]=TMP_SLOT_JDG                        
                  END IF                                                                   
                 END IF                                                                    
                 LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                 
                UNTIL          TMP_SLOT_POS>40                                             
                                                                                           
                REPEAT                                                                     
                 IF            TST_SLOT_SELECTABLE[TMP_SLOT_POS]=1                         
                  IF           PRB_SLOT_VALID[TMP_SLOT_POS]=1                              
                   LET         TMP_LOT_INDEX=TST_SLOT_LOT_INDEX[TMP_SLOT_POS]              
                   LET         TMP_I=TMP_SLOT_POS-40                                       
                   LET         TMP_SLOTD_SEL[TMP_I]=-2                                     
                   IF          PNL_TEST_CASS_STATE[TMP_SLOT_POS]="$YET"                    
                    LET        TMP_SLOTD_SEL[TMP_I]=+2                                     
                   END IF                                                                  
                   LET         TMP_SLOTD_RCP[TMP_I][1]=TST_LOT_ID[TMP_LOT_INDEX]           
                   LET         TMP_SLOTD_RCP[TMP_I][2]=TST_SLOT_WAFERID[TMP_SLOT_POS]      
                   LET         TMP_SLOTD_RCP[TMP_I][3]=TST_LOT_PROCESS[TMP_LOT_INDEX]      
                   LET         TMP_SLOTD_RCP[TMP_I][4]=TST_LOT_TPL[TMP_LOT_INDEX]          
                   LET         TMP_SLOTD_RCP[TMP_I][5]=TST_LOT_LIMIT[TMP_LOT_INDEX]        
                   ACTION      "HP_SET_JDG_WAFER"                                          
                   LET         TMP_SLOTD_RCP[TMP_I][6]=TMP_SLOT_JDG                        
                  END IF                                                                   
                 END IF                                                                    
                 LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                 
                UNTIL          TMP_SLOT_POS>50                                             
               END IF                                                                      
                                                                                           
               IF              (UIF_SEL_WAFER=0) AND (TST_FLW_DETAILED=0)                  
                LET            RTN_TEST_SEL_WAFER="Next"                                   
                RETURN                                                                     
               END IF                                                                      
 Select_slot                                                                               
               PANEL           "HP_SEL_SLOT_OBO_G8_" + TST_TYPE_USE + TST_SFX,2            
               PANEL           "",0                                                        
               IF              PANEL.RETURN="CANCEL"                                       
                LET            RTN_TEST_SEL_WAFER="Cancel"                                 
                RETURN                                                                     
               END IF                                                                      
               IF              PANEL.RETURN="PREV"                                         
                LET            RTN_TEST_SEL_WAFER="Prev"                                   
                RETURN                                                                     
               END IF                                                                      
               IF              PANEL.RETURN="CHNGFLW"                                      
                LET            RTN_TEST_SEL_WAFER="Change"                                 
                RETURN                                                                     
               END IF                                                                      
 Set_selection                                                                             
               ACTION          "HP_SET_SLOT_OBO_G8_" + TST_TYPE_USE                        
               IF              RTN_TEST_SEL_WAFER="Retry"                                  
                RETURN                                                                     
               END IF                                                                      
 Set_slot_lot                                                                              
               LET             RTN_TEST_SEL_WAFER="Next"                                   
