 Init_prober                                                                                                                                   
                ACTION        "HP_PROBER_CONFIG_" + PRB_MODEL                                                                                  
 Init_user                                                                                                                                     
                IF            PRB_ONLINE=1                                                                                                     
                 ACTION       "PLG_INIT_HW_ONLINE"                                                                                             
                ELSE                                                                                                                           
                 ACTION       "PLG_INIT_HW_OFFLINE"                                                                                            
                END IF                                                                                                                         
                IF            RTN_PLG_INIT_HW="Retry"                                                                                          
                 PANEL        "HP_DISP_MSG1_NONE",0                                                                                            
                 GOTO         Select_mode                                                                                                      
                END IF                                                                                                                         
                                                                                                                                               
 Display_config                                                                                                                                
                FILE_HWCONFIG File=FIL_HWCONF,Config=TESTER_START.Config                                                                       
                CHECK_PROBER  Symbol=PRB_MODEL                                                                                                 
                LET           PNL_HWCONF_PRB_CONF=CHECK_PROBER.Vendor + " " + CHECK_PROBER.Model                                               
                ACTION        "PLG_HW_CONF"                                                                                                    
                IF            DSP_INIT_HW=1                                                                                                    
                 PANEL        "HP_DISP_HWCONF",1                                                                                               
                 PANEL        "HP_DISP_HWCONF",0                                                                                               
                 IF           PANEL.RETURN="RETRY"                                                                                             
                  GOTO        Select_mode                                                                                                      
                 END IF                                                                                                                        
                END IF                                                                                                                         
                EXECUTE       "/usr/bin/rm " + FIL_HWCONF                                                                                      
                                                                                                                                               
                                                                                                                                               
                PANEL         "HP_DISP_BLANK",0                                                                                                
                RETURN                                                                                                                         
 Tester_error                                                                                                                                  
                LET           PNL_TTL_LABEL="$TTL_TESTER_ERROR"                                                                                
                LET           PNL_MSG_MSG1="$ERR_TESTER_START"                                                                                 
                LET           PNL_MSG_MSG2=TESTER_START.Message                                                                                
                PANEL         "HP_DISP_ERR2_RETRY_HALT",1                                                                                      
                PANEL         "HP_DISP_ERR2_RETRY_HALT",0                                                                                      
                                                                                                                                               
                IF            PANEL.RETURN="HALT"                                                                                              
                 PANEL        "HP_DISP_BLANK",0                                                                                                
                 LOG_SYSTEM   Session=SYSTEM.SESSIONID,Message="HP_INIT_HARDWARE: TESTER_START Error" + TESTER_START.Message                   
                 STOP                                                                                                                          
                END IF                                                                                                                         
                                                                                                                                               
                GOTO          Select_mode                                                                                                      
 Tester_restart                                                                                                                                
                TESTER_STOP                                                                                                                    
                                                                                                                                               
                IF            TESTER_STOP.Stat=-1                                                                                              
                 LET          PNL_TTL_LABEL="$TTL_TESTER_ERROR"                                                                                
                 LET          PNL_MSG_MSG1="$ERR_TESTER_STOP"                                                                                  
                 LET          PNL_KEY_OK="$HALT"                                                                                               
                 PANEL        "HP_DISP_ERR1_OK"                                                                                                
                 PANEL        "HP_DISP_ERR1_OK",0                                                                                              
                 PANEL        "HP_DISP_BLANK",0                                                                                                
                 LOG_SYSTEM   Session=SYSTEM.SESSIONID,Message="HP_INIT_HARDWARE: TESTER_STOP Error"                                           
                 ACTION       "PLG_FWK_END"                                                                                                    
                 STOP                                                                                                                          
                END IF                                                                                                                         
                                                                                                                                               
                GOTO          Init_tester                                                                                                      
