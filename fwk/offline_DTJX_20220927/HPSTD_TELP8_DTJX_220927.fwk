      LET               LOT.CTM_TESTEND=CTM_TIMEDATE.Time2                                            201203,sami: add
                                                                                                                      
      STRING_SPLIT      Src=CTM_WAF_JDG,Del=";"                                                                       
      LET               CTM_WAF_COUNT=STRING_SPLIT.Count-1                                                            
      IF                CTM_WAF_COUNT>=1                                                                              
        LET             I=1                                                                                           
