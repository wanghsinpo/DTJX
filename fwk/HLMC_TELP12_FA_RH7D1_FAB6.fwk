                       RETURN                                                                                                                                                                              Not to send 'NACK' event                                    
                     END IF                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                                       
 Case_proberstatus                                                                                                                                                                                                                                                     
                     IF                       FA_GEM_COMMAND=="ProberStatus"                                                                                                                               ProberStatus command                                        
                       RETURN                                                                                                                                                                              Not to send 'NACK' event                                    
                     END IF                                                                                                                                                                                                                                            
                     IF                       FA_GEM_COMMAND=="EXTRACT-PROBER-RECIPE"                                                                                                                      ProberStatus command                                        
                       RETURN                                                                                                                                                                              Not to send 'NACK' event                                    
                     END IF                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                                       
 Case_proberalarm                                                                                                                                                                                                                                                      
                     IF                       FA_GEM_COMMAND=="ProberAlarm"                                                                                                                                                                                            
                       IF                     (FA_EQUIP_STATE=="ABORTING") OR (FA_EQUIP_STATE=="STOPPING") OR (FA_EQUIP_STATE=="PJABORTING") OR (FA_EQUIP_STATE=="CJABORTING")                             ### FA3.0                                                   
                         RETURN                                                                                                                                                                            ### FA3.0                                                   
                       END IF                                                                                                                                                                              ### FA3.0                                                   
                       LET                    PNL_TTL_LABEL="Prober Alarm"                                                                                                                                                                                             
                       LET                    PNL_MSG_MSG1="ERROR 70-30004\n\nProber sent alarm. Alarm needs to be cleared manually."                                                                                                                                  
                       LET                    PNL_MSG_MSG2="FA_CHECK_MESSAGE"                                                                                                                                                                                          
                       IF                     FA_ALARM_ON=="OFF"                                                                                                                                           ### FA Panel disp bug fix 2                                 
                         PANEL                "FA_DISP_ERR2_NONE",2                                                                                                                                                                                                    
                       END IF                                                                                                                                                                              ### FA Panel disp bug fix 2                                 
                       LET                    FA_PAUSE_ON="ON"                                                                                                                                                                                                         
                       LET                    FA_ALARM_ON="ON"                                                                                                                                             ### FA Panel disp bug fix                                   
                     END IF                                                                                                                                                                                                                                            
 Case_AlarmCleared                                                                                                                                                                                         ### FA3.0                                                   
                     IF                       FA_GEM_COMMAND=="AlarmCleared"                                                                                                                               ### FA3.0                                                   
                       PANEL                  "FA_DISP_ERR2_NONE",0                                                                                                                                        ### FA3.0                                                   
                       LET                    FA_ALARM_ON="OFF"                                                                                                                                            ### FA3.0                                                   
                     END IF                                                                                                                                                                                ### FA3.0                                                   
#                    WAIT                     Wait=1                                                                                                                                                                                                                   
                     IF                       FA_PAUSE_ON=="ON"                                                                                                                                            Pause status                                                
                       GOTO                   Read_command                                                                                                                                                                                                             
                     END IF                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                                       
                     IF                       (FA_GEM_COMMAND=="CLOSE-LOT") AND (FA_LOTCANCEL_BYPRB==1)                                                                                                    ### FA3.2 PRB LOTCANCEL occur while 2nd after wafer loadings
                       ACTION                 "FA_SEND_ACK"                                                                                                                                                ### FA3.2                                                   
                       LET                    FA_PENDING=1                                                                                                                                                 ### FA3.2                                                   
                       RETURN                                                                                                                                                                              ### FA3.2                                                   
                     END IF                                                                                                                                                                                ### FA3.2                                                   
                     IF                       FA_GEM_COMMAND<>""                                                                                                                                           Other command                                               
                       ACTION                 "FA_SEND_NACK"                                                                                                                                                                                                           
                     END IF                                                                                                                                                                                                                                            
