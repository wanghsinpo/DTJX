$Type: Action
$Name: HP_PROBER_BREAK_TELP12
$Vers: 1
$Desc: Set prober state pause and verify return value.\n  SPECS2.5: Initial revision.
$Date: 11/05/2010
$Time: 20:44:03
$User: watop
$----------- ------- ---------------------------------- - - -
 BODY                                                        
             ACTION  "HP_PROBER_PRB_BREAK_TELP12"            
                                                             
             IF      RTN_PRB_PAUSE=="Wafer_ready"            
              LET    PRB_ABORT_WAFER=1                       
              REJECT WAFER                                   
              RETURN                                         
             END IF                                          
             IF      RTN_PRB_PAUSE=="Wafer_end"              
              LET    PRB_ABORT_WAFER=-1                      
              REJECT WAFER                                   
              RETURN                                         
             END IF                                          
             IF      RTN_PRB_PAUSE=="Lot_end"                
              LET    PRB_ABORT_ALL=1                         
              REJECT LOT                                     
              RETURN                                         
             END IF                                          
             IF      RTN_PRB_PAUSE<>"Wafer_cont"             
              LET    PRB_ABORT_ALL=-1                        
              REJECT LOT                                     
              GOTO   Fatal_error                             
             END IF                                          
                                                             
             RETURN                                          
 Fatal_error                                                 
             LET     PNL_TTL_LABEL="$TTL_PROBER_ERROR"       
             LET     PNL_MSG_MSG1="$TELP12_FATAL_ERROR"      
             LET     PNL_KEY_OK="$OK"                        
             PANEL   "HP_DISP_ERR1_OK",2                     
             PANEL   "HP_DISP_ERR1_OK",0                     
