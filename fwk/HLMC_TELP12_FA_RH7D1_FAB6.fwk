$Type: Action
$Name: HP_PROBER_PRB_BREAK_TELP12
$Vers: 1
$Desc: Ring prober's bell and wait to be continued.\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------------ ----------------------------------------------------------------------------------------- - - -
 BODY                                                                                                             
      IF           PRB_ONLINE==0                                                                                  
       LET         RTN_PRB_PAUSE="Wafer_cont"                                                                     
       WAIT        2                                                                                              
       RETURN                                                                                                     
      END IF                                                                                                      
                                                                                                                  
      PROBER_PAUSE                                                                                                
                                                                                                                  
      IF           PROBER_PAUSE.Status=="ERROR"                                                                   
       LET         RTN_PRB_PAUSE="Prober_abort"                                                                   
       LOG_PROBER  Session=SYSTEM.SESSIONID,Message="PRB_PAUSE: PROBER_PAUSE Error " + PROBER_PAUSE.Error         
       RETURN                                                                                                     
      END IF                                                                                                      
                                                                                                                  
      ACTION       "HP_PROBER_PRB_QUERY_READY_TELP12"                                                             
      LET          RTN_PRB_PAUSE="Prober_abort"                                                                   
                                                                                                                  
      IF           RTN_PRB_QUERY_READY=="Wafer_cont"                                                              
       LET         RTN_PRB_PAUSE="Wafer_cont"                                                                     
      END IF                                                                                                      
      IF           RTN_PRB_QUERY_READY=="Wafer_ready"                                                             
       LET         RTN_PRB_PAUSE="Wafer_ready"                                                                    
      END IF                                                                                                      
      IF           RTN_PRB_QUERY_READY=="Wafer_end"                                                               
       LET         RTN_PRB_PAUSE="Wafer_end"                                                                      
      END IF                                                                                                      
      IF           RTN_PRB_QUERY_READY=="Lot_end"                                                                 
       LET         RTN_PRB_PAUSE="Lot_end"                                                                        
      END IF                                                                                                      
                                                                                                                  
      IF           RTN_PRB_PAUSE=="Prober_abort"                                                                  
       LOG_PROBER  Session=SYSTEM.SESSIONID,Message="PRB_PAUSE: PRB_QUERY_READY Error " + PROBER_READY.Error      
      END IF                                                                                                      
