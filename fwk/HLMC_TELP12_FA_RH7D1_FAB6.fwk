                 LET             TMP_LOT_INDEX=TST_SLOT_LOT_INDEX[TMP_SLOT_POS]                                                                                                                                                                                          
                 WRITE_WAFER_SUM Slot=TMP_SLOT_POS,Lot=TST_LOT_ID[TMP_LOT_INDEX],Wafer=TST_SLOT_WAFERID[TMP_SLOT_POS],Yld_item=TST_SLOT_YLD_ITEM[TMP_SLOT_POS],Yld_die=TST_SLOT_YLD_DIE[TMP_SLOT_POS],Judge=TST_SLOT_YLD_PF[TMP_SLOT_POS]                                
