            GOTO         Mastererr                                               
           END IF                                                                
           LET           TST_RCP_LOTID=MASTER_LOOKUP.Field                       
                                                                                 
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=4               
           IF            MASTER_LOOKUP.Stat<0                                    
            GOTO         Mastererr                                               
           END IF                                                                
           LET           TST_RCP_PROCESS=MASTER_LOOKUP.Field                     
                                                                                 
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=5               
           IF            MASTER_LOOKUP.Stat<0                                    
            GOTO         Mastererr                                               
           END IF                                                                
           LET           TST_RCP_TPL=MASTER_LOOKUP.Field                         
                                                                                 
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=6               
           IF            MASTER_LOOKUP.Stat<0                                    
            GOTO         Mastererr                                               
           END IF                                                                
                                                                                 
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=7               
           LET           TST_RCP_LIMIT=MASTER_LOOKUP.Field                       
                                                                                 
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=8               
           LET           PRB_FILE_NAME=MASTER_LOOKUP.Field                       
                                                                                 
           LET           RTN_TEST_QUERY_RECIPE_ERRMSG=""                         
           LET           RTN_TEST_QUERY_RECIPE="OK"                              
           RETURN                                                                
 Mastererr                                                                       
           LET           RTN_TEST_QUERY_RECIPE="Error"                           
           LET           RTN_TEST_QUERY_RECIPE_ERRMSG="$ERR_RECIPE_NOFIELD"      
