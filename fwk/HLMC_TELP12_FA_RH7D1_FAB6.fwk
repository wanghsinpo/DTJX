$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------------- ------------------------------------ - - -
 BODY                                                         
      LET           MNU_COM_TTL="$TTL_UTIL"                   
      LET           MNU_COM_INS="$INS_SELECT_UTIL"            
      LET           MNU_COM_KEY[1]="$MAIN"                    
