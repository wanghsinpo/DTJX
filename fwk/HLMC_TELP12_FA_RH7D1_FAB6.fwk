                  ELSE                                                                                               
                   LET          PRB_SLOT_SELECT[TMP_SLOT_POS]=PRB_SKIP                              PRB_SKIP:1       
                  END IF                                                                                             
                  LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                                          
