             LET     PNL_TTL_LABEL="$TTL_PROBER_OPERATION"                            
             LET     PNL_MSG_MSG1="$TELP12_ALIGNMENT_ERROR"                           
 Query_ready                                                                          
             ACTION  "HP_PROBER_PRB_QUERY_READY_TELP12"         Detect 1st wafer ready
                                                                                      
             IF      TMP_DISP_ASSIST==1                                               
              LET    TMP_DISP_ASSIST=0                                                
              PANEL  "HP_DISP_ERR1_NONE",0                                            
