                UNTIL          TMP_SLOT_POS>50                                                             
                LET            PRB_CASSNO=2                                                                
                ACTION         "HP_PROBER_SETCMAP"                                                         
               END IF                                                                                      
                                                                                                           
               IF              UIF_SEL_WAFER==0                                                            
                LET            RTN_TEST_SEL_WAFER="Next"                                                   
                PANEL           "HP_DISP_BLANK",0                                                          
                RETURN                                                                                     
               END IF                                                                                      
                                                                                                           
                ACTION         "HP_SET_MODE_DISP"                                         ### FA INSPECTION
               IF              FA_CONTROL_MODE<>"REMOTE"                                  ### FA INSPECTION
                IF             (UIF_SEL_WAFER==0) AND (TST_FLW_DETAILED==0)               ### FA INSPECTION
                 LET           RTN_TEST_SEL_WAFER="Next"                                  ### FA INSPECTION
                 PANEL         "HP_DISP_BLANK",0                                          ### FA INSPECTION
                 RETURN                                                                   ### FA INSPECTION
                END IF                                                                    ### FA INSPECTION
               END IF                                                                     ### FA INSPECTION
 Select_slot                                                                                               
