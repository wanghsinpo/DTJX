$Type: Action
$Name: HP_EXEC_AUTOPROBE_ZDOWN
$Vers: 1
$Desc: Z-up the chuck to make contact.\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$----- ------------ ------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                
 Zdown                                                                                                               
       IF           PRB_ONLINE==1                                                                                    
        PROBER_DOWN                                                                                                  
        IF          PROBER_DOWN.Status<>"OK"                                                                         
         LOG_PROBER Session=SYSTEM.SESSIONID,Message="EXEC_PROBE_ZDOWN: PROBER_DOWN Error " + PROBER_DOWN.Error      
        END IF                                                                                                       
       END IF                                                                                                        
       LET          PNL_ZUP_DOWN="$Z_SEPARATE"                                                                       
       LET          PNL_COL_ZSTAT="LimeGreen"                                                                        
       LET          PRB_AT_DIEX=PRB_CURR_DIEX                                                                        
       LET          PRB_AT_DIEY=PRB_CURR_DIEY                                                                        
       LET          PRB_AT_MODN=PRB_CURR_MODN                                                                        
       PANEL        "HP_EXEC_AUTOPROBE"                                                                              
$----- ------------ ------------------------------------------------------------------------------------------- - - -

$Type: Action
$Name: HP_EXEC_AUTOPROBE_ZUP
$Vers: 1
$Desc: Z-up the chuck to make contact.\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ------------- ------------------------------------------------------------------------------------------- - - -
 BODY                                                                                                                
 Zup  IF            PNL_ZUP_DOWN="$Z_SEPARATE"                                                                       
       IF           PRB_ONLINE==1                                                                                    
        PROBER_UP                                                                                                    
        IF          PROBER_UP.Status<>"OK"                                                                           
         LOG_PROBER Session=SYSTEM.SESSIONID,Message="EXEC_PROBE_ZUP: PROBER_UP Error " + PROBER_UP.Error            
        END IF                                                                                                       
       END IF                                                                                                        
        LET         PNL_ZUP_DOWN="$Z_CONTACT"                                                                        
      LET           PNL_COL_ZSTAT="Red"                                                                              
        PANEL       "HP_EXEC_AUTOPROBE"                                                                              
      ELSE                                                                                                           
       IF           PRB_ONLINE==1                                                                                    
        PROBER_DOWN                                                                                                  
        IF          PROBER_DOWN.Status<>"OK"                                                                         
         LOG_PROBER Session=SYSTEM.SESSIONID,Message="EXEC_PROBE_ZDOWN: PROBER_DOWN Error " + PROBER_DOWN.Error      
        END IF                                                                                                       
       END IF                                                                                                        
        LET         PNL_ZUP_DOWN="$Z_SEPARATE"                                                                       
      LET           PNL_COL_ZSTAT="LimeGreen"                                                                        
        PANEL       "HP_EXEC_AUTOPROBE"                                                                              
      END IF                                                                                                         
      RETURN                                                                                                         
