      LET         WAFER.TMP_DIEY=PRB_SP_DIEY                              
      LET         WAFER.TMP_MODNAME=PRB_SP_MODN                           
      LET         WAFER.TMP_PRB_SUB_MOVE_MTHD[1]=PRB_SUB_MOVE_MTHD        
      LET         TMP_TYPE_NUM=1                                          
      WHILE       TMP_TYPE_NUM<=DIE.TYPETOTAL                             
       IF         DIE.DEFINELIST[TMP_TYPE_NUM]==PRB_SP_TYPN               
        BREAK                                                             
       END IF                                                             
       LET        TMP_TYPE_NUM=TMP_TYPE_NUM+1                             
      END WHILE                                                           
      GET_LOC_MOD Typeindex=TMP_TYPE_NUM,Modname=PRB_SP_MODN              
      IF          GET_LOC_MOD.Stat==0                                     
       LET        WAFER.TMP_MODX=GET_LOC_MOD.X                            
       LET        WAFER.TMP_MODY=GET_LOC_MOD.Y                            
      ELSE                                                                
       LET        WAFER.TMP_MODX=WAFER.ALIGNMODX                          
       LET        WAFER.TMP_MODY=WAFER.ALIGNMODY                          
      END IF                                                              
      IF          TST_SLOT_CURRENT<=25                                    
       LET        WAFER.TMP_CASSID=SYS_CASSID                             
      ELSE                                                                
       LET        WAFER.TMP_CASSID=SYS_CASSID2                            
      END IF                                                              
                                                                          
      LET         WAFER.PRB_SUB_CURR_X=WAFER.TMP_MODX                     
      LET         WAFER.PRB_SUB_CURR_Y=WAFER.TMP_MODY                     
      LET         WAFER.PRB_SUB_MOVE_X=0                                  
      LET         WAFER.PRB_SUB_MOVE_Y=0                                  
                                                                          
      IF          WAFER.PRB_SUB_ALGN_AUTO==1                              
       LET        WAFER.TMP_PRB_SUB_ALGN_AUTO[1]="$PNL_PRB_SEL_ALGN"      
      ELSE                                                                
       LET        WAFER.TMP_PRB_SUB_ALGN_AUTO[1]=""                       
      END IF                                                              
                                                                          
      ACTION      "HP_PROBER_TEMPMON_"+PRB_MODEL                          
                                                                          
      ACTION      "HP_PROBER_ZDOWN"                                       
      LET         PNL_ZUP_DOWN="$Z_SEPARATE"                              
      LET         PNL_COL_ZSTAT="LimeGreen"                               
                                                                          
      LET         RTN_PRB_SUBCONTROL=1                                    
      PANEL       "HP_EXEC_SUBCONTROL",2                                  
      REPEAT                                                              
      UNTIL       RTN_PRB_SUBCONTROL=0                                    
      PANEL       "HP_EXEC_SUBCONTROL",0                                  
                                                                          
      PRINT       WAFER.TMP_PRB_SUB_MOVE_MTHD[1]                          
      LET         PRB_SUB_MOVE_MTHD=WAFER.TMP_PRB_SUB_MOVE_MTHD[1]        
                                                                          
      IF          WAFER.TMP_PRB_SUB_ALGN_AUTO[1]<>""                      
       LET        WAFER.PRB_SUB_ALGN_AUTO=1                               
      ELSE                                                                
       LET        WAFER.PRB_SUB_ALGN_AUTO=0                               
      END IF                                                              
                                                                          
      ACTION      "HP_PROBER_ZDOWN"                                       
      LET         PNL_ZUP_DOWN="$Z_SEPARATE"                              
      LET         PNL_COL_ZSTAT="LimeGreen"                               
      PANEL       "HP_EXEC_PROBE"                                         
