$Type: Action
$Name: HP_PROBER_PRB_AMOVE_TELP12
$Vers: 1
$Desc: Probe die/module.\n  SPECS2.5: Moved main program to HP_PROBER_PRB_MOVE_OPT_TELP8.\n            Enabled to probe backup dice.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- -------------- ------------------------------------------------------------------------------------------------------------------------ - - -
 BODY                                                                                                                                              
      LET            PRB_MODX=WAFER.PRB_SUB_CURR_X+WAFER.PRB_SUB_MOVE_X                                                                            
      LET            PRB_MODY=WAFER.PRB_SUB_CURR_Y+WAFER.PRB_SUB_MOVE_Y                                                                            
                                                                                                                                                   
      ACTION         "HP_PROBER_PRB_MOVE_OPT_" + PRB_MODEL                                                                                         
                                                                                                                                                   
      LET            WAFER.PRB_SUB_CURR_X=PRB_MODX                                                                                                 
      LET            WAFER.PRB_SUB_CURR_Y=PRB_MODY                                                                                                 
                                                                                                                                                   
      IF             RTN_PRB_MOVE=="Wafer_ready"                                                                                                   
       LET           PRB_ABORT_WAFER=1                                                                                                             
       REJECT        WAFER                                                                                                                         
      END IF                                                                                                                                       
      IF             RTN_PRB_MOVE=="Wafer_end"                                                                                                     
       REJECT        WAFER                                                                                                                         
      END IF                                                                                                                                       
      IF             RTN_PRB_MOVE=="Lot_end"                                                                                                       
       LET           PRB_ABORT_ALL=1                                                                                                               
       REJECT        LOT                                                                                                                           
      END IF                                                                                                                                       
      IF             RTN_PRB_MOVE=="Module_invalid"                                                                                                
       WRITE_INTEGER "%d",DIE.INDEXX                                                                                                               
       LET           TMP_DIEX=WRITE_INTEGER.String                                                                                                 
       WRITE_INTEGER "%d",DIE.INDEXY                                                                                                               
       LET           TMP_DIEY=WRITE_INTEGER.String                                                                                                 
       WRITE_REAL    "%f",MODULE.POSX                                                                                                              
       LET           TMP_MODX=WRITE_REAL.String                                                                                                    
       WRITE_REAL    "%f",MODULE.POSY                                                                                                              
       LET           TMP_MODY=WRITE_REAL.String                                                                                                    
       LOG_PROBER    Session=SYSTEM.SESSIONID,Message="Module skipped: Module ("+TMP_MODX+","+TMP_MODY+") of Die ("+TMP_DIEX+","+TMP_DIEY+")"      
       REJECT        MODULE                                                                                                                        
      END IF                                                                                                                                       
      IF             RTN_PRB_MOVE=="Prober_abort"                                                                                                  
       LET           PRB_ABORT_ALL=-1                                                                                                              
       REJECT        LOT                                                                                                                           
       LET           PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                             
       LET           PNL_MSG_MSG1="$TELP12_FATAL_ERROR"                                                                                            
       LET           PNL_KEY_OK="$OK"                                                                                                              
       PANEL         "HP_DISP_ERR1_OK",2                                                                                                           
       PANEL         "HP_DISP_ERR1_OK",0                                                                                                           
      END IF                                                                                                                                       
