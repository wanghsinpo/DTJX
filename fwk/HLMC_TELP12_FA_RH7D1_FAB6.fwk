$Type: Action
$Name: HP_PROBER_MOVE_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 11/05/2010
$Time: 20:44:03
$User: watop
$---- -------------- ------------------------------------------------------------------------------------------------------------------------ - - --------------
 BODY                                                                                                                                                           
      IF             PRB_ONLINE==0                                                                                                                              
       RETURN                                                                                                                                                   
      END IF                                                                                                                                                    
                                                                                                                                                                
      ACTION         "HP_PROBER_PRB_MOVE_CURR_TELP12"                                                                                                           
                                                                                                                                                                
      IF             RTN_PRB_MOVE=="Wafer_end"                                                                                                                  
       LET           PRB_ABORT_WAFER=-1                                                                                                                         
       REJECT        WAFER                                                                                                                                      
      END IF                                                                                                                                                    
      IF             RTN_PRB_MOVE=="Module_invalid"                                                                                                             
       WRITE_INTEGER "%d",DIE.INDEXX                                                                                                                            
       LET           TMP_DIEX=WRITE_INTEGER.String                                                                                                              
       WRITE_INTEGER "%d",DIE.INDEXY                                                                                                                            
       LET           TMP_DIEY=WRITE_INTEGER.String                                                                                                              
       WRITE_REAL    "%f",MODULE.POSX                                                                                                                           
       LET           TMP_MODX=WRITE_REAL.String                                                                                                                 
       WRITE_REAL    "%f",MODULE.POSY                                                                                                                           
       LET           TMP_MODY=WRITE_REAL.String                                                                                                                 
       LOG_PROBER    Session=SYSTEM.SESSIONID,Message="Module skipped: Module ("+TMP_MODX+","+TMP_MODY+") of Die ("+TMP_DIEX+","+TMP_DIEY+")"                   
       REJECT        MODULE                                                                                                                                     
      END IF                                                                                                                                                    
      IF             RTN_PRB_MOVE=="Prober_abort"                                                                                                               
       RETURN                                                                                                                                                   
       LET           PRB_ABORT_ALL=-1                                                                                                                           
       REJECT        LOT                                                                                                                                        
       LET           PNL_TTL_LABEL="$TTL_PROBER_ERROR"                                                                                                          
       LET           PNL_MSG_MSG1="ERROR 70-30034\n\nProber reports the unexpected status."                                                                     
       LET           PNL_KEY_OK="$OK"                                                                                                                           
#      PANEL         "HP_DISP_ERR1_OK",2                                                                                                                        
#      PANEL         "HP_DISP_ERR1_OK",0                                                                                                                        
       LET           PNL_MSG_MSG2="HP_PROBER_MOVE_TELP12"                                                                                         ### AUTOMATION
       LET           FA_ALID="7000"                                                                                                               ### AUTOMATION
       LET           FA_ALTX="CustomAlarm"                                                                                                        ### AUTOMATION
       LET           FA_ALMSG=PNL_MSG_MSG1                                                                                                        ### AUTOMATION
       ACTION        "FA_WAIT_ABORT"                                                                                                              ### AUTOMATION
      END IF                                                                                                                                                    
