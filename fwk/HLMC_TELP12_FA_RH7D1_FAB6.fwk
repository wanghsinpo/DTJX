      LET          PNL_MSG_MSG1="$HOWTO_DIAG4062"                                                                                  
      PANEL        "HP_DISP_BLANK",2                                                                                               
      PANEL        "HP_DISP_MSG1_NONE",2                                                                                           
                                                                                                                                   
