                   RETURN                                                                                                                                                             
                  ELSE                                                                                                                                                                
                   UPDATE  "HP_PROBER_TEMPSET"                                                                                                                                        
                  END IF                                                                                                                                                              
                                                                                                                                                                                      
                 END WHILE                                                                                                                                                            
                 PANEL     "HP_PROBER_TEMPSET",0                                                                                                                                      
                                                                                                                                                                                      
                 IF        HP_TEMPSET_FINISH==1                                                                                                                                       
                  LET      PNL_MSG_MSG1="$TEMPSET_ABORTED"                                                                                                                            
                 ELSE                                                                                                                                                                 
                  LET      PNL_MSG_MSG1="$FINISH_TEMPSET"                                                                                                                             
                 END IF                                                                                                                                                               
                 PANEL     "HP_DISP_MSG1_NONE",2                                                                                                                                      
                 WAIT      Wait=3                                                                                                                                                     
                 PANEL     "HP_DISP_MSG1_NONE",0                                                                                                                                      
                 IF        (WAFER.TMP_PRB_SUB_ALGN_AUTO)[1]==PNL_CHOICE_ALIGN[1]                                                                                                      
                  IF       WAFER.PRB_SUB_SOAK>0                                                                                                                                       
                   LET     PNL_MSG_MSG1="$WAIT_SOAK"                                                                                                                                  
