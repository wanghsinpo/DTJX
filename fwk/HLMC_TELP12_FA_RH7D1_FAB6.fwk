$Type: Action
$Name: HP_PROBER_PRB_GET_CASSMAP_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- ---------------- ------------------------------------------------------------------------------------------------ - - -------------------------------------------------------------------------
 BODY                                                                                                                                                                                                
      IF               PRB_ONLINE==0                                                                                                                                                                 
       SET_TAG_INTEGER "PRB_SLOT_VALID",1                                                                                                                                                            
       LET             RTN_PRB_GET_CASSMAP="Cassmap_ready"                                                                                                                                           
       RETURN                                                                                                                                                                                        
      END IF                                                                                                                                                                                         
      IF               FA_START_OK == 0                                                                                     ### FA3.4                                                                
        IF             RTN_PRB_QUERY_READY<>"Cassmap_ready"                                                                 If STB==112(#CASSMAP_READY) is somehow already returned                  
          ACTION       "HP_PROBER_PRB_QUERY_READY_TELP12"                                                                   you don't have to query it again                                         
        END IF                                                                                                                                                                                       
      ELSE                                                                                                                  ### FA3.4  STB=112 has read in FA_MAIN_LOOP                              
        LET            RTN_PRB_QUERY_READY="Cassmap_ready"                                                                  ### FA3.4                                                                
      END IF                                                                                                                ### FA3.4                                                                
                                                                                                                                                                                                     
      IF               RTN_PRB_QUERY_READY=="Prober_abort"                                                                                                                                           
       LET             RTN_PRB_GET_CASSMAP="Prober_abort"                                                                                                                                            
       RETURN                                                                                                                                                                                        
      END IF                                                                                                                                                                                         
                                                                                                                                                                                                     
      IF               RTN_PRB_QUERY_READY=="File_ready"                                                                    STB==82(#SETUP_READY) is returned instead of STB==112(#CASSMAP_READY)    
       LET             RTN_PRB_GET_CASSMAP="Not_ready"                                                                      It's caused by the missmatch of GPIB setting between prober and framework
       PROBER_SETUP                                                                                                         To restore the prober control (put prober to READY state)                
       LOG_PROBER      Session=SYSTEM.SESSIONID,Message="PRB_GET_CASSMAP: PRB_QUERY_READY Error " + PROBER_READY.Error                                                                               
       RETURN                                                                                                                                                                                        
      END IF                                                                                                                                                                                         
                                                                                                                                                                                                     
      IF               RTN_PRB_QUERY_READY<>"Cassmap_ready"                                                                                                                                          
       LET             RTN_PRB_GET_CASSMAP="Prober_abort"                                                                                                                                            
       LOG_PROBER      Session=SYSTEM.SESSIONID,Message="PRB_GET_CASSMAP: PRB_QUERY_READY Error " + PROBER_READY.Error                                                                               
       RETURN                                                                                                                                                                                        
      END IF                                                                                                                                                                                         
                                                                                                                                                                                                     
      IF               FA_USECS==1                                                                                          ### AUTOMATION                                                           
        LET            RTN_PRB_GET_CASSMAP="Cassmap_ready"                                                                  ### AUTOMATION                                                           
                                                                                                                                                                                                     
        ACTION         "FA_PRB_GETCMAP_TELP12"                                                                              ### AUTOMATION Check cassette map                                        
        IF             FA_RTN_PRB_GETCMAP<>"Cassmap_ready"                                                                  ### AUTOMATION                                                           
          LET          RTN_PRB_GET_CASSMAP="Not_ready"                                                                      ### AUTOMATION                                                           
        END IF                                                                                                              ### AUTOMATION                                                           
                                                                                                                                                                                                     
        ACTION         "FA_SELECT_SLOT"                                                                                     ### AUTOMATION Set slot map again                                        
#       ACTION         "FA_PRB_SETCMAP_TELP12"                                                                              ### FA3.0                                                                
        ACTION         "FA_PRB_SETCMAP_ODR_TELP12"                                                                          ### FA3.0                                                                
                                                                                                                                                                                                     
        IF             FA_RTN_PRB_SETCMAP<>"OK"                                                                             ### AUTOMATION                                                           
          LET          RTN_PRB_GET_CASSMAP="Prober_abort"                                                                   ### AUTOMATION                                                           
        END IF                                                                                                              ### AUTOMATION                                                           
        RETURN                                                                                                              ### AUTOMATION                                                           
      END IF                                                                                                                ### AUTOMATION                                                           
                                                                                                                                                                                                     
      PROBER_CASSMAP                                                                                                                                                                                 
                                                                                                                                                                                                     
      IF               PROBER_CASSMAP.Status=="OK"                                                                                                                                                   
       LET             RTN_PRB_GET_CASSMAP="Cassmap_ready"                                                                                                                                           
       LET             PRB_SLOT_VALID=PROBER_CASSMAP.Cassmap                                                                                                                                         
       LET             TMP_SLOT_POS=1                                                                                       ### AUTOMATION                                                           
       REPEAT                                                                                                               ### AUTOMATION                                                           
         IF            FA_PORTNUMBER=="2"                                                                                   ### AUTOMATION                                                           
           LET         PRB_SLOT_VALID[TMP_SLOT_POS]=PRB_SLOT_VALID[TMP_SLOT_POS+PRB_SLOT_TOTAL_CASS]                        ### AUTOMATION Ignore over 26 slot...                                    
         END IF                                                                                                             ### AUTOMATION                                                           
         LET           TMP_SLOT_POS=TMP_SLOT_POS+1                                                                          ### AUTOMATION                                                           
       UNTIL           TMP_SLOT_POS>25                                                                                      ### AUTOMATION                                                           
      ELSE                                                                                                                                                                                           
       LET             RTN_PRB_GET_CASSMAP="Prober_abort"                                                                                                                                            
