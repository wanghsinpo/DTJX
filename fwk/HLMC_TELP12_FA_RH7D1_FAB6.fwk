              UNTIL              TMP_I>PRB_SLOT_TOTAL_ALL                                                                                                                                                                                                                
                                                                                                                                                                                                                                                                         
              PANEL              "HP_DISP_SUMMARY_MERGE",0                                                                                                                                                                                                               
 Summary_all                                                                                                                                                                                                                                                             
              SET_TAG_STRING     Name="TMP_SLOT_RESULT"                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                                         
