 Next_cass1      LET             TMP_SLOT_POS=TMP_SLOT_POS+1                                                                            
                UNTIL            TMP_SLOT_POS>25                                                                                        
                REPEAT                                                                                                                  
                 IF              TMP_SLOT_LOTID[TMP_SLOT_POS]==TST_RCP_LOTID                                                            
