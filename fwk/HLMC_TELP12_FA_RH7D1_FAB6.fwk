$Vers: 1
$Desc: Select wafer to test (first time only).\n  SPECS2.5: Initial revision.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---- --------- -------------------------------------------------------- - - ------------------
 BODY                                                                                          
      LET       PNL_LBL_BTN1="$TEST_START"                                   ### FA INSPECTION 
      LET       PNL_LBL_BTN4="$END_TEST"                                     ### FA INSPECTION 
      IF        FA_CONTROL_MODE<>"REMOTE"                                    ### FA INSPECTIONC
       ACTION   "HP_SET_MODE_DISP"                                           ### FA INSPECTION 
       IF       TST_FLW_DETAILED==1                                          ### FA INSPECTION 
        LET     TST_SFX="_P"                                                 ### FA INSPECTION 
       ELSE                                                                  ### FA INSPECTION 
        LET     TST_SFX=""                                                   ### FA INSPECTION 
       END IF                                                                ### FA INSPECTION 
      ELSE                                                                   ### FA INSPECTION 
       ACTION   "HP_SET_MODE_DISP"                                           ### FA INSPECTION 
       LET      TST_SFX=""                                                   ### FA INSPECTION 
      END IF                                                                 ### FA INSPECTION 
      LET       PRB_SEL_OBO=PRB_SEL_OBO_OFF                                                    
      IF        FA_CONTROL_MODE<>"REMOTE"                                    ### FA INSPECTIONC
       ACTION   "HP_SET_MODE_DISP"                                           ### FA INSPECTION 
       IF       TST_FLW_DETAILED==1                                          ### FA INSPECTION 
        IF      PRB_RANDOM==1                                                ### FA INSPECTION 
         IF     TST_FLW_RNDLD=1                                              ### FA INSPECTION 
          LET   PRB_SEL_OBO=PRB_SEL_OBO_ON                                   ### FA INSPECTION 
         END IF                                                              ### FA INSPECTION 
        END IF                                                               ### FA INSPECTION 
       END IF                                                                ### FA INSPECTION 
      END IF                                                                 ### FA INSPECTION 
                                                                                               
      IF        TST_LOT_NUM=="$MULTIPLE"                                                       
       ACTION   "HP_TEST_SEL_WAFER_" + PRB_SEL_OBO + PRB_GENE + "_MULTI"                       
      ELSE                                                                                     
       ACTION   "HP_TEST_SEL_WAFER_" + PRB_SEL_OBO + PRB_GENE + "_1OR2"                        
      END IF                                                                                   
      IF        RTN_TEST_SEL_WAFER=="Cancel"                                                   
       ACTION   "HP_PROBER_LOAD_CANCEL_" + PRB_MODEL                                           
      END IF                                                                                   
                                                                                               
      LET       TST_SFX=""                                                                     
