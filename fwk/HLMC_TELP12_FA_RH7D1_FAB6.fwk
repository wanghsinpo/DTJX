                   LET          PRB_ABORT_ALL=-1                                                                                                            
                   RETURN                                                                                                                                   
                  END IF                                                                                                                                    
                                                                                                                                                            
                  IF            TST_SLOT_SELECT[TST_SLOT_CURRENT]==0                                                                                        
