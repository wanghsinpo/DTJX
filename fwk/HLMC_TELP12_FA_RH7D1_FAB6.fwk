$Vers: 1
$Desc: Desc: Move port2 tag paramter to current tag parameter.\nDate: '01/Jan/07 : initial revision\n      '01/Jan/20 : RSV -> PT2\n      '01/Feb/28 : Port index is used.
$Date: 03/07/2012
$Time: 02:24:50
$User: wat
$---- ------------ --------------------------------------------------------- - - --------------------------------------------------------
 BODY                                                                                                                                    
      IF           FA_PORTNUMBER=="1"                                                                                                    
        LET        FA_RSV_PORTNUMBER=1                                                                                                   
        LET        FA_WAF_LIST=FA_RSV_WAF_LIST1                                  Waferlist                                               
      ELSE                                                                                                                               
        LET        FA_RSV_PORTNUMBER=2                                                                                                   
        LET        FA_WAF_LIST=FA_RSV_WAF_LIST2                                  Waferlist                                               
      END IF                                                                                                                             
      LET          FA_WAF_SPEC_NAME=FA_RSV_WAF_SPEC_NAME[FA_RSV_PORTNUMBER]      WaferSpecFileName                                       
                                                                                                                                         
      LET          TMP_I=0                                                                                                               
      WHILE        TMP_I < FA_MAX_DIETEST                                                                                                
        LET        TMP_I=TMP_I+1                                                                                                         
        IF         FA_RSV_PORTNUMBER==1                                                                                                  
          LET      FA_DIE_TEST[TMP_I][1]=FA_RSV_DIE_TEST1[TMP_I][1]              Die spec                                                
          LET      FA_DIE_TEST[TMP_I][2]=FA_RSV_DIE_TEST1[TMP_I][2]              Test spec                                               
        ELSE                                                                                                                             
          LET      FA_DIE_TEST[TMP_I][1]=FA_RSV_DIE_TEST2[TMP_I][1]              Die spec                                                
          LET      FA_DIE_TEST[TMP_I][2]=FA_RSV_DIE_TEST2[TMP_I][2]              Test spec                                               
        END IF                                                                                                                           
      END WHILE                                                                                                                          
      LET          FA_PRB_SPEC_NAME=FA_RSV_PRB_SPEC_NAME[FA_RSV_PORTNUMBER]      ProbeSpecFileName                                       
      LET          FA_PRB_RCP_NAME=FA_RSV_PRB_RCP_NAME[FA_RSV_PORTNUMBER]        ProberRecipeFileName                                    
#     LET          PRB_FILE_NAME=FA_RSV_PRB_RCP_NAME[FA_RSV_PORTNUMBER]          ProberRecipeFileName -- C.C.Y 20100904 FOR DATA TRANSFER
      LET          FA_PPEXEC_NAME=FA_RSV_PPEXEC_NAME[FA_RSV_PORTNUMBER]          PPExecName                                              
      LET          FA_CARRIER_ID=FA_RSV_CARRIER_ID[FA_RSV_PORTNUMBER]            CarrierID                                               
      LET          FA_LOT_ID=FA_RSV_LOT_ID[FA_RSV_PORTNUMBER]                    LotID                                                   
      LET          FA_OPERATOR_ID=FA_RSV_OPERATOR_ID[FA_RSV_PORTNUMBER]          OperatorID                                              
      LET          FA_PRODUCT_ID=FA_RSV_PRODUCT_ID[FA_RSV_PORTNUMBER]            ProductID                                               
                                                                                                                                         
      LET          FA_TPL_NAME=FA_RSV_TPL_NAME[FA_RSV_PORTNUMBER]                TestPlanName                                            
      LET          FA_LIM_NAME=FA_RSV_LIM_NAME[FA_RSV_PORTNUMBER]                LimitFileName                                           
                                                                                                                                         
      LET          FA_PJ_DIE_USE=FA_PJ_IW_CONDITION[FA_RSV_PORTNUMBER]           ### FA3.0                                               
      READ_INTEGER String=FA_PJ_IW_NUMDIE[FA_RSV_PORTNUMBER]                     ### FA3.0                                               
      IF           READ_INTEGER.Stat==1                                          ### FA3.0                                               
        LET        FA_SAMPLE_DIE_COUNT=READ_INTEGER.Value+FA_DIE_OFFSET          ### FA3.0                                               
      END IF                                                                     ### FA3.0                                               
      LET          FA_PJ_DIE_LABEL=FA_PJ_IW_DIELABEL[FA_RSV_PORTNUMBER]          ### FA3.0                                               
      LET          FA_DIE_JDG_RULE=FA_PJ_IW_JDGRULE[FA_RSV_PORTNUMBER]           ### FA3.0                                               
      READ_REAL    Format="%f",String=FA_PJ_IW_JDGTLEVEL[FA_RSV_PORTNUMBER]      ### FA3.0                                               
      IF           READ_REAL.Stat==1                                             ### FA3.0                                               
        LET        FA_DIE_FAIL_RATIO=READ_REAL.Value                             ### FA3.0                                               
      END IF                                                                     ### FA3.0                                               
                                                                                 ### FA3.0                                               
      LET          FA_PJ_WAF_USE=FA_PJ_BW_CONDITION[FA_RSV_PORTNUMBER]           ### FA3.0                                               
      READ_INTEGER String=FA_PJ_BW_NUMWAFS[FA_RSV_PORTNUMBER]                    ### FA3.0                                               
      IF           READ_INTEGER.Stat==1                                          ### FA3.0                                               
        IF         FA_WAFCHANGE==1                                                                                                       
          LET      FA_SAMPLE_WAF_COUNT=READ_INTEGER.Value                                                                                
        ELSE                                                                                                                             
          IF       FA_WAF_TEST[FA_RSV_PORTNUMBER]=="Retest"                                                                              
            LET    FA_SAMPLE_WAF_COUNT=READ_INTEGER.Value                                                                                
          ELSE                                                                                                                           
            LET    FA_SAMPLE_WAF_COUNT=READ_INTEGER.Value+FA_WAF_OFFSET                                                                  
          END IF                                                                                                                         
        END IF                                                                   ### FA3.0                                               
      END IF                                                                     ### FA3.0                                               
      LET          FA_PJ_WAFID=FA_PJ_BW_WAFID[FA_RSV_PORTNUMBER]                 ### FA3.0                                               
      LET          FA_WAF_JDG_RULE=FA_PJ_BW_JDGRULE[FA_RSV_PORTNUMBER]           ### FA3.0                                               
      READ_REAL    Format="%f",String=FA_PJ_BW_JDGTLEVEL[FA_RSV_PORTNUMBER]      ### FA3.0                                               
      IF           READ_REAL.Stat==1                                             ### FA3.0                                               
        LET        FA_WAF_FAIL_RATIO=READ_REAL.Value                             ### FA3.0                                               
      END IF                                                                     ### FA3.0                                               
                                                                                 ### FA3.0                                               
      LET          FA_PJE_LOOP_USE=FA_PJ_PJE_CONDITION[FA_RSV_PORTNUMBER]        ### FA3.0                                               
      LET          FA_WAFS_JDG_RULE=FA_PJ_PJE_JDGRULE[FA_RSV_PORTNUMBER]         ### FA3.0                                               
      READ_REAL    Format="%f",String=FA_PJ_PJE_JDGTLEVEL[FA_RSV_PORTNUMBER]     ### FA3.0                                               
      IF           READ_REAL.Stat==1                                             ### FA3.0                                               
        LET        FA_LOOP_FAIL_RATIO=READ_REAL.Value                            ### FA3.0                                               
      END IF                                                                     ### FA3.0                                               
      IF           FA_RSV_PORTNUMBER==1                                          ### FA3.0                                               
        LET        FA_TEST_DIELABEL=FA_RSV_DIELABEL1                             ### FA3.0                                               
        LET        FA_TEST_DIETYPE=FA_RSV_DIETYPE1                               ### FA3.0                                               
        LET        FA_TEST_MODLABEL=FA_RSV_MODLABEL1                             ### FA3.0                                               
        LET        FA_RSV_TCO=FA_RSV_TCO1                                        ### FA3.0                                               
        LET        FA_RSV_TCONUM=FA_RSV_TCONUM1                                  ### FA3.3                                               
      ELSE                                                                       ### FA3.0                                               
        LET        FA_TEST_DIELABEL=FA_RSV_DIELABEL2                             ### FA3.0                                               
        LET        FA_TEST_DIETYPE=FA_RSV_DIETYPE2                               ### FA3.0                                               
        LET        FA_TEST_MODLABEL=FA_RSV_MODLABEL2                             ### FA3.0                                               
        LET        FA_RSV_TCO=FA_RSV_TCO2                                        ### FA3.0                                               
        LET        FA_RSV_TCONUM=FA_RSV_TCONUM2                                  ### FA3.3                                               
      END IF                                                                     ### FA3.0                                               
