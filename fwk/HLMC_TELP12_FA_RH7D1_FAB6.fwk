                    IF             RTN_PRB_QUERY_READY=="Prober_abort"                                                                                                                 
                     LOG_PROBER    Session=SYSTEM.SESSIONID,Message="PRB_QUERY_READY: PROBER_READY Error " + PROBER_READY.Error                                                        
