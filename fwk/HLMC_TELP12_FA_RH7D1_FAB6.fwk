$Type: Action
$Name: FA_CHECK_WAFERBEGIN
$Vers: 1
$Desc: Desc: Set wafer id and send event message\nDate: '01/Jan/11 Initial revision
$Date: 11/12/2012
$Time: 16:48:30
$User: wat
$--------------- --------------------- ----------------------------------------------------------- - - --------------------------------------------------------
 BODY                                                                                                                                                          
                 LET                   FA_SUBLOT_ID=TST_SLOT_WAFERID[TST_SLOT_CURRENT]                 Specify the wafer id in the framework tag               
 Ocr_off_slot                                                                                          OCR-off, assign slot number as wafer id                 
                 IF                    FA_OCR_STATE=="OFF_SLOT"                                                                                                
                   WRITE_INTEGER       Value=TST_SLOT_CURRENT                                                                                                  
                   LET                 FA_SUBLOT_ID=WRITE_INTEGER.String                               Assign slot number as wafer id                          
                 END IF                                                                                                                                        
                                                                                                                                                               
 Ocr_off_host                                                                                          OCR-off, assign the number from host as wafer id        
                 IF                    FA_OCR_STATE=="OFF_HOST"                                                                                                
                   IF                  FA_WAF_LIST[1][1]<>"-1"                                         No wafer list                                           
                     LET               TMP_I=1                                                                                                                 
                     REPEAT                                                                                                                                    
                       WRITE_INTEGER   Value=TST_SLOT_CURRENT                                                                                                  
                       IF              FA_WAF_LIST[TMP_I][1]=WRITE_INTEGER.String                                                                              
                         LET           FA_SUBLOT_ID=FA_WAF_LIST[TMP_I][2]                              Assign wafer id                                         
                         BREAK                                                                                                                                 
                       END IF                                                                                                                                  
                       LET             TMP_I=TMP_I+1                                                                                                           
                     UNTIL             TMP_I>PRB_SLOT_TOTAL_CASS                                                                                               
                   ELSE                                                                                                                                        
                     WRITE_INTEGER     Value=TST_SLOT_CURRENT                                                                                                  
                     LET               FA_SUBLOT_ID=WRITE_INTEGER.String                               Assign slot number as wafer id                          
                   END IF                                                                                                                                      
                 END IF                                                                                                                                        
                                                                                                                                                               
 Ocr_on_slot                                                                                           OCR-on, if fail, assign slot number as wafer id         
                 IF                    FA_OCR_STATE=="ON_SLOT"                                                                                                 
                   PROBER_WAFID                                                                                                                                
                   IF                  PROBER_WAFID.Wafid<>FA_FAIL_WAFID                                                                                       
                     LET               FA_SUBLOT_ID=PROBER_WAFID.Wafid                                 Get wafer id from the prober                            
                   ELSE                                                                                                                                        
                     WRITE_INTEGER     Value=TST_SLOT_CURRENT                                                                                                  
                     LET               FA_SUBLOT_ID=FA_FAIL_SUBLOTID+WRITE_INTEGER.String              Assign slot number as wafer id                          
                   END IF                                                                                                                                      
                 END IF                                                                                                                                        
                                                                                                                                                               
 Ocr_on_keyinput                                                                                       OCR-on, if fail, operator input the number as wafer id  
                 IF                    FA_OCR_STATE=="ON_KEYINPUT"                                                                                             
                   PROBER_WAFID                                                                                                                                
                   IF                  PROBER_WAFID.Wafid<>FA_FAIL_WAFID                                                                                       
                     LET               FA_SUBLOT_ID=PROBER_WAFID.Wafid                                 Get wafer id from the prober                            
                   ELSE                                                                                                                                        
                     PANEL             "FA_SET_SUBLOT_ID",2                                            Show panel to input wafer id.                           
                     PANEL             "FA_SET_SUBLOT_ID",0                                            Close                                                   
                   END IF                                                                                                                                      
                 END IF                                                                                                                                        
                                                                                                                                                               
 Ocr_on_host                                                                                           OCR-on, if fail, assign the number from host as wafer id
                 IF                    FA_OCR_STATE=="ON_HOST"                                                                                                 
                   PROBER_WAFID                                                                                                                                
                   IF                  PROBER_WAFID.Wafid<>FA_FAIL_WAFID                                                                                       
                     LET               FA_SUBLOT_ID=PROBER_WAFID.Wafid                                 Get wafer id from the prober                            
                   ELSE                                                                                                                                        
                     IF                FA_WAF_LIST[1][1]<>"-1"                                         No wafer list                                           
                       LET             TMP_I=1                                                                                                                 
                       REPEAT                                                                                                                                  
                         WRITE_INTEGER Value=TST_SLOT_CURRENT                                                                                                  
                         IF            FA_WAF_LIST[TMP_I][1]=WRITE_INTEGER.String                                                                              
                           LET         FA_SUBLOT_ID=FA_WAF_LIST[TMP_I][2]                              Assign wafer id                                         
                           BREAK                                                                                                                               
                         END IF                                                                                                                                
                         LET           TMP_I=TMP_I+1                                                                                                           
                       UNTIL           TMP_I>PRB_SLOT_TOTAL_CASS                                                                                               
                     ELSE                                                                                                                                      
                       WRITE_INTEGER   Value=TST_SLOT_CURRENT                                                                                                  
                       LET             FA_SUBLOT_ID=FA_FAIL_SUBLOTID+WRITE_INTEGER.String              Assign slot number as wafer id                          
                     END IF                                                                                                                                    
                   END IF                                                                                                                                      
                 END IF                                                                                                                                        
                                                                                                                                                               
                 LET                   TST_SLOT_WAFERID[TST_SLOT_CURRENT]=FA_SUBLOT_ID                 Specify the wafer id in the framework tag               
                 LET                   TMP_I=1                                                         ### Copy previous PF results                            
                 WHILE                 TMP_I<PRB_SLOT_TOTAL_ALL                                        ###                                                     
                   IF                  TST_SLOT_YLD_PF[TMP_I] == 1                                     ###                                                     
                     LET               PNL_TEST_CASS_STATE[TMP_I]="$PASS"                              ###                                                     
                   ELSE IF             TST_SLOT_YLD_PF[TMP_I] == 0                                     ###                                                     
                     LET               PNL_TEST_CASS_STATE[TMP_I]="$FAIL"                              ###                                                     
                   END IF                                                                              ###                                                     
                   LET                 TMP_I=TMP_I+1                                                   ###                                                     
                 END WHILE                                                                             ###                                                     
#                LET                   PNL_TEST_CASS_ITEM=TST_SLOT_WAFERID                             201107,sami: comment                                    
                 LET                   I=1                                                             201107,sami: add                                        
                 WHILE                 I<=25                                                                                                                   
                  STRING_SPLIT         Src=TST_SLOT_WAFERID[I],Del="#"                                                                                         
                  LET                  PNL_TEST_CASS_ITEM[I]=STRING_SPLIT.Elem[STRING_SPLIT.Count]                                                             
                  READ_INTEGER         String=STRING_SPLIT.Elem[STRING_SPLIT.Count]                    201211,sami: add                                        
                  LET                  CTM_WAFID_CASS[I]=READ_INTEGER.Value                            201211,sami: add                                        
                  LET                  I=I+1                                                                                                                   
                 END WHILE                                                                             201107,sami: add                                        
                 PANEL                 "HP_DISP_TEST_RUN",1                                            ### To set WaferID to CassMap                           
                                                                                                                                                               
 Set_offset                                                                                                                                                    
                 IF                    FA_ON_RETEST=="ON"                                                                                                      
                   LET                 PRB_OFFSETX=FA_ADD_OFFSETX                                      Specify offset length by x-index                        
                   LET                 PRB_OFFSETY=FA_ADD_OFFSETY                                      Specify offset length by y-index                        
                 ELSE                                                                                                                                          
                   LET                 PRB_OFFSETX=0                                                                                                           
                   LET                 PRB_OFFSETY=0                                                                                                           
                 END IF                                                                                                                                        
                                                                                                                                                               
                 ACTION                "FA_SEND_WAFERBEGIN"                                            ### AUTOMATION                                          
