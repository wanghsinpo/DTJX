 Query_prober_ready                                                                                                                                                                    
                    PROBER_READY   Timeout=PRB_TIME_READY                                                                                                                              
                                                                                                                                                                                       
                    ACTION         "FA_CHECK_MESSAGE"                                                                               ### AUTOMATION Check prober_alarm                  
                    IF             FA_GEM_COMMAND=="RESUME"                                                                         ### AUTOMATION   Resume or Abort after prober alarm
                      PROBER_LOGWR Message="RESUME 025"                                                                             For debugging                                      
                    END IF                                                                                                                                                             
                    IF             (FA_GEM_COMMAND=="ABORT") OR (FA_GEM_COMMAND=="STOP")                                            ### FA3.0                                          
                      PROBER_LOGWR Message="ABORT 025"                                                                              For debugging                                      
                      LET          RTN_PRB_QUERY_READY="Prober_abort"                                                               ### AUTOMATION                                     
                      LET          FA_NOMOREU=1                                                                                     ### AUTOMATION                                     
                      RETURN                                                                                                        ### AUTOMATION                                     
                    END IF                                                                                                          ### AUTOMATION                                     
                                                                                                                                                                                       
                    LET            RTN_PRB_QUERY_READY="Prober_abort"                                                                                                                  
                                                                                                                                                                                       
                    IF             PROBER_READY.Status=="ERROR"                                                                     Status == "ERROR"                                  
                     IF            PROBER_READY.Error=="#TIMEOUT_SRQ"                                                                                                                  
                      GOTO         Query_prober_ready                                                                                                                                  
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#ALIGN_REJECT"                                                                                                                 
                      LET          RTN_PRB_QUERY_READY="Align_reject"                                                                                                                  
                      LET          FA_LOTCANCEL_BYPRB=1                                                                             ### FA3.2                                          
                      RETURN                                                                                                        #Tune up                                           
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#ALIGN_ASSIST"                                                                                                                 
                      LET          RTN_PRB_QUERY_READY="Align_assist"                                                                                                                  
                      RETURN                                                                                                        #Tune up                                           
                     END IF                                                                                                                                                            
                    END IF                                                                                                                                                             
                                                                                                                                                                                       
                    IF             PROBER_READY.Status=="END"                                                                       Status == "END"                                    
                     LET           FA_LOTCANCEL_BYPRB=1                                                                             ### FA3.2                                          
                     LET           RTN_PRB_QUERY_READY="Lot_end"                                                                                                                       
                     RETURN                                                                                                         #Tune up                                           
                    END IF                                                                                                                                                             
                                                                                                                                                                                       
                    IF             PROBER_READY.Status=="OK"                                                                        Status == "OK"                                     
                     IF            PROBER_READY.Error=="#INIT_WAFER"                                                                                                                   
                      LET          RTN_PRB_QUERY_READY="Wafer_ready"                                                                                                                   
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#INIT_CHIP"                                                                                                                    
                      LET          RTN_PRB_QUERY_READY="Wafer_ready"                                                                                                                   
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#WAF_END"                                                                                                                      
                      LET          RTN_PRB_QUERY_READY="Wafer_end"                                                                                                                     
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#CONTINUE"                                                                                                                     
                      LET          RTN_PRB_QUERY_READY="Wafer_cont"                                                                                                                    
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#SETUP_READY"                                                                                                                  
                      LET          RTN_PRB_QUERY_READY="File_ready"                                                                                                                    
                     END IF                                                                                                                                                            
                     IF            PROBER_READY.Error=="#SLOAD_READY"                                                                                                                  
                      LET          RTN_PRB_QUERY_READY="Sload_ready"                                                                                                                   
                     END IF                                                                                                                                                            
