$Type: Action
$Name: HP_TEST_SET_RECIPE_1L1C
$Vers: 1
$Desc: Set test recipe (1 lot / 1 cassette).
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$---------------- ---------------- --------------------------------------------------- - - --------------
 BODY                                                                                                    
 Enter_key                                                                                               
                  LET              TST_RCP_LOTID=""                                                      
                  LET              TST_RCP_CASSID=""                                                     
                  LET              TST_RCP_PROCESS=""                                                    
                  LET              TST_RCP_TPL=""                                                        
                  LET              TST_RCP_LIMIT=""                                                      
                                                                                                         
                  ACTION           "FA_SET_RECIPE_SINGLE"                                  ### AUTOMATION
                  GOTO             Set_filename                                            ### AUTOMATION
                                                                                                         
                  IF               TST_TPL_LOADED<>0                                                     
                   GOTO            Enter_taginfo                                                         
                  END IF                                                                                 
                  IF               TST_RCP_USE==0                                                        
                   GOTO            Enter_recipe                                                          
                  END IF                                                                                 
                                                                                                         
                  ACTION           "HP_GET_RECIPE_SINGLE"                                                
                                                                                                         
                  IF               RTN_PLG_GET_RECIPE=="Main"                                            
                   LET             RTN_TEST_SET_RECIPE="Main"                                            
                   RETURN                                                                                
                  END IF                                                                                 
                  IF               RTN_PLG_GET_RECIPE=="Retry"                                           
                   GOTO            Enter_key                                                             
                  END IF                                                                                 
                                                                                                         
                  IF               UIF_SET_RECIPE==0                                                     
                   GOTO            Verify_recipe                                                         
                  END IF                                                                                 
 Enter_recipe                                                                                            
                  ACTION           "HP_SET_RECIPE_SINGLE"                                                
                                                                                                         
                  IF               RTN_PLG_SET_RECIPE=="Main"                                            
                   LET             RTN_TEST_SET_RECIPE="Main"                                            
                   RETURN                                                                                
                  END IF                                                                                 
                  IF               RTN_PLG_SET_RECIPE=="Retry"                                           
                   GOTO            Enter_key                                                             
                  END IF                                                                                 
 Verify_recipe                                                                                           
                  VERIFY_FILE      TST_RCP_TPL,"Normal"                                                  
                  IF               VERIFY_FILE.Readable==0                                               
                   LET             PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                     
                   LET             PNL_MSG_MSG1="$ERR_TPL_NOT_READABLE"                                  
                   LET             PNL_KEY_OK="$RETRY"                                                   
                   PANEL           "HP_DISP_ERR1_OK"                                                     
                   PANEL           "HP_DISP_ERR1_OK",0                                                   
                   GOTO            Enter_recipe                                                          
                  END IF                                                                                 
                                                                                                         
                  IF               TST_RCP_LIMIT<>""                                                     
                   VERIFY_FILE     TST_RCP_LIMIT,"Normal"                                                
                   IF              VERIFY_FILE.Readable==0                                               
                    LET            PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                     
                    LET            PNL_MSG_MSG1="$ERR_LIMIT_NOT_READABLE"                                
                    LET            PNL_KEY_OK="$RETRY"                                                   
                    PANEL          "HP_DISP_ERR1_OK"                                                     
                    PANEL          "HP_DISP_ERR1_OK",0                                                   
                    GOTO           Enter_recipe                                                          
                   END IF                                                                                
                  END IF                                                                                 
                                                                                                         
 CheckTPLvsLim                                                                                           
                  ACTION           "HP_TEST_CHECK_FILE"                                                  
                  IF               RTN_TEST_CHKFILE<>"OK"                                                
                   GOTO            Enter_recipe                                                          
                  END IF                                                                                 
                  GOTO             Set_filename                                                          
 Enter_taginfo                                                                                           
                  LET              TST_RCP_TPL=SYSTEM.TESTPLAN                                           
                                                                                                         
                  ACTION           "HP_SET_TAGINFO_SINGLE"                                               
                  IF               RTN_PLG_SET_TAGINFO=="Main"                                           
                   LET             RTN_TEST_SET_RECIPE="Main"                                            
                   RETURN                                                                                
                  END IF                                                                                 
                  IF               RTN_PLG_SET_TAGINFO=="Retry"                                          
                   GOTO            Enter_taginfo                                                         
                  END IF                                                                                 
                                                                                                         
                  IF               TST_RCP_LIMIT<>""                                                     
                   VERIFY_FILE     TST_RCP_LIMIT,"Normal"                                                
                   IF              VERIFY_FILE.Readable==0                                               
                    LET            PNL_TTL_LABEL="$TTL_SYSTEM_ERROR"                                     
                    LET            PNL_MSG_MSG1="$ERR_LIMIT_NOT_READABLE"                                
                    LET            PNL_KEY_OK="$RETRY"                                                   
                    PANEL          "HP_DISP_ERR1_OK"                                                     
                    PANEL          "HP_DISP_ERR1_OK",0                                                   
                    GOTO           Enter_taginfo                                                         
                   END IF                                                                                
                  END IF                                                                                 
                                                                                                         
 CheckTPLvsLIMtag                                                                                        
                  ACTION           "HP_TEST_CHECK_LIM"                                                   
                  IF               RTN_TEST_CHKFILE<>"OK"                                                
                   GOTO            Enter_taginfo                                                         
                  END IF                                                                                 
 Set_filename                                                                                            
                  LET              TST_DATA_DIR=""                                                       
                  LET              TST_DATA_NAME=""                                                      
                  ACTION           "HP_NAME_DATAFILE"                                                    
                  IF               RTN_PLG_NAME_DATAFILE=="Error"                                        
                   IF              TST_TPL_LOADED<>0                                                     
                    GOTO           Enter_taginfo                                                         
                   ELSE                                                                                  
                    GOTO           Enter_recipe                                                          
                   END IF                                                                                
                  END IF                                                                                 
 Set_final                                                                                               
                  LET              TST_LOT_TOTAL=1                                                       
                  LET              TST_LOT_ID[1]=TST_RCP_LOTID                                           
                  LET              TST_LOT_CASS1[1]=TST_RCP_CASSID                                       
                  LET              TST_LOT_PROCESS[1]=TST_RCP_PROCESS                                    
                  LET              TST_LOT_TPL[1]=TST_RCP_TPL                                            
                  LET              TST_LOT_LIMIT[1]=TST_RCP_LIMIT                                        
                  LET              TST_LOT_DATA_DIR[1]=TST_DATA_DIR                                      
                  LET              TST_LOT_DATA_NAME[1]=TST_DATA_NAME                                    
                  IF               FA_TUNE_FLG<>1                                                        
                    SET_TAG_STRING "TST_SLOT_WAFERID",""                                                 
                  END IF                                                                                 
                  SET_TAG_INTEGER  "TST_SLOT_LOT_INDEX",0                                                
                  SET_TAG_INTEGER  "TST_SLOT_SELECT",0                                                   
                                                                                                         
                  LET              TMP_SLOT_POS=1                                                        
                  REPEAT                                                                                 
                   IF              FA_TUNE_FLG<>1                                                        
                    WRITE_INTEGER  Format="%d",Value=TMP_SLOT_POS                                        
                    LET            TST_SLOT_WAFERID[TMP_SLOT_POS]=WRITE_INTEGER.String                   
                   END IF                                                                                
                   LET             TST_SLOT_LOT_INDEX[TMP_SLOT_POS]=1                                    
                   LET             TST_SLOT_SELECT[TMP_SLOT_POS]=1                                       
                   LET             TMP_SLOT_POS=TMP_SLOT_POS+1                                           
                  UNTIL            TMP_SLOT_POS>PRB_SLOT_TOTAL_ALL                                       
                                                                                                         
                  LET              TST_SLOT_SELECTABLE=TST_SLOT_SELECT                                   
                  LET              RTN_TEST_SET_RECIPE="Next"                                            
                  ACTION           "FA_TEST_SEL_WAFER"                                     ### AUTOMATION
