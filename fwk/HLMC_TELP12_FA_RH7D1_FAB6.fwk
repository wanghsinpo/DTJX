$Type: Action
$Name: DIE_END
$Vers: 1
$Desc: ACTION called after each die-test.\n  SPECS2.5: Added auto-probing function on failure.
$Date: 03/25/2011
$Time: 13:28:41
$User: wat
$------------ ------------- --------------------------------------------------------------------------------------------------------------------- - - -------------------
 BODY                                                                                                                                                                    
 Exec_hook                                                                                                                                                               
              IF            SYSTEM.RETEST=="DIE"                                                                                                                         
               IF           ENV_CLEARONRETEST=="TRUE"                                                                                                                    
                SET_JDG_DIE Typeindex=DIE.TYPENUMBER,Dieindex=DIE.NUMBER,Judge=PNL_TEST_WAFER_YET,Offsetx=DIE.PRB_OFFSETX,Offsety=DIE.PRB_OFFSETY                        
               END IF                                                                                                                                                    
               RETURN                                                                                                                                                    
              END IF                                                                                                                                                     
 Set_tag                                                                                                                                                                 
              LET           WAFER.SYS_DIECOUNT=WAFER.SYS_DIECOUNT+1                                                                                                      
                                                                                                                                                                         
              IF            SYSTEM.REJECT<>""                                                                                                                            
               LET          DIE.SYS_DIEEND="Reject"                                                                                                                      
              ELSE                                                                                                                                                       
               LET          DIE.SYS_DIEEND="Complete"                                                                                                                    
              END IF                                                                                                                                                     
 Judge_die                                                                                                                                                               
              ACTION        "HP_JDG_DIE_AT_DIE"                                                                                                                          
 Update_panel                                                                                                                                                            
              IF            DIE.SYS_DIEPASS==1                                                                                                                           
               LET          TMP_WAFER_JUDGE=PNL_TEST_WAFER_PSS                                                                                                           
              ELSE                                                                                                                                                       
               LET          TMP_WAFER_JUDGE=PNL_TEST_WAFER_FIL                                                                                                           
              END IF                                                                                                                                                     
              SET_JDG_DIE   Typeindex=DIE.TYPENUMBER,Dieindex=TST_DIE_NUM,Judge=TMP_WAFER_JUDGE,Offsetx=DIE.PRB_OFFSETX,Offsety=DIE.PRB_OFFSETY                          
              UPDATE        "HP_DISP_TEST_RUN",UPD_TEST_DIE_END                                                                                                          
              IF            FA_DIE_TEST_CNT==0                                                                                                        ### FA3.0          
                GOTO        Judge_wafer                                                                                                               ### FA3.0          
              END IF                                                                                                                                  ### FA3.0          
              ACTION        "FA_SEND_DIEEND"                                                                                                          ### AUTOMATION     
                                                                                                                                                                         
              IF            FA_SKIP_DIE==0                                                                                                            ### FA3.0          
                ACTION      "FA_PJ_DIE_CONTROL"                                                                                                       ### FA3.0 Supported
              END IF                                                                                                                                  ### FA3.0          
              IF            FA_EQUIP_STATE=="IDLEREADY"                                                                                               ### FA3.0          
                RETURN                                                                                                                                ### FA3.0          
              END IF                                                                                                                                  ### FA3.0          
                                                                                                                                                                         
              IF            TST_ADP_CHNG<>"KEEP"                                                                                                      ### FA3.0          
               IF           TST_ADP_CHNG="NEXT_WAF"                                                                                                   ### FA3.0          
                REJECT      WAFER                                                                                                                     ### FA3.0          
                RETURN                                                                                                                                ### FA3.0          
               END IF                                                                                                                                 ### FA3.0          
               IF           TST_ADP_CHNG="CURR_WAF"                                                                                                   ### FA3.0          
                REJECT      WAFER                                                                                                                     ### FA3.0          
                RETURN                                                                                                                                ### FA3.0          
               END IF                                                                                                                                 ### FA3.0          
              END IF                                                                                                                                  ### FA3.0          
                                                                                                                                                                         
 Judge_wafer                                                                                                                                                             
              IF            WAFER.JDG_RUNTIME==1                                                                                                                         
               ACTION       "HP_JDG_WAFER_AT_DIE"                                                                                                                        
               IF           RTN_JDG_WAFER_AT_DIE=="Fail"                                                                                                                 
                GOTO        Wafer_fail                                                                                                                                   
               END IF                                                                                                                                                    
              END IF                                                                                                                                                     
                                                                                                                                                                         
              IF            PRB_ABORT_ALL<>0                                                                                                                             
               RETURN                                                                                                                                                    
              END IF                                                                                                                                                     
              IF            PRB_ABORT_WAFER<>0                                                                                                                           
               RETURN                                                                                                                                                    
              END IF                                                                                                                                                     
                                                                                                                                                                         
              IF            DIE.JDG_RUNTIME==0                                                                                                                           
               IF           DIE.SYS_DIEPASS==0                                                                                                                           
                IF          DIE.SYS_DIEEND=="Complete"                                                                                                                   
                 IF         JDG_DIE_TYPE=="$JDG_DIE_PAUSE_END"                                                                                                           
                  LET       PNL_MSG_MSG1="$" + PRB_MODEL + "_PAUSE_DIE_FAIL"                                                                                             
                  ACTION    "HP_PROBER_PAUSE"                                                                                                                            
                 END IF                                                                                                                                                  
                END IF                                                                                                                                                   
               END IF                                                                                                                                                    
              END IF                                                                                                                                                     
              RETURN                                                                                                                                                     
 Wafer_fail                                                                                                                                                              
              IF            JDG_WAFER_TYPE=="$JDG_WAFER_REJECT"                                                                                                          
               LET          TST_WAFER_ENABLE_PAUSE=0                                                                                                                     
               REJECT       WAFER                                                                                                                                        
               IF           TST_FLW_DETAILED+TST_PRB_AUTO<2                                                                                           ### FA INSPECTION  
#              IF           TST_PRB_AUTO<2                                                                                                            ### FA INSPECTION  
                RETURN                                                                                                                                                   
               END IF                                                                                                                                                    
              END IF                                                                                                                                                     
                                                                                                                                                                         
              IF            PRB_ABORT_ALL<>0                                                                                                                             
               RETURN                                                                                                                                                    
              END IF                                                                                                                                                     
              IF            PRB_ABORT_WAFER<>0                                                                                                                           
               RETURN                                                                                                                                                    
              END IF                                                                                                                                                     
 Wait_cont                                                                                                                                                               
              LET           PNL_MSG_MSG1="$" + PRB_MODEL + "_PAUSE_WAFER_FAIL"                                                                                           
              ACTION        "HP_PROBER_PAUSE"                                                                                                                            
                                                                                                                                                                         
              IF            SYSTEM.REJECT==""                                                                                                                            
               LET          WAFER.JDG_RUNTIME=0                                                                                                                          
              END IF                                                                                                                                                     
