$Type: Action
$Name: HP_TEST_QUERY_RECIPE_1L1C
$Vers: 1
$Desc: Query master file about recipe.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$--------- ------------- -------------------------------------------------- - - -
 BODY                                                                            
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=1               
                                                                                 
           IF            MASTER_LOOKUP.Stat==-1                                  
            LET          RTN_TEST_QUERY_RECIPE="Error"                           
            LET          RTN_TEST_QUERY_RECIPE_ERRMSG="$ERR_RECIPE_NOKEY"        
            RETURN                                                               
           END IF                                                                
                                                                                 
           IF            MASTER_LOOKUP.Stat==-3                                  
            LET          RTN_TEST_QUERY_RECIPE="Error"                           
            LET          RTN_TEST_QUERY_RECIPE_ERRMSG="$ERR_RECIPE_NOFILE"       
            RETURN                                                               
           END IF                                                                
                                                                                 
           MASTER_LOOKUP File=FIL_RECIPE,Key=TST_RCP_CASSID,Fnum=2               
