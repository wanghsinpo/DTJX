$Type: Action
$Name: HP_PROBER_LOAD_CANCEL_PPL_TELP12
$Vers: 1
$Desc: Description for this Action spec.
$Date: 04/02/2009
$Time: 00:00:00
$User: root
$--------------- ------- ----------------------------------- - - ----------------------
 BODY                                                                                  
                 IF      PRB_ONLINE==0                                                 
                  LET    PRB_ABORT_ALL=1                                               
                  RETURN                                                               
                 END IF                                                                
                                                                                       
                 ACTION  "HP_PROBER_LOAD_READY_PPL_TELP12"       Detect 1st wafer ready
                                                                                       
                 IF      RTN_LOAD_READY=="Wafer_ready"                                 
                  GOTO   Handle_cancel                                                 
                 END IF                                                                
                 IF      RTN_LOAD_READY=="Align_reject"                                
                  GOTO   Load_next_wafer                                               
                 END IF                                                                
                 IF      RTN_LOAD_READY=="Lot_end"                                     
                  LET    PRB_ABORT_ALL=1                                               
                 ELSE                                                                  
                  LET    PRB_ABORT_ALL=-1                        "Prober_abort"        
                 END IF                                                                
                 RETURN                                                                
 Load_next_wafer                                                                       
                 LET     TST_SLOT_REJECT=0                                             
                 ACTION  "HP_PROBER_PRB_LOAD_PPL_TELP12"                               
                                                                                       
                 IF      RTN_PRB_LOAD_PPL=="Wafer_ready"                               
                  GOTO   Handle_cancel                                                 
                 END IF                                                                
                 IF      RTN_PRB_LOAD_PPL=="Align_reject"                              
                  GOTO   Load_next_wafer                                               
                 END IF                                                                
                 IF      RTN_PRB_LOAD_PPL=="Lot_end"                                   
                  LET    PRB_ABORT_ALL=1                                               
                  RETURN                                                               
                 END IF                                                                
                 IF      RTN_PRB_LOAD_PPL<>"Align_assist"                              
                  LET    PRB_ABORT_ALL=-1                                              
                  RETURN                                                               
                 END IF                                                                
 Query_assist                                                                          
                 ACTION  "HP_PROBER_PRB_QUERY_READY_TELP12"                            
                                                                                       
                 IF      RTN_PRB_QUERY_READY=="Align_assist"                           
                  GOTO   Query_assist                                                  
                 END IF                                                                
                                                                                       
                 IF      RTN_PRB_QUERY_READY=="Wafer_ready"                            
                  GOTO   Handle_cancel                                                 
                 END IF                                                                
                 IF      RTN_PRB_QUERY_READY=="Align_reject"                           
                  GOTO   Load_next_wafer                                               
                 END IF                                                                
                 LET     PRB_ABORT_ALL=-1                                              
                 RETURN                                                                
 Handle_cancel                                                                         
                 ACTION  "HP_PROBER_PRB_PAUSE_TELP12"                                  
                                                                                       
                 IF      RTN_PRB_PAUSE=="Wafer_cont"                                   
                  GOTO   Handle_cancel                                                 
                 END IF                                                                
                 IF      RTN_PRB_PAUSE=="Wafer_end"                                    
                  GOTO   Load_next_wafer                                               
                 END IF                                                                
                                                                                       
                 LET     PRB_ABORT_ALL=-1                                              
