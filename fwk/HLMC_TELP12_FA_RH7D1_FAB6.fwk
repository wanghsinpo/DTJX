$--------------- ---------- -------------------------------------------------------------------------------- - - -----------------
 BODY                                                                                                                             
                 IF         FA_CONTROL_MODE<>"REMOTE"                                                            ### FA INSPECTION
                  IF        TST_FLW_DETAILED==1                                                                  ### FA INSPECTION
                   DATADUMP                                                                                      ### FA INSPECTION
                                                                                                                 ### FA INSPECTION
                   IF       TST_LOT_DATA_FIRST[TST_LOT_CURRENT]=""                                               ### FA INSPECTION
                    GOTO    Is_lot_file                                                                          ### FA INSPECTION
                   END IF                                                                                        ### FA INSPECTION
                   IF       TMP_DATANAME<>TST_LOT_DATA_FIRST[TST_LOT_CURRENT]                                    ### FA INSPECTION
                    GOTO    Is_wafer_file                                                                        ### FA INSPECTION
                   END IF                                                                                        ### FA INSPECTION
 Is_lot_file                                                                                                     ### FA INSPECTION
                   LET      TMP_DATAFILE=SYS_DATADIR+"/"+TMP_DATANAME+"."+ENV_ADTFMT                             ### FA INSPECTION
                   EXECUTE  "mv "+SYS_DATADIR+"/"+SYS_DATATMP+"."+ENV_ADTFMT+" "+TMP_DATAFILE+" 2>/dev/null"     ### FA INSPECTION
                   GOTO     End_create_file                                                                      ### FA INSPECTION
 Is_wafer_file                                                                                                   ### FA INSPECTION
                   LET      TMP_DATAFILE=SYS_DATADIR+"/"+SYS_DATATMP+"."+ENV_ADTFMT                              ### FA INSPECTION
 End_create_file  END IF                                                                                         ### FA INSPECTION
                 END IF                                                                                          ### FA INSPECTION
                                                                                                                                  
                 LET        UIF_DISP_TEST_PAUSE=1                                                                                 
                 IF         FA_CONTROL_MODE<>"REMOTE"                                                            ### FA INSPECTION
                  ACTION    "HP_SET_MODE_DISP"                                                                   ### FA INSPECTION
                  IF        TST_FLW_DETAILED==0                                                                  ### FA INSPECTION
                   PANEL    "HP_DISP_TEST_" + UIF_PAUSE_TYPE,2                                                   ### FA INSPECTION
                  ELSE                                                                                           ### FA INSPECTION
                   PANEL    "HP_DISP_TEST_" + UIF_PAUSE_TYPE + "_DETAIL",2                                       ### FA INSPECTION
