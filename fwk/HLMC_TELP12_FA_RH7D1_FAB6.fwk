             LET     RTN_LOAD_READY="Prober_abort"                                    
             IF      RTN_PRB_QUERY_READY=="Wafer_ready"                               
              LET    RTN_LOAD_READY="Wafer_ready"                                     
             END IF                                                                   
             IF      RTN_PRB_QUERY_READY=="Lot_end"                                   
              LET    RTN_LOAD_READY="Lot_end"                                         
             END IF                                                                   
             IF      RTN_PRB_QUERY_READY=="Align_reject"                              
