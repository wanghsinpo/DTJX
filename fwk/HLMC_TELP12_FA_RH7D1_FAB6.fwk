 BODY                                                                           
      LET        RTN_TEST_VERIFY_WAFERID="OK"                                   
      LET        RTN_TEST_DUPLICATE_WAFERID=""                                  
                                                                                
      IF         TST_VERIFY_WAFERID==0                                          
       RETURN                                                                   
      END IF                                                                    
                                                                                
      LET        TMP_SLOT_POS=1                                                 
      WHILE      TMP_SLOT_POS<=PRB_SLOT_TOTAL_CASS                              
       LET       TMP_I=TMP_SLOT_POS+1                                           
       WHILE     TMP_I<=PRB_SLOT_TOTAL_CASS                                     
        IF       TST_SLOT_WAFERID[TMP_SLOT_POS]==TST_SLOT_WAFERID[TMP_I]        
         LET     RTN_TEST_VERIFY_WAFERID="Error"                                
         LET     RTN_TEST_DUPLICATE_WAFERID=TST_SLOT_WAFERID[TMP_SLOT_POS]      
         RETURN                                                                 
        END IF                                                                  
        LET      TMP_I=TMP_I+1                                                  
